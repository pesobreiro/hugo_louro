var wfui_obj;
/*
 * @criacao 2011.09.29 17:21 daniel.silva
 */
function wfui_load_toolbar_pendCur(pv_tool_bar_id, pv_parameters) {

	var parameters = "";

	if (pv_parameters) {
		parameters = pv_parameters;
	}

	var options = {
		procedimento: 'wfui_act_std_ajax.tool_bar_options',
		parametros: parameters,
		div_actualizacao: pv_tool_bar_id,
		showLoading: false
	};
	ajaxRequest(options);

	$("#" + pv_tool_bar_id).css("height", '65px');

}
/*
 * @criacao 2011.10.03 10:18 daniel.silva
 */
function wfui_load_toolbar_own(pv_tool_bar_id, pv_parameters) {
	var parameters = "";

	if (pv_parameters) {
		parameters = pv_parameters;
	}

	var options = {
		procedimento: 'wfui_mdl_ins_ajax.own_toolbar',
		parametros: parameters,
		div_actualizacao: pv_tool_bar_id,
		showLoading: false
	};
	ajaxRequest(options);

	$("#" + pv_tool_bar_id).css("height", '30px');

}
/*
 * @criacao 2011.09.30 10:25 daniel.silva
 * @alteracao 2013.04.11 20:06 daniel.silva
 */
function convertTableToGrid(options) {
	var p_toppager = '',
		p_colonetotop = '';
	var pager = options.pagerId;
	var p_tp_pager = pager + "_toppager";
	p_tp_pager = p_tp_pager.replace("pager", "generator_grid");

	$("[id=" + options.gridId + "][rel*=flexigrid]").each(function () {
		var table = $(this);
		var myToolbar = [];
		var myUrl = "!" + options.ajaxUrl;
		if (options.conditions || options.qtype) {
			myUrl = myUrl + "?query=" + options.conditions + "&qtype=" + options.qtype;
		}
		if (options.parameters) {
			if (myUrl != "!" + options.ajaxUrl) {
				myUrl = myUrl + "&" + options.parameters;
			} else {
				myUrl = myUrl + "?" + options.parameters;
			}
		}
		wfui_obj = table;

		if (options.toolbarFun) {
			myToolbar = [true, 'top'];
		}

		//cristiano.martins 2014.09.30 GP102593
		if (options.pager != 'BOTTOM') {
			p_toppager = true;
			p_colonetotop = true;
		} else {
			p_toppager = false;
			p_colonetotop = false;
		}

		$(this).jqGrid({
			//------
			gessi_usr_grid_id: options.gridId,
			gessi_usr_pager_id: options.pagerId,
			gessi_usr_block_name: options.blockId,
			//-------
			url: myUrl,
			dataType: 'xml',
			colNames: options.colNames,
			colModel: options.colModel,
			sortname: options.sortname,
			sortorder: options.sortorder,
			pager: options.pagerId,
			toppager: p_toppager,
			rowNum: options.rowNum,
			page: options.page,
			height: "auto",
			autowidth: true,
			shrinkToFit: false,
			rowList: [10, 25, 50, 100, 500],
			multiselect: true,
			multikey: "",
			multiboxonly: true,
			hidegrid: false,
			//------
			sortable: usr_prefs_grid_sortable(jQuery(this)),
			//------
			viewrecords: true,
			loadComplete: function () {
				if (options.toolbarFun) {
					try {
						eval(options.toolbarFun + "('" + table.closest("[id*=gview_]").find("[id^=t_]").attr("id") + "','" + options.toolbarParam + "');");
					} catch (err) {
						var msg = 'Erro LoadComplete' + table.closest("[id*=gview_]").find("[id^=t_]").attr("id") +
							'<br> erro: ' + err + '<br> chamada:' + options.toolbarFun + "('" + table.closest("[id*=gview_]").find("[id^=t_]").attr("id") + "','" + options.toolbarParam + "');";

						alert(msg);
					}

				}
				actAll();
			},
			toolbar: myToolbar,
			gridComplete: function () {
				usr_prefs_grid_pageInRange(jQuery(this));
				//console.log("gridComplete");
			},
			onSortCol: function (index, iCol, sortorder) {
				submitAjaxNoResp("GESSI_USR_PREFS_MAIN.grids_submit_several_data", "pv_grid_id=" + options.blockId + "&pv_pagenumber=" + jQuery(this).jqGrid("getGridParam", "page") + "&pv_sortname=" + index + "&pv_sortorder=" + sortorder);
				//console.log("onSortCol");
			},
			onPaging: function () {
				submitAjaxNoResp("GESSI_USR_PREFS_MAIN.grids_submit_several_data", "pv_grid_id=" + options.blockId + "&pv_pg_reg_number=" + jQuery(this).jqGrid("getGridParam", "rowNum") + "&pv_pagenumber=" + jQuery(this).jqGrid("getGridParam", "page"));
				//console.log("onPaging");
			},
			resizeStop: function (width, index) {
				submitAjaxNoResp("GESSI_USR_PREFS_MAIN.grids_save_column_width", "pv_column_name=" + usr_prefs_grid_returnColModNameByIndex(jQuery("#" + options.gridId), index) + "&pv_grid_id=" + options.blockId + "&pv_column_width=" + width);
				//console.log("resizeStop");
			}
		});

		//usr_prefs_grids_init($(this));

		$(this).jqGrid("navGrid", "#" + options.pagerId, {
			edit: false,
			add: false,
			del: false,
			cloneToTop: p_colonetotop,
			search: false
		});
		if (options.excel || options.columns) {
			$(this).jqGrid("navSeparatorAdd", "" + options.pagerId);
			if (options.pager != 'BOTTOM') {
				$(this).jqGrid("navSeparatorAdd", "" + p_tp_pager);
			}
		}
		if (options.columns) {
			//$(this).jqGrid("navSeparatorAdd", "" + options.pagerId);

			$(this).jqGrid("navButtonAdd", "" + options.pagerId, {
				caption: "",
				title: "Esconder/Mostrar colunas",
				buttonicon: "ui-icon-newwin",
				onClickButton: function () {
					jQuery(grid).jqGrid('columnChooser');
				}
			});

			$(this).jqGrid("navButtonAdd", "" + p_tp_pager, {
				caption: "",
				title: "Esconder/Mostrar colunas",
				buttonicon: "ui-icon-newwin",
				onClickButton: function () {
					jQuery(grid).jqGrid('columnChooser');
				}
			});

			bindShowHideCols(jQuery("#" + options.gridId));
		}
		if (options.excel) {

			$(this).jqGrid("navButtonAdd", "#" + options.pagerId, {
				caption: "",
				title: "Exportar para Excel",
				buttonicon: "ui-icon-document",
				onClickButton: function () {
					var rows = $(this).jqGrid("getGridParam", "rowNum"),
						page = $(this).jqGrid("getGridParam", "page"),
						sortname = $(this).jqGrid("getGridParam", "sortname"),
						sortorder = $(this).jqGrid("getGridParam", "sortorder"),
						gridrecords = null,
						rownum = null,
						url = options.excel;

					url = url + "name_array=_search&value_array=false" +
						"&name_array=sidx&value_array=" + sortname +
						"&name_array=sord&value_array=" + sortorder;

					var gridUrl = $(this).jqGrid("getGridParam", "url");

					var gridUrlParam = gridUrl.split("?")[1].split("&");

					var param;

					for (var i in gridUrlParam) {
						var paramTmp = gridUrlParam[i].split("=");

						url = url + "&name_array=" + paramTmp[0] + "&value_array=" + paramTmp[1];
					}

					gridrecords = $(this).jqGrid("getGridParam", "records");

					if (parseInt(gridrecords) + 0 > parseInt(rows) + 0) {
						if (confirm("Deseja obter todos os registos no Excel?")) {
							url = url + "&name_array=rows&value_array=" + gridrecords + "&name_array=page&value_array=1";
						} else {
							url = url + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
						}
					} else {
						url = url + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
					}

					window.open(url);
				}
			});

			if (options.pager != 'BOTTOM') {
				$(this).jqGrid("navButtonAdd", "#" + p_tp_pager, {
					caption: "",
					title: "Exportar para Excel",
					buttonicon: "ui-icon-document",
					onClickButton: function () {
						var rows = $(this).jqGrid("getGridParam", "rowNum"),
							page = $(this).jqGrid("getGridParam", "page"),
							sortname = $(this).jqGrid("getGridParam", "sortname"),
							sortorder = $(this).jqGrid("getGridParam", "sortorder"),
							gridrecords = null,
							rownum = null,
							url = options.excel;

						url = url + "name_array=_search&value_array=false" +
							"&name_array=sidx&value_array=" + sortname +
							"&name_array=sord&value_array=" + sortorder;

						var gridUrl = $(this).jqGrid("getGridParam", "url");

						var gridUrlParam = gridUrl.split("?")[1].split("&");

						var param;

						for (var i in gridUrlParam) {
							var paramTmp = gridUrlParam[i].split("=");

							url = url + "&name_array=" + paramTmp[0] + "&value_array=" + paramTmp[1];
						}

						gridrecords = $(this).jqGrid("getGridParam", "records");

						if (parseInt(gridrecords) + 0 > parseInt(rows) + 0) {
							if (confirm("Deseja obter todos os registos no Excel?")) {
								url = url + "&name_array=rows&value_array=" + gridrecords + "&name_array=page&value_array=1";
							} else {
								url = url + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
							}
						} else {
							url = url + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
						}

						window.open(url);
					}
				});
			}

		}

		$(this).attr("rel", "");
	});
}
/*
 * @criacao 2013.04.11 20:06 daniel.silva
 */
function bindShowHideCols(gridId) {

	jQuery("[title='Esconder/Mostrar colunas']").click(
		function () {
			jQuery("[multiple=multiple]").bind("mouseup", function () {
				usr_prefs_grid_visibility_array(gridId);
			});
			jQuery("[multiple=multiple]").bind("keyup", function () {
				usr_prefs_grid_visibility_array(gridId);
			});

			jQuery(".ui-dialog-buttonset").find("[role=button]").each(
				function (i) {
					if (i == 0)
						jQuery(this).bind("click", function () {
							usr_prefs_grid_submit_column_visible(gridId)
						});
					else
						jQuery(this).bind("click", function () {
							usr_prefs_grid_clear_visible_columns();
						});
				});
		});

}
/*
 * @criacao 2011.09.30 11:15 daniel.silva
 * @alteracao 2013.04.11 20:06 daniel.silva
 */
function activities_load(options) {

	//ajaxUrl, actId, mdlId, engId, usrId, iniDate, endDate, qtype, query, excel

	$("[id=" + options.gridId + "][rel*=flexigrid]").each(function () {

		var table = $(this);

		var colModelArray = [];
		var colModelHash = [];

		var colModelPrio = [];

		colModelHash = options.arrColMdl;

		for (var i in colModelHash) {
			//in IE8 this for return a KEY named filter, we dont need this key
			if (i != "filter") {
				colModelPrio[colModelPrio.length] = i;
			}
		}

		colModelPrio = colModelPrio.sort(function (a, b) {
			return a - b
		});

		for (i = 0; i < colModelPrio.length; i++) {
			colModelArray[colModelArray.length] = colModelHash[colModelPrio[i]];
			if (colModelHash[colModelPrio[i]].name == 'ea.action')
				wfui_col_ord_action = colModelArray.length - 1;
			if (colModelHash[colModelPrio[i]].name == 'ea.history')
				wfui_col_ord_history = colModelArray.length - 1;

		}
		wfui_last_colModelArray = colModelArray;
		// a ser usado para confirmção de número de colunas.
		wfui_nCols = colModelArray.length;

		if (options.qtype == undefined) {
			$(this).getObjTab(".wfui_container").attr("wfui_complex_conditions", "");
		} else {
			$(this).getObjTab(".wfui_container").attr("wfui_complex_conditions", options.qtype);
		}
		if (options.conditions == undefined) {
			$(this).getObjTab(".wfui_container").attr("wfui_conditions", "");
		} else {
			$(this).getObjTab(".wfui_container").attr("wfui_conditions", options.conditions);
		}

		var myColNames = [];
		var myColModel = [];
		for (i = 0; i < colModelArray.length; i++) {
			if (colModelArray[i].name) {
				myColNames[i] = colModelArray[i].display;
				myColModel[i] = {
					name: colModelArray[i].name,
					index: colModelArray[i].name,
					width: colModelArray[i].width,
					resizable: true,
					sortable: colModelArray[i].sortable,
					align: colModelArray[i].align,
					search: colModelArray[i].search,
					classes: colModelArray[i].classes,
					hidden: colModelArray[i].hidden
				}
			}
		}
		var myOptions = {
			blockId: options.blockId,
			gridId: options.gridId,
			pagerId: options.pagerId,
			pager: options.pager,
			ajaxUrl: options.ajaxUrl,
			conditions: options.conditions,
			qtype: options.qtype,
			parameters: options.parameters,
			colNames: myColNames,
			colModel: myColModel,
			sortname: options.sortname,
			sortorder: options.sortorder,
			rowNum: options.rowNum,
			page: options.page,
			toolbarFun: options.toolbarFun,
			toolbarParam: options.toolbarParam,
			excel: options.excel,
			columns: options.columns
		};

		convertTableToGrid(myOptions);
	})
}
/*
 * @criacao 2011.09.30 11:15 daniel.silva
 */
function notifications_load(options) {

	$("[id=" + options.gridId + "][rel*=flexigrid]").each(function () {

		var table = $(this);
		wfui_obj = table;

		var colModelArray = [];

		colModelArray[colModelArray.length] = {
			display: 'Opções',
			name: 'action',
			width: 95,
			sortable: false,
			align: 'center'
		};

		colModelArray[colModelArray.length] = {
			display: 'Modelo',
			name: 'ins_mdl_dst_id',
			width: 100,
			sortable: true,
			align: 'center'
		};
		colModelArray[colModelArray.length] = {
			display: 'Actividade',
			name: 'ins_act_dst_id',
			width: 100,
			sortable: true,
			align: 'center'
		};
		colModelArray[colModelArray.length] = {
			display: 'Enviado por',
			name: 'usr_key',
			width: 200,
			sortable: true,
			align: 'center'
		};
		colModelArray[colModelArray.length] = {
			display: 'Data',
			name: 'ins_date',
			width: 100,
			sortable: true,
			align: 'center'
		};
		colModelArray[colModelArray.length] = {
			display: 'Assunto',
			name: 'subject',
			width: 200,
			sortable: true,
			align: 'center'
		};
		var order_by = 'ins_date';

		var myColNames = [];
		var myColModel = [];
		for (i = 0; i < colModelArray.length; i++) {
			if (colModelArray[i].name) {
				myColNames[i] = colModelArray[i].display;
				myColModel[i] = {
					name: colModelArray[i].name,
					index: colModelArray[i].name,
					width: colModelArray[i].width,
					resizable: true,
					sortable: colModelArray[i].sortable,
					align: colModelArray[i].align,
					search: colModelArray[i].search,
				}
			}
		}

		var myOptions = {
			gridId: options.gridId,
			pagerId: options.pagerId,
			pager: options.pager,
			ajaxUrl: options.ajaxUrl,
			conditions: options.conditions,
			qtype: options.qtype,
			parameters: options.parameters,
			colNames: myColNames,
			colModel: myColModel,
			sortname: order_by,
			toolbarFun: options.toolbarFun,
			toolbarParam: options.toolbarParam
		};

		convertTableToGrid(myOptions);
	})
}

function refresh_own_act(pv_act_type) {

	var conditions = $(this).getObjTab(".wfui_container").attr("wfui_conditions");
	var qtype = $(this).getObjTab(".wfui_container").attr("wfui_complex_conditions");

	var conditionsArr = conditions.split(',');
	var qtypeArr = qtype.split(',');

	if (pv_act_type) {
		conditionsArr[2] = "'" + pv_act_type + "'";
	} else {
		conditionsArr[2] = "''";
	}

	for (i = 0; i < conditionsArr.length; i++) {
		if (conditionsArr[i] == "''") {
			conditionsArr[i] = "";
		}
	}

	var parameters = "";

	parameters = parameters + 'pi_act_id=' + conditionsArr[0].replace(/'/g, "");
	parameters = parameters + '&pv_act_code=' + conditionsArr[1].replace(/'/g, "");
	parameters = parameters + '&pv_act_type=' + conditionsArr[2].replace(/'/g, "");
	parameters = parameters + '&pv_mdl_id=' + conditionsArr[3].replace(/'/g, "");
	parameters = parameters + '&pv_mdl_code=' + conditionsArr[4].replace(/'/g, "");
	parameters = parameters + '&pv_ini_date=' + conditionsArr[5].replace(/'/g, "");
	parameters = parameters + '&pv_end_date=' + conditionsArr[6].replace(/'/g, "");

	//alteracao cristiano.martins 2013.07.22
	for (i = 0; i < qtypeArr.length; i++) {
		parameters = parameters + '&parr_attrs=' + qtypeArr[i].replace(/'/g, "");
		//parameters = parameters + '&parr_attrs=' + qtypeArr[i].replace(/'/g, "");
	}

	var options = {
		procedimento: 'wfui_mdl_ins_main.own_process',
		parametros: parameters,
		div_actualizacao: $(this).getObjTab(".wfui_container").parent().attr("id"),
		showLoading: true
	};
	ajaxRequest(options);
}
/*
 * alteracao 2011.10.03 15:57 daniel.silva
 */

function nfilters(obj) {
	var parameters = "";
	var l_user_id = "";
	wfui_obj = obj;

	if (obj.getObjTab(".wfui_container").attr("wfui_type")) {
		parameters = parameters + "pv_view=" + obj.getObjTab(".wfui_container").attr("wfui_type");
	} else {
		parameters = parameters + "pv_view=";
	}
	if (obj.getObjTab(".wfui_container").attr("wfui_conditions")) {
		parameters = parameters + "&pv_simple_filters=" + obj.getObjTab(".wfui_container").attr("wfui_conditions");
	} else {
		parameters = parameters + "&pv_simple_filters=";
	}
	if (obj.getObjTab(".wfui_container").attr("wfui_complex_conditions")) {
		parameters = parameters + "&pv_complex_filters=" + obj.getObjTab(".wfui_container").attr("wfui_complex_conditions");
	} else {
		parameters = parameters + "&pv_complex_filters=";
	}

	//alteracao cristiano.martins 2014.07.18 GP102625
	if (obj.getObjTab(".wfui_container").attr("wfui_user_id")) {
		//console.log(obj.getObjTab(".wfui_container").attr("wfui_user_id"));
		l_user_id = obj.getObjTab(".wfui_container").attr("wfui_user_id");
		parameters = parameters + "&pv_user_id=" + l_user_id;
	} else {
		parameters = parameters + "&pv_user_id=";
	}

	var options = {
		procedimento: 'wfui_act_std_ajax.opt_filters',
		parametros: parameters,
		showInBox: true
	};
	ajaxRequest(options);

}
/*
 *alteracao 2011.08.25 daniel.silva
 *alteracao 2011.09.15 11:50 daniel.silva
 *alteracao 2011.10.03 15:50 daniel.silva
 *alteracao 2013.09.03 16:50 daniel.silva
 */
function filtersYes() {
	var hdrs = "";
	var values = "";
	var attrs = "";
	var apl_name = "";
	var l_wfui_complex_conditions = "";
	var l_filter_org = "";
	var l_wfui_conditions = "";
	var l_parameters = "";
	var l_exist = 0;
	var l_user_id = "";
	var lv_showAllActs = $("#console_acts").attr('showAllActs');
	var lv_param_show_all_Acts;

	if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "OWN_PROC") {
		l_wfui_complex_conditions = wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions");
		l_wfui_conditions = wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions");
		l_filter_org = wfui_obj.getObjTab(".wfui_container").attr("wfui_orig_filters");

		l_wfui_conditions = l_wfui_conditions.split(",");

		$("[name='parr_key']").each(function () {
			hdrs = hdrs + $(this).val() + ",";
		});

		hdrs = hdrs.split(",");

		$("[name='parr_value']").each(function () {
			values = values + $(this).val() + ",";
		});
		values = values.split(",");

		if (hdrs != "") {
			for (i = 0; i < hdrs.length - 1; i++) {
				if (values[i]) {
					attrs = attrs + "&parr_attrs=" + hdrs[i] + "&parr_attrs=" + values[i];
					if (hdrs[i] = 'CAD_NRE') {
						l_exist = 1;
					}
				}
			}
		}

		l_filter_org = l_filter_org.split(",");
		for (i = 0; i < l_filter_org.length; i++) {
			attrs = attrs + "&parr_attrs_orig=" + l_filter_org[i].substring(1, l_filter_org[i].length - 1);
			if (l_exist == 0) {
				attrs = attrs + "&parr_attrs=" + l_filter_org[i].substring(1, l_filter_org[i].length - 1);
			}
		}

		l_parameters = "pi_act_id=" + l_wfui_conditions[0].substring(1, l_wfui_conditions[0].length - 1) +
			"&pv_act_code=" + l_wfui_conditions[1].substring(1, l_wfui_conditions[1].length - 1) +
			"&pv_act_type=" + l_wfui_conditions[2].substring(1, l_wfui_conditions[2].length - 1) +
			"&pv_mdl_id=" + l_wfui_conditions[3].substring(1, l_wfui_conditions[3].length - 1) +
			"&pv_mdl_code=" + l_wfui_conditions[4].substring(1, l_wfui_conditions[4].length - 1) +
			"&pv_ini_date=" + l_wfui_conditions[5].substring(1, l_wfui_conditions[5].length - 1) +
			"&pv_end_date=" + l_wfui_conditions[6].substring(1, l_wfui_conditions[6].length - 1) +
			attrs;

		var options = {
			parametros: l_parameters,
			div_actualizacao: wfui_obj.getObjTab(".wfui_container").parent().attr("id"),
			showLoading: true
		};

	} else {
		//alteracao cristiano.martins 2014.07.22 GP102625
		if (wfui_obj.getObjTab(".wfui_container").attr("wfui_user_id")) {
			l_user_id = wfui_obj.getObjTab(".wfui_container").attr("wfui_user_id");
		}

		apl_name = wfui_obj.getObjTab(".wfui_container").attr("wfui_apl_name");

		wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions", "'" + $("[name='pv_act']").val() + "','" + $("[name='pv_mdl']").val() + "','" + $("[name='pv_eng']").val() + "','" + $("[name='pv_usr']").val() + "','" + $("[name='pv_date_min']").val() + "','" + $("[name='pv_date_max']").val() + "','" + apl_name + "','" + $("[name='pv_user_assign']").val() + "'");
		$("[name='parr_key']").each(function () {
			hdrs = hdrs + "'" + $(this).val() + "',";
		});
		hdrs = hdrs.split(",");

		$("[name='parr_value']").each(function () {
			values = values + "'" + $(this).val() + "',";
		});
		values = values.split(",");

		if (hdrs != "") {
			for (i = 0; i < hdrs.length - 1; i++) {
				if (i != hdrs.length - 2) {
					attrs = attrs + hdrs[i] + "," + values[i] + ",";
				} else {
					attrs = attrs + hdrs[i] + "," + values[i];
				}
			}
		}
		wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions", attrs);

		//console.log("qtype "+wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions"));
		//console.log("query "+wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions"));
		//@alteracao jose.duarte 2016.07.01 GP 110581 se for a consola devemos passar o parâmetro pv_show_all_acts_cons para aplicar o filtro à selecção previamente feita
		if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CONS") {
			if (lv_showAllActs != undefined) {
				if (lv_showAllActs == 'Y') {
					lv_param_show_all_Acts = '&pv_show_all_acts_cons=Y'
				} else {
					lv_param_show_all_Acts = '&pv_show_all_acts_cons=N'
				}
			} else {
				lv_param_show_all_Acts = '';
			}
		} else {
			lv_param_show_all_Acts = '';
		}

		var options = {
			parametros: "qtype=" + wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions") + "&query=" + wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions") + "&pv_user_id=" + l_user_id,
			div_actualizacao: wfui_obj.getObjTab(".wfui_container").parent().attr("id"),
			showLoading: true
		};
	}
	if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CUR") {
		options.procedimento = 'wfui_act_cur_ajax.act_current_list_view';
	} else if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "PEND") {
		options.procedimento = 'wfui_act_pend_ajax.act_pending_list_view';
	} else if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "HISTCUR") {
		options.procedimento = 'wfui_act_hist_ajax.act_current_list_view';
		/*cristiano.martins 2013.03.22 CONSOLE*/
	} else if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CONS") {
		options.procedimento = 'wfui_act_cons_ajax.act_current_list_view';
	}
	/* alteracao 2013.09.02 18:09 daniel.silva */
	else if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "OWN_PROC") {
		options.procedimento = 'wfui_mdl_ins_main.own_process';
	}
	if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CONS") {
		refreshTree();
	}
	ajaxRequest(options);
	closeFacebox();
}

function filtersNo() {
	closeFacebox();
}
/*
 * @alteracao 2011.09.30 11:15 daniel.silva
 */
function action(obj) {

	var actIds = "";

	var arr = obj.getObjTab(".wfui_container").find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');

	var arrfinal = new Array();
	for (i = 0; i < arr.length; i++) {
		if (arr[i] && arr[i] != "") {
			actIds = actIds + "'" + arr[i] + "',";
		}
	}

	actIds = actIds.substr(0, actIds.length - 1);
	if (actIds == "") {
		alert("Não foi seleccionado qualquer item.");
	} else if (obj.getObjTab(".wfui_container").attr("wfui_all") == "true") {
		pedidoAjax("wfui_act_std_ajax.opt_actions", "pv_view=" + obj.getObjTab(".wfui_container").attr("wfui_type") + "&pv_simple_filters=" + obj.getObjTab("#wfui_container").attr("wfui_conditions") + "&pv_complex_filters=" + obj.getObjTab("#wfui_container").attr("wfui_complex_conditions"), true, true, true, 0, false);
	} else {
		pedidoAjax("wfui_act_std_ajax.opt_actions", "pv_view=" + obj.getObjTab(".wfui_container").attr("wfui_type") + "&pv_ins_acts_ids=" + actIds, true, true, true, 0, false);
	}
}
/*
 * @criacao 2011.09.30 11:15 daniel.silva
 */
jQuery.fn.extend({
	//This function return any selector object inside the current tab,
	//or if its not inside any tab then return the document object
	getObjTab: function (selector) {
		try {
			if (jQuery(this).closest(".ui-tabs-panel").find(selector).size() > 0)
				return jQuery(this).closest(".ui-tabs-panel").find(selector);
			else
				return jQuery(selector);
		} catch (e) {
			return jQuery(selector);
		}
	},

});
/*
 * criacao cristiano.martins
 * alteracao 2011.10.12 12:37 daniel.silva
 */
function convertToTree(options) {
	var d = new Date();
	$("[rel*=tree]").jstree({
		"themes": {
			"theme": options.theme.name,
			"dots": options.theme.dots,
			"icons": options.theme.icons
		},
		"types": {
			"types": {
				"eng": {
					"icon": {
						"image": "imagens/" + gv_eng_icon
					}
				},
				"mdl": {
					"icon": {
						"image": "imagens/" + gv_mdl_icon
					}
				},
				"act_i": {
					"icon": {
						"image": "imagens/" + gv_act_i_icon
					}
				},
				"act_f": {
					"icon": {
						"image": "imagens/" + gv_act_f_icon
					}
				},
				"act_n": {
					"icon": {
						"image": "imagens/" + gv_act_n_icon
					}
				},
				"act_or": {
					"icon": {
						"image": "imagens/" + gv_act_or_icon
					}
				},
				"act_and": {
					"icon": {
						"image": "imagens/" + gv_act_and_icon
					}
				},
				"act_swp": {
					"icon": {
						"image": "imagens/" + gv_act_swp_icon
					}
				},
				"act_wswp": {
					"icon": {
						"image": "imagens/" + gv_act_wswp_icon
					}
				}
			}
		},
		"plugins": ["themes", "html_data", "ui", "types"],
		"html_data": {
			"ajax": {
				"url": options.url,
				"data": function (n) {
					return {
						"operation": "get_children",
						"id": n.attr ? n.attr("id").replace("node_", "") : 1,
						"pv_mdl_org_id": n.attr ? n.attr("mdl_org_id") : "",
						"pv_act_org_id": n.attr ? n.attr("act_org_id") : "",
						"pv_apl_name": options.apl_name,
						"pv_user_id": options.user_id,
						"qtype": options.qtype,
						"pv_time": d.getTime(),
						"pv_act_type": options.act_type,
						"pv_ignore_user": options.ignore_user,
					};
				}
			}
		},
		//"plugins" : ["themes", "html_data"]
	});
	$('[rel*=tree]').bind('load_node.jstree', function (event, data) {
		var $treeview = $(this);
		var $nodeToOpen = $treeview.find('li[id*=MT]');
		$nodeToOpen.each(function () {
			$treeview.jstree('open_node', $(this));
		});
	});
	$("[rel*=tree]").attr("rel", "");
}

/*
 * criacao cristiano.martins 2013.03.20
 */
function convertToConsoleTree(options) {
	var d = new Date();
	$("[rel*=tree]").jstree({
		"plugins": ["themes", "html_data", "ui", "types"],
		//"plugins" : ["html_data", "ui"],
		"ui": {
			"initially_select": ["-1"]
		},
		"core": {
			"animation": true,
			"initially_open": ["-1"]
		},
		"themes": {
			"theme": options.theme.name,
			"dots": options.theme.dots,
			"icons": options.theme.icons
		},
		"types": {
			"types": {
				"root": {
					"icon": {
						"image": "imagens/" + gv_root_icon
					}
				},
				"eng": {
					"icon": {
						"image": "imagens/" + gv_eng_icon
					}
				},
				"mdl": {
					"icon": {
						"image": "imagens/" + gv_mdl_icon
					}
				},
				"act_i": {
					"icon": {
						"image": "imagens/" + gv_act_i_icon
					}
				},
				"act_f": {
					"icon": {
						"image": "imagens/" + gv_act_f_icon
					}
				},
				"act_n": {
					"icon": {
						"image": "imagens/" + gv_act_n_icon
					}
				},
				"act_or": {
					"icon": {
						"image": "imagens/" + gv_act_or_icon
					}
				},
				"act_and": {
					"icon": {
						"image": "imagens/" + gv_act_and_icon
					}
				},
				"act_swp": {
					"icon": {
						"image": "imagens/" + gv_act_swp_icon
					}
				},
				"act_wswp": {
					"icon": {
						"image": "imagens/" + gv_act_wswp_icon
					}
				}
			}
		},
		"html_data": {
			"ajax": {
				"url": options.url,
				"data": function (n) {
					return {
						"pv_operation": "get_children",
						"pv_id": n.attr ? n.attr("id") : "",
						"pv_mdl_org_id": n.attr ? n.attr("mdl_org_id") : "",
						"pv_act_org_id": n.attr ? n.attr("act_org_id") : "",
						"pv_eng_id": n.attr ? n.attr("eng_id") : options.pv_eng_id ? options.pv_eng_id : "",
						"pv_mdl_id": n.attr ? n.attr("mdl_id") : options.pv_mdl_id ? options.pv_mdl_id : "",
						"pv_type": n.attr ? n.attr("type") : "",
						"pv_apl_name": options.apl_name,
						"pv_user_id": options.user_id,
						"pv_qtype": options.qtype,
						"pv_time": d.getTime(),
						"pv_act_type": options.act_type,
						"pv_show_all_acts_cons": options.pv_show_all_acts_cons,
						"pv_act_id": n.attr ? n.attr("act_id") : options.pv_act_id ? options.pv_act_id : ""
					};
				}
			}
		}
		//"plugins" : ["themes", "html_data"]
	});
	$("[rel*=tree]").attr("rel", "");
}

function refresh_list_cons_view(obj, pv_parameters) {
	//console.log(obj);
	//console.log(obj.getObjTab(".wfui_container").parent().attr("id"));
	var parameters = "";

	if (pv_parameters) {
		parameters = pv_parameters;
	}

	var options = {
		procedimento: "wfui_act_cons_ajax.act_current_list_view",
		parametros: parameters,
		div_actualizacao: obj.getObjTab(".wfui_container").parent().attr("id"),
		showLoading: false
	};
	ajaxRequest(options);

}

/*
 * @criacao 2011.10.10 11:15 daniel.silva
 */
function toggleImportIns(obj, pv_type, pv_id) {

	if (pv_type == "INSMDL") {
		if (obj.attr("src") == "imagens/wfui_exclamation_none") {
			obj.attr("src", "imagens/wfui_exclamation")

		} else {
			obj.attr("src", "imagens/wfui_exclamation_none")

		}
		submitAjaxNoResp("wfui_act_std_main.set_order_ins_mdl", "pv_ins_mdl_id=" + pv_id);
	} else if (pv_type == "INSACT") {
		if (obj.attr("src") == "imagens/wfui_exclamationAct_none") {
			obj.attr("src", "imagens/wfui_exclamationAct")

		} else {
			obj.attr("src", "imagens/wfui_exclamationAct_none")

		}
		submitAjaxNoResp("wfui_act_std_main.set_order_ins_act", "pv_ins_act_id=" + pv_id);
	}

}
/*
 * @criacao 2011.10.11 16:06 daniel.silva
 */
function wfuiRefreshTabs(obj, pv_apl_name) {
	var parameters = "";
	var user_lower = obj.getObj("#wfui_act_std_main_std_main_block_search_persons_lower_level_ac").find("input[name=pv_user_lower]").val();
	var struct_assignment = obj.getObj("#wfui_act_std_main_std_main_block_search_struct").find("input[name=pv_struct_assignment]").val();
	//parameters = "pv_user_id=" + obj.getObj("input[name=pv_user_id]").val() + "&pv_apl_name=" + pv_apl_name;
	if (!user_lower) {
		user_lower = '';
	}
	parameters = "pv_user_id=" + user_lower + "&pv_struct_assignment=" + struct_assignment + "&pv_apl_name=" + pv_apl_name;

	var options = {
		procedimento: 'wfui_act_std_ajax.show_wfui_tabs',
		parametros: parameters,
		div_actualizacao: 'WFUI_ACT_MAIN_TABS',
		showLoading: true
	};

	ajaxRequest(options);
}

/*
 * alteracao 2011.10.12 19:51 daniel.silva
 * alteracao 2011.02.22 19:35 daniel.silva
 */
function actionYes(actId, actIds, tstId, tstTypeAsg, tstNtfAsg) {

	var myPostFun = myNextPostAjaxFun; //"";
	var myDivAct;
	if (wfui_obj) {
		myDivAct = wfui_obj.getObjTab(".wfui_container").parent().parent().attr("id");
	}
	var parameters = "";

	if (tstTypeAsg == 'M' || tstNtfAsg == 'Y') {
		var options = {
			//title : 'Atribuição de Tarefas',
			procedimento: 'wfui_act_std_ajax.adhoc_asg',
			parametros: 'pv_ins_act_id=' + actId + '&pv_tst_id=' + tstId + "&pv_ins_comments=" + $("#wfui_comments").val() + '&pv_type_asg=' + tstTypeAsg + '&pv_ntf_asg=' + tstNtfAsg,
			showInBox: true,
			force: true,
			delay: 0,
			showLoading: false
		};
		ajaxRequest(options);
	} else {
		if (wfui_obj) {
			parameters = parameters + "query=" + escape(wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions"));
			parameters = parameters + "&qtype=" + escape(wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions"));
			if (wfui_obj.getObj("input[name=pv_user_id]").val()) {
				parameters = parameters + "&pv_user_id=" + wfui_obj.getObj("input[name=pv_user_id]").val();
			} else {
				parameters = parameters + "&pv_user_id=";
			}
			if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CUR") {
				myPostFun += "wfui_obj.ajaxRequestOnMyWindow('wfui_act_cur_ajax.show_act_current', '" + parameters + "', '#" + myDivAct + "'); ";
			} else if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CONS") {
				myPostFun += "wfui_obj.ajaxRequestOnMyWindow('wfui_act_cons_ajax.show_console', '" + parameters + "', '#" + myDivAct + "'); ";
			} else {
				myPostFun += "wfui_obj.ajaxRequestOnMyWindow('wfui_act_pend_ajax.show_act_pending', '" + parameters + "', '#" + myDivAct + "');";
			}
		}
		if (actId != undefined) {
			myNextPostAjaxFun = myPostFun;
			submitAjax("wfui_act_std_ajax.opt_actions_ins_tst", "pi_ins_act_id=" + actId + "&pi_tst_id=" + tstId + "&pv_ins_comments=" + $("#wfui_comments").val());
		}
		if (actIds != undefined) {
			var ids = "";
			var param = "";
			ids = actIds.split(",");
			if (ids != "") {
				param = "pi_tst_id=" + tstId + "&pv_ins_comments=" + $("#wfui_comments").val();
				for (i = 0; i < ids.length; i++) {
					param = param + "&parr_ins_act_id=" + ids[i];
				}
				myNextPostAjaxFun = myPostFun;
				submitAjax("wfui_act_std_ajax.opt_actions_ins_tst_arr", param);
			}
		}

	}
	closeFacebox();
	closeMyWindow(myLastObj);
}

/*
 * alteracao 2011.10.13 20:08 daniel.silva
 */
function actionNtfYes(MsgtstNtfId, MsgtstNtfIds) {

	if (MsgtstNtfId != undefined) {
		myNextPostAjaxFun = "wfui_obj.getObjTab('.wfui_container').find('.ui-jqgrid-btable').trigger('reloadGrid')";
		submitAjax("wfui_not_ajax.opt_actions_msg_tst", "pi_tst_ntf_messages_id=" + MsgtstNtfId + "&pv_ins_comments=" + $("#wfui_comments").val());
	}

	if (MsgtstNtfIds != undefined) {
		var ids = "";
		ids = MsgtstNtfIds.split(",");
		if (ids != "") {
			for (i = 0; i < ids.length; i++) {
				if (i == ids.length)
					myNextPostAjaxFun = "wfui_obj.getObjTab('.wfui_container').find('.ui-jqgrid-btable').trigger('reloadGrid')";
				submitAjax("wfui_not_ajax.opt_actions_msg_tst", "pi_tst_ntf_messages_id=" + ids[i] + "&pv_ins_comments=" + $("#wfui_comments").val());
			}
		}
	}
	closeFacebox();
}

function filtersMdl(mdlId) {
	pedidoAjax("wfui_act_std_ajax.act_cmbx", "pv_name=pv_act&pv_mdl=" + mdlId, "wfui_activities", false, true, 0, false);
}
