function wfui_load_toolbar_histCur(pv_tool_bar_id, pv_parameters) {

	var parameters = "";

	if (pv_parameters) {
		parameters = pv_parameters;
	}

	var options = {
		procedimento: 'wfui_act_std_ajax.tool_bar_options',
		parametros: parameters,
		div_actualizacao: pv_tool_bar_id,
		showLoading: false
	};
	ajaxRequest(options);

	$("#" + pv_tool_bar_id).css("height", '65px');

}
function refreshView() {
	var myDivAct = wfui_obj.getObjTab(".wfui_container").parent().parent().attr("id");
	var parameters = "";

	parameters = parameters + "query=" + escape(wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions"));
	parameters = parameters + "&qtype=" + escape(wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions"));
	if (wfui_obj.getObj("input[name=pv_user_id]").val()) {
		parameters = parameters + "&pv_user_id=" + wfui_obj.getObj("input[name=pv_user_id]").val();
	} else {
		parameters = parameters + "&pv_user_id=";
	}
	if (wfui_obj.getObjTab(".wfui_container").attr("wfui_type") == "CUR") {
		wfui_obj.ajaxRequestOnMyWindow('wfui_act_cur_ajax.show_act_current', parameters, '#' + myDivAct);
	} else {
		wfui_obj.ajaxRequestOnMyWindow('wfui_act_pend_ajax.show_act_pending', parameters, '#' + myDivAct);
	}

}

function change_assignment_load(obj) {
	var parameters = "";
	wfui_obj = obj;

	var process = wfui_obj.getObjTab(".wfui_container").find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	if (process.length == 0) {
		alert("Não foi seleccionado qualquer item.");

		return;
	}

	loadBlockOnFaceBox({
		title: "Alterar Assignment",
		name: "WFUI_ACT_STD_MAIN_CHANGE_ASSIGNMENT",
		mode: "E"
	});
}

function change_assignment_submit(obj) //criacao cristiano.martins 2013.03.25
{
	var org = obj.getObj("#wfui_act_std_main_change_assignment_origin").find("input[name=pv_origin]").val();
	var dst = obj.getObj("#wfui_act_std_main_change_assignment_destination").find("input[name=pv_dest]").val();
	var process = wfui_obj.getObjTab(".wfui_container").find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	var params = getArrGetParams(process, "parr_process");
	params += "&pv_origin=" + org + "&pv_dest=" + dst;
	//alert(process.length);
	//console.log(params);
	submitAjax("wfui_act_cons_ajax.change_assignment_process_sub", params);
}
/*
 *@criacao 2013.04.12 10:57 daniel.silva
 *
 *
 */
function showHideTree(obj) {

	if (obj.getObjTab('[id*=tree]').parent().is(":visible")) {
		obj.getObjTab('[id*=tree]').parent().hide();
		obj.getObjTab('[id*=list]').css("width", "98%");
		//@alteracao jose.duarte 2016.07.08 GP 110581 alterar a imagem que aparece quando escondemos a árvore
		$(obj).attr("src", 'imagens/wfui_main_view_tree_icon_off');
		resizeAll();
	} else {
		obj.getObjTab('[id*=tree]').parent().show();
		obj.getObjTab('[id*=list]').css("width", "78%");
		//@alteracao jose.duarte 2016.07.08 GP 110581 alterar a imagem que aparece quando escondemos a árvore
		$(obj).attr("src", 'imagens/wfui_main_view_tree_icon');
		resizeAll();
	}
}


//jose.duarte Função que faz o refresh À árvore da consola do WF
function refreshTree(pv_show_all_acts_cons) {
    var l_user_id = "";
    var lv_show_all_acts_cons;
    if (pv_show_all_acts_cons != undefined) {
		lv_show_all_acts_cons = pv_show_all_acts_cons;
    }
    else {
		lv_show_all_acts_cons = $(console_acts).attr("showallacts");
    }
	if (wfui_obj.getObjTab(".wfui_container").attr("wfui_user_id")) {
        l_user_id = wfui_obj.getObjTab(".wfui_container").attr("wfui_user_id");
    }
    var lv_apl_name = wfui_obj.getObjTab(".wfui_container").attr("wfui_apl_name");
    ajaxRequest({
        parametros: "pv_qtype=" + wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions") + "&pv_user_id=" + l_user_id + "&pv_apl_name=" + lv_apl_name + "&pv_user_id=" + l_user_id + "&pv_apl_name=" + lv_apl_name + "&pv_show_all_acts_cons=" + lv_show_all_acts_cons + "&pv_query=" + wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions"),
        div_actualizacao: $("div[id^='wfui_act_cons_tree']").attr("id"),
        showLoading: true,
        procedimento: 'wfui_act_cons_tree_ajax.show_tree'
    });
}

function filterConsoleActs(obj) {
    var lv_action;
    var lvShowAllActs = obj.attr('showAllActs');
    if (lvShowAllActs == 'Y') {
        lv_action = 'N';
        $(obj).attr('showAllActs', 'N');
		$(obj).attr("src", 'imagens/wfui_no_filter_console_acts');
    } else {
        lv_action = 'Y';
        $(obj).attr('showAllActs', 'Y');
		$(obj).attr("src", 'imagens/WFUI_FILTER_CONSOLE_ACTS');
    }
    ajaxRequest({
        parametros: "qtype=" + wfui_obj.getObjTab(".wfui_container").attr("wfui_complex_conditions") + "&query=" + wfui_obj.getObjTab(".wfui_container").attr("wfui_conditions") + "&pv_user_id=" + "" + "&pv_show_all_acts_cons=" + lv_action,
        div_actualizacao: wfui_obj.getObjTab(".wfui_container").parent().attr("id"),
        showLoading: true,
        procedimento: 'wfui_act_cons_ajax.act_current_list_view'
    });
    refreshTree(lv_action);
}

