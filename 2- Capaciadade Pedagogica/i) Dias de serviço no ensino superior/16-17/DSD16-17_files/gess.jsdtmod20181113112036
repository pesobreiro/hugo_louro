}

//cristiano.martins 2011-11-26
function add_atoms_to_profile(obj) {
	//get selected lines
	var arr = jQuery(obj).getObj("#gessi_vld_atoms_main_atoms_main_div_p_grid")
		.find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	var params = "";
	var total = 0;
	if (arr.length == 0)
		return;

	for (i = 0; i < arr.length; i++) {
		if (arr[i] && arr[i] != "") {
			if (total > 0)
				params += ";";
			params += arr[i];
			total++;
		}
	}
	// var params = getArrGetParams(arr,"parr_atoms_id");
	//alert(params);
	//submitAjax('gessi_vld_atoms_main.profile_add_multi_atoms_sub',params);
	loadBlockOnFaceBox({
		name: 'GESSI_VLD_ATOMS_MAIN_ATOMS_MAIN_DIV_P_FORM_ATOM_TO_PROFILES',
		parr_name: new Array('PV_IDS'),
		parr_value: new Array(params),
		title: 'Adicionar Atomos ao Perfil',
		mode: 'E'
	});
}

function update_atom_modulo_cmbx() {
	if (!myLastObj.val()) {
		myLastObj.getObj("#gessi_vld_atoms_main_atoms_main_div_p_form_amo_id").children().attr('disabled', 'disabled');
		myLastObj.getObj("#gessi_vld_atoms_main_atoms_main_div_p_form_amo_id").val("");
	} else {
		myNextRefreshBlockFun = "myLastObj.getObj('#gessi_vld_atoms_main_atoms_main_div_p_form_amo_id').children().removeAttr('disabled');";
		refreshComponent({
			name: 'gessi_vld_atoms_main_atoms_main_div_p_form_amo_id',
			parr_name: ['PV_APL_ID'],
			parr_value: [myLastObj.val()],
			mode: 'E'
		});
	}
}

