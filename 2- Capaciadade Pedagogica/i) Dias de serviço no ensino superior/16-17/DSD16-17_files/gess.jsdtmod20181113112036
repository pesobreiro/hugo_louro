		jQuery(this).attr("rel", "");
	});
}
/*
 * criacao 2001.11.18 17:00 daniel.silva
 */
function centerDialog() {
	jQuery(".ui-dialog").position({
		my : "center",
		at : "center",
		of : window
	});

	/*alteracao cristiano.martins 2016.04.21 GP109525 - MultiFacebox*/
	if (jQuery(window).width() > 1024) {
		var left = 0;
		var ln_divs = jQuery('[id*=div_dialog][class="ui-dialog-content ui-widget-content"]');

		if (ln_divs.length > 1) {

			dialog.parent().addClass('ui-dialog-secondary');
			jQuery(".ui-dialog-secondary").position({
				my : "right",
				at : "center",
				of : window
			});

			left = jQuery(".ui-dialog-secondary").css('left');
			left = left.replace('px', '');
			left = Number(left) - 10;
			left = left + 'px';

			jQuery(".ui-dialog-secondary").css('left', left);

			if (ln_divs.length === 2) {
				jQuery('div[id*=div_dialog]:first').parent().addClass('ui-dialog-primary');
				jQuery(".ui-dialog-primary").position({
					my : "left",
					at : "center",
					of : window
				});

				left = jQuery(".ui-dialog-primary").css('left');
				left = left.replace('px', '');
				left = Number(left) + 10;
				left = left + 'px';

				jQuery(".ui-dialog-primary").css('left', left);
			}
		}
	}
}

function resizeAll() {
	resizeAllGrids();
}
function actAll() {
	/*
	 *@alteracao 2014.09.03 14:59 daniel.silva
	 * checks if the startsWith function, if not, creates it
	 * startsWith function Only exists in Firefox
	 */
	if (typeof String.prototype.startsWith != 'function') {
		// see below for better implementation!
		String.prototype.startsWith = function (str) {
			return this.indexOf(str) == 0;
		};
	}

	//Inicializa todas as Tabs
	allTabViews();
	//Inicializa todos progressBar
	allprogressBars();
	//Inicializa todos os Forms Vals
	allFormVals();
	//Inicializa todos os Forms Ajax
	allFormAjax();
	//Inicializa todos os Accordions
	allAccordion();
	//Inicializa todas as QTips
	allToolTips();
	//Inicializa todos os timepickers
	try {
		all_timepickers();
	} catch (e) {};
	//Inicializa todos os autocomplets
	allAutoCompletes();
	//Inicializa todos os datepickers
	allDatePickers();
	//FW esconder todos as opções de admin
	jQuery().ready(function () {
		jQuery('.all_hide').hide();
		jQuery('.all_show').show();
		jQuery('.options_admin').hide();
		jQuery('.title_admin').hide();
	});
	//remover todas as tooltips cujo target já não exista
	if (closeToolTipOnLoad) {
		jQuery('[role="tooltip"]').each(function () {
			if (jQuery(document).has(jQuery(this).qtip("api").elements.target).length == 0) {
				jQuery(this).remove();
			}
		});
	}
	closeToolTipOnLoad = true;

	/*alteracao 2013.03.13 daniel.silva*/
	if (!gvBindMouseDown) {
		hideAllToolTip();
		gvBindMouseDown = true;
	}
	//Inicializa todos os SliderDiv
	allSliderDiv();
	//Inicializa todos os PopUpObj
	allPopUpObj();
	//Inicializa todos os CLEditor
	allCLEditor();
	//Inicializa todos as ProgressBar por Ajax
	allProgressBar();
	//alteracao 2013.07.19 14:57 daniel.silva
	//Inicializa todos os colorPickers
	allColorPickers();
	//alteracao 2014.07.10 10:08 daniel.silva
	allCheckButtons();
	//Resize all Objects
	resizeAll();
	//2013.10.24 17:12 daniel.silva
	//View jqgrid Header on right click
	//jqgridViewHeader();
	//Center Dialog
	centerDialog();
	//2012.07.20 14:56 daniel.silva
	//Update gvMyLastWindow with the last window clicked
	actMyLastWindow();
	//alteracao 2014.07.22 11:08 daniel.silva
	enable_input_submit();
	//2013.07.04 11:26 daniel.silva
	//remove all script tags
	rmScriptTags();
	//remove empty block titles
	rmEmptyBlockTitles();
	//alteracao 2015.01.08 11:24 daniel.silva
	//force focus to autocomplete elements
	focusAutocomplete();

}

//Função que fecha todas as faceboxes
function closeFacebox(obj) {
	/*alteracao cristiano.martins 2016.04.21 GP109525 */
	var ln_divs = jQuery('[id*=div_dialog][class="ui-dialog-content ui-widget-content"]');
	var lb_centerDialog = false;
	if (ln_divs.length > 1) {
		lb_centerDialog = true;
	}
	/*--------------------------------------*/
	//alteracao 2013.06.28 daniel.silva
	if (!obj) {
		if (jQuery(dialog).hasClass("ui-dialog-content")) {
			jQuery(dialog).dialog("destroy").remove();
		}
	} else {
		if (jQuery(obj).hasClass("ui-dialog-content")) {
			jQuery(obj).dialog("destroy").remove();
		}
	}
	/*alteracao 2013.03.13 daniel.silva*/
	closeZombieToolTip();

	/*alteracao cristiano.martins 2016.04.21 GP109525 */
	if (lb_centerDialog) {
		centerDialog();
	}
	/*-------------------------*/
}

function toggle_divs(idTab1, idTab2) {
	jQuery('#' + idTab1).hide();
	jQuery('#' + idTab1).css({
		visibility : "hidden"
	});
	jQuery('#' + idTab2).show();
	jQuery('#' + idTab2).css({
		visibility : "visible"
	});
	centerDialog();
}

//This functions makes an ajax request in the window context of the object obj to the selector element
jQuery.fn.extend({
	//This function return any selector object inside the current window or facebox,
	//or if its not inside any window or facebox then return the document object
	getObj : function (selector) {
		try {
			if (jQuery(this).closest(".window").find(selector).size() > 0)
				return jQuery(this)
				.closest(".window").find(selector);
			// @modified fabio.maia 2016.04.22 GP 109525
			// necessário ir buscar o div_dialog_content e depois o seu pai para obter o div_dialog, uma vez que agora temos sempre "div_dialog_<time>"
			else if (jQuery(this).closest("[id*=div_dialog_content]").parent().find(selector).size() > 0)
				return jQuery(this).closest("[id*=div_dialog_content]").parent().find(selector);
			else
				return jQuery(selector);
		} catch (e) {
			return jQuery(selector);
		}
	},
	//This function makes a toogle div inside the window of the object
	toggleDivs : function (selector1, selector2) {
		jQuery(this).getObj(selector1).hide().css({
			visibility : "hidden"
		});
		jQuery(this).getObj(selector2).show().css({
			visibility : "visible"
		});
	}
});

/*Jquery DatePicker Functions*/
var noWorkingDaysCadNre = new Array();

function getNoWorkingDays(rel_cad_nre, pv_year, pv_url) {

	var val = null;
	jQuery.ajax({
		async : false,
		global : false,
		url : pv_url,
		data : {
			pv_cad_nre : rel_cad_nre,
			pv_year : pv_year
		},
		success : function (data) {
			val = data.split(' ');
		}
	});

	return val;

}

function noWorkingDays(date, rel_cad_nre, pv_url, pv_id_dt_min, pv_id_dt_max) {
	if (pv_url) {
		try {
			if (!noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()]) {
				var noWorkDays = null;
				noWorkDays = getNoWorkingDays(rel_cad_nre, date.getFullYear(), pv_url);

				for (var i = 0; i < noWorkDays.length; i++) {
					var daysArray = noWorkDays[i].split(',');

					var tmp2 = new Array();
					tmp2[daysArray[0]] = new Array(daysArray[1], daysArray[2], daysArray[3]);

					noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()] = jQuery.extend(noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()], tmp2);

				}

			}
		} catch (err) {
			noWorkingDaysCadNre[rel_cad_nre] = [[]];
			noWorkingDaysCadNre[rel_cad_nre][0] = [];
			if (!noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()]) {
				var noWorkDays = null;
				noWorkDays = getNoWorkingDays(rel_cad_nre, date.getFullYear(), pv_url);

				for (var i = 0; i < noWorkDays.length; i++) {
					var daysArray = noWorkDays[i].split(',');

					var tmp2 = new Array();
					tmp2[daysArray[0]] = new Array(daysArray[1], daysArray[2], daysArray[3]);

					noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()] = jQuery.extend(noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()], tmp2);

				}

			}
		}
	}

	var dt_min;
	var dt_max;

	try {
		dt_min = jQuery("#" + pv_id_dt_min).datepicker("getDate");
	} catch (err) {}
	try {
		dt_max = jQuery("#" + pv_id_dt_max).datepicker("getDate");
	} catch (err) {}

	if (!!dt_min && !!dt_max && dt_min.length !== 0 && dt_max.length !== 0) {
		if ((dt_min && dt_min.getTime() == date.getTime()) || (dt_max && dt_max.getTime() == date.getTime())) {
			return [true, "ui-state-selectedday"];
		}
		if ((dt_min && dt_max) && (dt_min <= date && date <= dt_max)) {
			return [true, "ui-state-selecteddays"];
		}
	}
	if (pv_url) {
		var m = date.getMonth() + 1;
		if (m < 10)
			m = "0" + m;
		var d = date.getDate();
		if (d < 10)
			d = "0" + d;
		var y = date.getFullYear();

		var dateTmp = "" + y + "-" + m + "-" + d;

		if (noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()][dateTmp]) {
			var myBoolean = true;
			if (noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()][dateTmp][0] == "false")
				myBoolean = false;
			else
				myBoolean = true;

			return [myBoolean, noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()][dateTmp][1], noWorkingDaysCadNre[rel_cad_nre][date.getFullYear()][dateTmp][2]];
		} else
			return [true, ""];
	} else
		return [true, ""];

}

function datepickerDefaults() {
	jQuery.datepicker.setDefaults(jQuery.datepicker.regional[""]);
	jQuery.datepicker.setDefaults(jQuery.datepicker.regional[gv_isoLang]);
	jQuery.datepicker.setDefaults({
		//alteracao 2013.03.06 12:08 daniel.silva
		//dateFormat : 'yy-mm-dd',
		dateFormat : gv_datepicker_mask,
		//showOn: 'both',
		//buttonImageOnly: true,
		//buttonImage: 'imagens/CalcOn',
		//buttonText: 'Calendário',
		showButtonPanel : true,
		changeMonth : true,
		changeYear : true,
		showOtherMonths : true,
		selectOtherMonths : true,
		showWeek : true,
		//yearRange: 'c-50:c+10',
		firstDay : 1,
		//showAnim: "blind",
		hideIfNoPrevNext : true,
		/*onClose: function(selectedDate) {
		jQuery( this ).blur();
		var altDate = 0;
		try {
		var instance = jQuery( this ).data( "datepicker" );
		if(selectedDate){
		if( new Date(selectedDate) <= new Date(jQuery(this).datepicker( "option", "minDate" )) ){
		jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
		jQuery.datepicker._defaults.dateFormat, new Date(jQuery(this).datepicker( "option", "minDate" ))));
		altDate = 1;
		}
		if( new Date(selectedDate) >= new Date(jQuery(this).datepicker( "option", "maxDate" )) ){
		jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
		jQuery.datepicker._defaults.dateFormat, new Date(jQuery(this).datepicker( "option", "maxDate" ))));
		altDate = 1;
		}
		}
		} catch (err){}
		jQuery( this ).blur();
		try {
		var instance = jQuery( this ).data( "datepicker" );
		var mydate = jQuery.datepicker.parseDate( instance.settings.dateFormat || jQuery.datepicker._defaults.dateFormat,
		selectedDate, instance.settings );
		if(altDate==0){
		jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
		jQuery.datepicker._defaults.dateFormat, mydate));
		}
		} catch (err){
		jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
		jQuery.datepicker._defaults.dateFormat, new Date()));
		}
		jQuery( this ).blur();
		},*/
	});
}
/*Utilities*/
//Função que permite alterar todos os checks das checkboxes com nome NAME e colocar igual ao check da checkbox ELEM
function chg_checkbox(elem, name) {
	jQuery(name).each(function () {
		this.checked = jQuery(elem).prop("checked");
	});
}
//Função que permite correr uma função de x em x tempo
function ajaxTimerRefresh() {
	var AjaxRefresh = new Object();
	AjaxRefresh.AjaxRefreshID = 0;
	AjaxRefresh.Start = function (funcstr, interval_time) {
		AjaxRefresh.AjaxRefreshID = setInterval(funcstr, interval_time);
	};
	AjaxRefresh.Stop = function () {
		clearInterval(AjaxRefresh.AjaxRefreshID);
	};
	return AjaxRefresh;
}

//this function sends an array over Get with the identified param
function getArrGetParams(arr, param) {
	var i = 0;
	var total = 0;
	var params = "";
	for (i = 0; i < arr.length; i++) {
		if (arr[i] && arr[i] != "") {
			if (total > 0)
				params += "&";
			params += param + "=" + arr[i];
			total++;
		}
	}
	return params;
}
//This function checks if a value exists in the array
function existsValInArray(value, arr) {
	var i = 0;
	for (i = 0; i < arr.length; i++) {
		if (arr[i] == value)
			return true;
	}
	return false;
}
/*
 *@criacao 2013.07.19 14:55 daniel.silva
 *<div rel="colorPicker" relColorPicked="red">
 *	<input type="hidden" name="xxx" rel="colorPicked"/>
 *</div>
 */
function allColorPickers() {
	jQuery("[rel=colorPicker]").each(function () {
		var that = jQuery(this);
		var initColor = that.attr("relColorPicked");
		that.addClass("hoverBox");
		that.wColorPicker({
			onSelect : function (color) {
				that.find("[rel=colorPicked]").val(color);
			},
			initColor : initColor,
			mode : "hover",
			effect : "none",
			theme : "white"
		});
		that.removeAttr("rel");
	});
}

/*
 *@criacao 2013.07.19 14:55 daniel.silva
 *<div rel="colorPicker" relColorPicked="red">
 *	<input type="hidden" name="xxx" rel="colorPicked"/>
 *</div>
 */
function showpagehelp(pageId) {
	jQuery("img[rel=pagehelp]")
	.removeAttr("rel")
	.show()
	.attr("onclick", "openWindow('gessi_pages_main.help_page','pv_page_id=" + pageId + "','Página de Ajuda',null,400,500);event.stopPropagation();");
}
/*
 *@criacao 2014.07.10 10:08 daniel.silva
 */
function allCheckButtons() {
	jQuery("[rel=check_button]").each(function () {
		var that = jQuery(this);
		that.button();
		that.removeAttr("rel");
	});
}
/*
 *@criacao 2014.07.22 11:24 daniel.silva
 */

function enable_input_submit() {
	jQuery("[rel_sub_form=yes]").each(function () {
		var that = jQuery(this);
		that.find("input").keydown(function (e) {
			if (e.keyCode == 13) {
				jQuery(this).parents('form').submit();
				return false;
			}
		});
		that.removeAttr("rel_sub_form");
	});
}
