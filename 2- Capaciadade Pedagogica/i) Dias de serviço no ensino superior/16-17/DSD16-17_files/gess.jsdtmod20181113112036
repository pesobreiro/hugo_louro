				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'SELECT':
				result = elem.find('select').val();
				break;
			case 'TEXTAREA':
				result = elem.find('textarea').val();
				break;
			default:
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
		};
		return result;
	}
});
jQuery.fn.extend({
	getValue: function () {
		var elem = jQuery(this);
		var result = '';
		switch (elem.attr('fw_type').toUpperCase()) {
			case 'AC':
				result = elem.find('input:hidden').val();
				break;
			case 'BUTTON':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'CHECKBOX':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'DATA':
				result = jQuery.datepicker.formatDate(elem.find('input').data("datepicker").settings.dateFormat || jQuery.datepicker._defaults.dateFormat, elem.find('input').datepicker("getDate"));
				break;
			case 'DOMD':
				result = elem.find('select').val();
				break;
			case 'DOMG':
				result = elem.find('select').val();
				break;
			case 'ERRO':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'FILE':
				result = elem.find('input').val();
				break;
			case 'VGO':
				result = elem.find('select').val();
				break;
			case 'HIDDEN':
				result = elem.val();
				break;
			case 'HORA':
				result = elem.find('[rel=timepicker]').val();
				break;
			case 'IMG':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'INFO':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'INPUT':
				result = elem.find('input').val();
				break;
			case 'LABEL':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'LINK':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'OPTION':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'PASSWORD':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'PROC':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'SELECT':
				result = elem.find('select').val();
				break;
			case 'TEXTAREA':
				result = elem.find('textarea').val();
				break;
			default:
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
		};
		return result;
	}
});

jQuery.fn.extend({
	refreshTextAreaCount: function (force) {
		var text = jQuery(this).val();
		var lentghmin = jQuery(this).attr('lengthmin');
		var lentghmax = jQuery(this).attr('lengthmax') || text.length;

		if (text.length > lentghmax || text.length < lentghmin) {
			//set span red
			if (jQuery(this).next().next().find('#length_min').hasClass('textarea-count-red')) {
				;
			} else {
				jQuery(this).next().next().find('#length_min').addClass('textarea-count-red');
			}
			if (text.length > lentghmax) {
				jQuery(this).val(text.substr(0, lentghmax));
			}
		} else {
			jQuery(this).next().next().find('#length_min').removeClass('textarea-count-red');
		}
		jQuery(this).next().next().find('#length_min').html(text.length);
	}
});

function ajaxProgressBar(p_elem) {
	var val,
		refresher = ajaxTimerRefresh();
	refresher.url = p_elem.attr('pg_url');
	refresher.params = p_elem.attr('pg_param');
	refresher.time = p_elem.attr('pg_time');
	refresher.pgbar = p_elem;
	refresher.calls = 0;
	function refreshProgressBar() {
		/* {
		value: 100 ,
		texto:"Concluido" ,
		status:"I",
		img="Y",
		callback="jsfun",
		callback100="jsfun",
		}
		 */
		jQuery.ajax({
			type: 'POST',
			url: refresher.url,
			data: refresher.params,
			//dataType : "json",
			cache: false,
			success: function (resp) {
				//alert(resp);
				//console.log(resp);
				val = jQuery.parseJSON(resp);
				refresh();
			}
		});
		refresher.calls = refresher.calls + 1;
	}
	function refresh() {
		l_stop = false;

		if (val.value >= 100) {
			lv_html = "<div style='text-align: center;'>";

			if (val.img == "Y") {
				lv_html = lv_html + '<img class="border0" title="" alt="" src ="imagens/fw_progress_bar_finished">';
			}
			if (val.texto) {
				lv_html = lv_html + val.texto;
			}
			lv_html = lv_html + "</div>";
			refresher.pgbar.replaceWith(lv_html);

			if (val.callback100 && refresher.calls > 1) {
				try {
					eval(val.callback100);

				} catch (err) { }
			}
			l_stop = true;

		} else {
			//verify if element in page
			if (refresher.pgbar.closest("html").length > 0) {
				refresher.pgbar.progressbar({
					value: parseInt(val.value)
				});

				lv_html = "";
				if (val.img == "Y") {
					if (val.status == "I") {
						lv_html = lv_html + '<img class="border0" title="" alt="" src ="imagens/fw_progress_bar_warning">' + "&nbsp;&nbsp;";
					}
				}
				lv_html = lv_html + val.value + "%";

				$pgbar = jQuery(refresher.pgbar);
				$pgbar.css("position", "relative");

				$pgbar.find('.ui-progressbar-value').css("position", "absolute");
				$pgbar.find("#percText").remove();

				$pgbar.append('<div id="percText" class="" style="text-align: center; position: absolute; width: 100%; color: rgb(23, 94, 184); font-weight: bold; top: 50%; transform: translateY(-50%);">' + lv_html + '</div>');

			} else {
				l_stop = true;
			}

			if (val.callback && refresher.calls > 1) {
				try {
					eval(val.callback);

				} catch (err) { }
			}

		}
		if (val.status == "I") {
			l_stop = true;
		}
		if (l_stop) {
			refresher.Stop();
		}
	};
	this.go = function () {
		refresher.Start(refreshProgressBar, refresher.time * 1000);
	};
	this.go();
	refreshProgressBar();
}


function formatNumber(number, n, x, s, c) {
	var re = '\\d(?=(\\d{' + (x || 3) + '})+' + (n > 0 ? '\\D' : '$') + ')',
		num = parseFloat(number).toFixed(Math.max(0, ~~n));
	return (c ? num.replace('.', c) : num).replace(new RegExp(re, 'g'), '$&' + (s || ','));
};



function getKeys(obj) {
	temp = [];
	jQuery.each(obj, function (key, value) {
		temp.push(key);
	});
	return temp;
}

function getValues(obj) {

	temp = [];
	jQuery.each(obj, function (key, value) {
		temp.push(value);
	});
	return temp;
}



/**********************moved from fw_interface_builder_js.geral****************************/

function OnChangeAplCmbx() {
	if (myLastObj.val()) {
		refreshComponent({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_modulo',
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()]
		});
		refreshComponent({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_pagina',
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()]
		});
		refreshComponent({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_bloco',
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()]
		});
	}
}
function OnChangeModCmbx() {
	if (myLastObj.val()) {
		var sel_apl_id = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_apl').getValue();
		refreshComponent({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_pagina',
			parr_name: ['pv_apl_id', 'pv_mod_id'],
			parr_value: [sel_apl_id, myLastObj.val()]
		});
		refreshComponent({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_bloco',
			parr_name: ['pv_apl_id', 'pv_mod_id'],
			parr_value: [sel_apl_id, myLastObj.val()]
		});
	}
}
function OnChangePagCmbx() {
	if (myLastObj.val()) {
		var sel_apl_id = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_apl').getValue();
		var sel_mod_id = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_modulo').getValue();
		refreshComponent({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_bloco',
			parr_name: ['pv_apl_id', 'pv_mod_id', 'pv_pag_id'],
			parr_value: [sel_apl_id, sel_mod_id, myLastObj.val()]
		});
	}
}

function OnChangeTipoComponentCmbx() {
	//cristiano.martins 2011-10-28 09:50

	if (myLastObj.val()) {
		var tipo = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_geral_tipo').getValue();
		var TabNome = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_geral_tipo').find('select').find('option').filter(':selected').html();
		refreshBlock({
			name: 'fw_interface_builder_geral_show_component_ins_tabs_tipo_dados',
			parr_name: new Array('pv_tipo'),
			parr_value: new Array(tipo),
			tabs: 'S',
			mode: 'E'
		});
		jQuery('#fw_interface_builder_geral_show_component_ins_tabs').find('li').last().find('a').find('span').html(TabNome);
	}
}

