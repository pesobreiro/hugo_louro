var gv_act_f_icon = 'WFUI_ACTIVITY_F.png';
var gv_act_n_icon = 'WFUI_ACTIVITY_N.png';
var gv_act_i_icon = 'WFUI_ACTIVITY_I.png';
var gv_act_or_icon = 'WFUI_ACTIVITY_OR.png';
var gv_act_and_icon = 'WFUI_ACTIVITY_AND.png';
var gv_act_swp_icon = 'WFUI_ACTIVITY_SWP.png';
var gv_act_wswp_icon = 'WFUI_ACTIVITY_WSWP.png';
var gv_root_icon = 'WFUI_CONS_ROOT_NODE.png';
var gv_eng_icon = 'WFUI_CONS_ENG_NODE.png';
var gv_mdl_icon = 'WFUI_CONS_MDL_NODE.png';

function mdlNHstClick(mdlId, n, count) {
	pedidoAjax("wfui_act_std_ajax.act_nhist", "pi_ins_mdl_id=" + mdlId + "&pi_n=" + n + "&pi_max_n=" + count, "wfui_details_wfm", false, true, 0, true);

	$(".scroll-content-item").each(function () {
		if ($(this).hasClass("active")) {
			$(this).removeClass("active");
		}
		$("#wfui_item" + n).addClass("active");
	});
}

