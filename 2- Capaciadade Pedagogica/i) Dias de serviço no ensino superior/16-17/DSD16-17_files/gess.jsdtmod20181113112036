function refreshJobDetails(pv_job_id, pv_duration, pv_date, pv_has_docs, pv_dt_label) {
	jQuery('#duration_' + pv_job_id + '').html(pv_duration);
	jQuery('#date_' + pv_job_id + '').html(pv_date);
	jQuery('#label_date_' + pv_job_id + '').html(pv_dt_label);
	if (pv_has_docs == 'Y') {
		jQuery('#docs_' + pv_job_id + '').removeAttr("style");
	} else {
		jQuery('#docs_' + pv_job_id + '').css("display", "none");
	}
}

function openWindowPDF(options) {

	var l_params = "";
	l_params = "pv_procedure=" + options.procedure;
	l_params = l_params + "&" + options.params;

	openWindow("!FW_INTERFACE_BUILDER_GERAL.PDF_VIEWER", l_params, options.title);

}

function pageContentEdit(p_page_id,p_content_id) {

    var params= {};

    params.p_page_id = p_page_id;
    params.p_id = p_content_id;
    
    myNextPostAjaxFun="location.reload();";

    var options = {
        procedimento: 'web_gessi_estatico.edit_content',
        parametros: $.param(params),
        showInBox: true
    };

    ajaxRequest(options);

}

function pageContentDisableConfirm(p_content_id) {

    var params= {};

    params.p_id = p_content_id;

    jQuery("#content_disable_confirm").html( '<p class="formulariobotoes"><input type="submit" onclick="pageContentDisable(' + p_content_id + ')" value="Sim">&nbsp;&nbsp;&nbsp;&nbsp;<input type="submit" onclick="jQuery(\'#content_disable_confirm\').dialog(\'close\')" value="Não"></p>' );

    jQuery("#content_disable_confirm").dialog({title:"Confirmar inactivação?"});

}

function pageContentDisable(p_content_id) {

    var params= {};

    params.p_id = p_content_id;

    myNextRefreshAjaxFun="location.reload();";

    var options = {
        procedimento: 'web_gessi_estatico.disable_content_sub',
        parametros: $.param(params),
        showInBox: false,
        div_actualizacao:"",
        showLoading:true
    };

    ajaxRequest(options);

}

