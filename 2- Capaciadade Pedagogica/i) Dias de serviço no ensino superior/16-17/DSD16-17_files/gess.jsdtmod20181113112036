	jQuery("[rel=autocomplete]").each(function () {
		try {
			eval(jQuery(this).attr("relacfun"));
		} catch (err) {
			//console.log(err);
		}
	});
	jQuery("[rel=autocomplete]").attr("rel", "");
}
function allDatePickers() {
	datepickerDefaults();
	jQuery("[rel=datepicker]").each(function () {
		var myMinDate;
		var myMaxDate;

		if (jQuery(this).attr("rel_input_dt_min_id") && jQuery(this).attr("rel_input_dt_max_id")) {
			if (this.id == jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).attr("ID")) {
				try {
					myMinDate = jQuery(this).attr("rel_dt_min");
					myMaxDate = jQuery("#" + jQuery(this).attr("rel_input_dt_max_id")).val();

				} catch (err) {}
			} else {
				try {
					myMinDate = jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).val();
					myMaxDate = jQuery(this).attr("rel_dt_max");
				} catch (err) {}
			}
		}

		if (!myMinDate)
			myMinDate = jQuery(this).attr("rel_dt_min");
		if (!myMaxDate)
			myMaxDate = jQuery(this).attr("rel_dt_max");
		try {
			jQuery(this).datepicker({
				onSelect : function (selectedDate) {
					var instance = jQuery(this).data("datepicker");
					var date = jQuery.datepicker.parseDate(
							instance.settings.dateFormat ||
							jQuery.datepicker._defaults.dateFormat,
							selectedDate, instance.settings);
					if (jQuery(this).attr("rel_input_dt_min_id") && jQuery(this).attr("rel_input_dt_max_id")) {
						var option = this.id == jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).attr("ID") ? "minDate" : "maxDate";

						if (this.id == jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).attr("ID")) {
							try {
								jQuery("#" + jQuery(this).attr("rel_input_dt_max_id")).datepicker("option", option, date);
								jQuery("#" + jQuery(this).attr("rel_input_dt_max_id")).datepicker("option", "defaultDate", date);
							} catch (err) {}
						} else {
							try {
								jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).datepicker("option", option, date);
								jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).datepicker("option", "defaultDate", date);
							} catch (err) {}
						}
					}
				},
				beforeShowDay : function (date) {

					return noWorkingDays(date,
						jQuery(this).attr("rel_cad_nre"),
						jQuery(this).attr("rel_nw_url"),
						jQuery(this).attr("rel_input_dt_min_id"),
						jQuery(this).attr("rel_input_dt_max_id"));
				},
				onClose : function (selectedDate) {
					myLastDateObj = jQuery(this);
					var l_next_fun = jQuery(this).attr("rel_next_fun");
					try {
						eval(l_next_fun + "(myLastDateObj)");
					} catch (err) {}

					var altDate = 0;
					try {
						var instance = jQuery(this).data("datepicker");
						if (selectedDate) {
                                                    
							/*if (new Date(selectedDate) <= new Date(jQuery(this).datepicker("option", "minDate"))) {
								jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
										jQuery.datepicker._defaults.dateFormat, jQuery.datepicker.parseDate(instance.settings.dateFormat || jQuery.datepicker._defaults.dateFormat,
											jQuery(this).datepicker("option", "minDate"), instance.settings)));
								altDate = 1;
							}
							if (new Date(selectedDate) >= new Date(jQuery(this).datepicker("option", "maxDate"))) {
								jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
										jQuery.datepicker._defaults.dateFormat, jQuery.datepicker.parseDate(instance.settings.dateFormat || jQuery.datepicker._defaults.dateFormat,
											jQuery(this).datepicker("option", "maxDate"), instance.settings)));
								altDate = 1;
							}*/
						
                                                         //Alterado por David.ricardo GP 115679 em 29/01/19  
                                                        if (this.id == jQuery("#" + jQuery(this).attr("rel_input_dt_min_id")).attr("ID")) {
                                                            if (new Date(selectedDate) >= new Date(jQuery(this).datepicker("option", "maxDate"))) {
								jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
									jQuery.datepicker._defaults.dateFormat, jQuery(this).datepicker("option", "maxDate")));
                                                                altDate = 1;
                                                            }
                                                         }
                                                         else if (this.id == jQuery("#" + jQuery(this).attr("rel_input_dt_max_id")).attr("ID")) {
                                                                if (new Date(selectedDate) <= new Date(jQuery(this).datepicker("option", "minDate"))) {
                                                                    jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
                                                                    jQuery.datepicker._defaults.dateFormat, jQuery(this).datepicker("option", "minDate")));
                                                                    altDate = 1;
                                                            }
                                                         }
                                                         //Fim GP 115679  
                                                }
					} catch (err) {}
					
                                        try {
						var instance = jQuery(this).data("datepicker");
						var mydate = jQuery.datepicker.parseDate(instance.settings.dateFormat || jQuery.datepicker._defaults.dateFormat,
								selectedDate, instance.settings);
                                                
						if (altDate == 0) {
                                                    
							jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
									jQuery.datepicker._defaults.dateFormat, mydate));
						}
                                                
                                                
					} catch (err) {
						jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
								jQuery.datepicker._defaults.dateFormat, new Date()));
					}
                                        if(altDate == 1) {
                                            //Sem este Blur há uma situação na que permite ultrapassar a validção,
                                            //Ao escrever uma data inválida e fazendo enter ele valida e fecha o picker,
                                            //se depois do picker fechar escrevermos esse texto não é validadado, com este blur nunca conseguimos escrever sem abrir o picker
                                            jQuery(this).blur();
                                        }
				},
				minDate : myMinDate,
				maxDate : myMaxDate
			});
		} catch (err) {}
		jQuery(this).bind('change', function () {
			var instance = jQuery(this).data("datepicker");
			jQuery(this).val(jQuery.datepicker.formatDate(instance.settings.dateFormat ||
					jQuery.datepicker._defaults.dateFormat, jQuery(this).datepicker("getDate")));

		});
	});

	jQuery("[rel=datepicker]").attr("rel", "");
	//comentado em 2013.02.07 12:34 daniel.silva
	//jQuery("#ui-datepicker-div").hide();
}
/*
 * @criacao 2011.10.27 15:21 daniel.silva
 */
function allSliderDiv() {
	jQuery("[rel=sliderDiv]").each(function () {

		var scrollPane = jQuery(this).find(".scroll-pane");
		var scrollContent = jQuery(this).find(".scroll-content");

		var scrollContentMinWith = 0;
		scrollContent.children().each(function (index) {
			scrollContentMinWith = scrollContentMinWith + jQuery(this).width() + 22;
		});
		scrollContent.css("min-width", scrollContentMinWith);

		//build slider
		var scrollbar = jQuery(this).find(".scroll-bar").slider({

				slide : function (event, ui) {
					if (scrollContent.width() > scrollPane.width()) {
						scrollContent.css("margin-left", Math.round(
								ui.value / 100 * (scrollPane.width() - scrollContent.width())) + "px");
					} else {

						scrollContent.css("margin-left", 0);
					}
				}
			});
		//disable slider if needed
		if (scrollContent.width() > scrollPane.width()) {
			scrollbar.slider("option", "disabled", false);
		} else {
			scrollbar.slider("option", "disabled", true);
		}

		//append icon to handle
		var handleHelper = scrollbar.find(".ui-slider-handle")
			.mousedown(function () {
				scrollbar.width(handleHelper.width());
			})
			.mouseup(function () {
				scrollbar.width("100%");
			})
			.append("<span class='ui-icon ui-icon-grip-dotted-vertical'></span>")
			.wrap("<div class='ui-handle-helper-parent'></div>").parent();

		//change overflow to hidden now that slider handles the scrolling
		scrollPane.css("overflow", "hidden");

		//size scrollbar and handle proportionally to scroll distance
		function sizeScrollbar() {
			var remainder = scrollContent.width() - scrollPane.width();
			if (remainder < 0) {
				remainder = 0;
			}
			var proportion = remainder / scrollContent.width();
			var handleSize = scrollPane.width() - (proportion * scrollPane.width());

			if (remainder > 0) {
				scrollbar.find(".ui-slider-handle").css({
					width : handleSize,
					"margin-left" : -handleSize / 2
				});
			} else {
				scrollbar.find(".ui-slider-handle").css({
					width : handleSize - 5,
					"margin-left" : 0
				});
			}
			handleHelper.width("").width(scrollbar.width() - handleSize);
		}

		//reset slider value based on scroll content position
		function resetValue() {
			var remainder = scrollPane.width() - scrollContent.width();
			var leftVal = scrollContent.css("margin-left") === "auto" ? 0 :
				parseInt(scrollContent.css("margin-left"));
			var percentage = Math.round(leftVal / remainder * 100);
			scrollbar.slider("value", percentage);
		}

		//change handle position on window resize

		resetValue();
		sizeScrollbar();

		jQuery(this).attr("rel", "");
	})

}

function resizeAllGrids() {
	larr_ = {};
	if (grid = jQuery('.ui-jqgrid-btable:visible')) {
		var gap = 2;
		var minWidth = 650;
		grid.each(function (index) {
			minWidth = 650;
			// @modified fabio.maia 2016.04.22 GP 109525
			// necessário ir buscar o div_dialog_content e depois o seu pai para obter o div_dialog, uma vez que agora temos sempre "div_dialog_<time>"
			if (jQuery(this).closest("[id*=div_dialog_content]").parent().length == 0) {
				minWidth = 100;
			}
			gridId = jQuery(this).attr('id');
			jQuery(this).jqGrid('setGridWidth', minWidth);
			//alteracao 2012.05.25 15:39 daniel.silva
			jQuery('#' + jQuery(this).jqGrid("getGridParam", "gessi_usr_pager_id") + '_left').css('width', '');
		});
		//alteracao 2014.09.04 16:39 daniel.silva
		grid.each(function (index) {
			gridId = jQuery(this).attr('id');
			gridParentWidth = jQuery('#gbox_' + gridId).parent().width();
			larr_[gridId] = gridParentWidth;
		});
		grid.each(function (index) {
			gridId = jQuery(this).attr('id');
			jQuery(this).jqGrid('setGridWidth', larr_[gridId] - gap);
		});

		//var gapHeight = 35;
		//var rowHeight = 25;

		grid.each(function (index) {
			//	var minRows = jQuery(this).attr("minRows");
			//	var maxRows = jQuery(this).attr("maxRows");
			//	var gridCount = jQuery(this).jqGrid("getGridParam", "reccount");
			//	var gridRows;

			//	if (!minRows)
			//		minRows = 5;
			//	if (!maxRows)
			//		maxRows = 15;

			//	if (gridCount < minRows)
			//		gridRows = minRows;
			//	else if (gridCount > maxRows)
			//		gridRows = maxRows;
			//	else
			//		gridRows = gridCount;

			//jQuery(this).jqGrid('setGridHeight', Math.min(410,parseInt(jQuery(".ui-jqgrid-btable").css('height'))));
			jQuery(this).parents('div.ui-jqgrid-bdiv').css("max-height", "410px");
			jQuery(this).parents('div.ui-jqgrid-bdiv').css("min-height", "160px");
			//jQuery(this).jqGrid('setGridHeight', gridRows * rowHeight + gapHeight);

		});

	}
	//alteracao 2014.09.03 16:22 daniel.silva
	//add class to sorted cols
	jQuery('.ui-jqgrid-hdiv:visible').find("[role=columnheader]").each(function () {
		if (jQuery(this).find(".ui-grid-ico-sort").not(".ui-state-disabled").length > 0) {
			jQuery(this).addClass("jqgrid-col-sorted");
		} else {
			jQuery(this).removeClass("jqgrid-col-sorted");
		}
	})
}
/*
 * criação 2013.10.24 17:08 daniel.silva
 * View grid header on right click
 */
function jqgridViewHeader() {

	var grid = jQuery('.ui-jqgrid-btable:visible');

	if (grid.length != 0) {
		var bdiv = grid.closest(".ui-jqgrid-bdiv");
		var hdiv = jQuery(grid[0].grid.hDiv);
		var hdivClone;
		grid.bind("mousedown", function (e) {
			try {
				try {
					hdivClone.remove();
				} catch (e) {}
				switch (e.which) {
					//right click
				case 3:
					e.stopPropagation();
					e.preventDefault();
					var hOffset = hdiv.offset();
					hdivClone = hdiv.clone();
					hdivClone.find("tr:nth-child(2)").remove();
					hdivClone.css("position", "absolute");
					hdivClone.css("z-index", "2147483647");
					hdiv.after(hdivClone);

					hdivClone.css({
						top : e.pageY - hOffset.top - 25
					});
					hdivClone.scrollLeft(bdiv.scroll().scrollLeft());
					break;
				default:
					break;
				}
			} catch (e) {}
		});
		//disable context menu
		grid.closest("[id*=gbox_generator_grid]").bind("contextmenu", function (e) {
			return false;
		});
		grid.bind("mouseup", function (e) {
			try {
				hdivClone.remove();
			} catch (e) {}
		});
	}
}

/*
 *criacao 2001.10.10 daniel.silva
 */
function viewPopUpObj(obj) {
	//Height on Mouse Enter
	var heightEnter = obj.attr("relHeightEnter");
	//Height on Mouse Leave
	var heightLeave = obj.attr("relHeightLeave");
	//default if not defined
	if (!heightEnter) {
		heightEnter = "390px";
	}
	//default if not defined
	if (!heightLeave) {
		heightLeave = "";
	}
	//
	obj.css("min-width", obj.parent().css("width"));
	obj.parent().css("position", "relative");
	obj.parent().css("height", "350px");
	obj.addClass("popupview");
	obj.css("overflow", "");
	//unbind
	obj.unbind('mouseenter mouseleave');
	//bind
	obj.hover(function () {
		//setTimeout(function () {
		/*obj.css("top", "-1px");
		obj.css("left", "-1px");
		obj.css("z-index", "9002");
		obj.css("background-color", "#FFFFFF");
		obj.css("border", "1px solid #CCCCCC");
		obj.css("border-radius", "4px");
		obj.css("padding-right", "20px");
		//obj.css("padding-left", "15px");
		obj.css("height", "100%");*/

		if (heightEnter > obj.css("height")) {
			var tmpHeight = "";
		} else {
			var tmpHeight = heightEnter;
		}
		obj.css("height", tmpHeight);
		/*obj.css("overflow-y", "auto");
		obj.css("overflow-x", "hidden");*/

		//}, 10)

	}, function () {

		//setTimeout(function () {
		/*obj.css("top", "0px");
		obj.css("left", "0px");
		obj.css("z-index", "auto");
		obj.css("background-color", "");
		obj.css("border", "");
		obj.css("border-radius", "");
		obj.css("border", "");
		obj.css("padding-right", "");
		obj.css("padding-left", "");*/
		obj.css("height", heightLeave);
		/*obj.css("overflow-y", "hidden");*/
		//}, 70)

	});
}
/*
 *criacao	2011.11.09	17:33	daniel.silva
 */
function allPopUpObj() {
	jQuery("div[rel=PopUpObj]").each(function () {
		viewPopUpObj(jQuery(this));
		jQuery(this).attr("rel", "");
	})

}
/*
 *@criacao 2012.07.20 14:56 daniel.silva
 * Function that bind all elements with onclick and all windows to update the variable  gvMyLastWindow
 */
function actMyLastWindow() {

	jQuery(".window").find("[onclick]").not('[relLocation]').each(function (index) {
		jQuery(this).mousedown(function () {
			gvMyLastWindow = jQuery(this);
		});
		jQuery(this).attr('relLocation', 'true');
	});
	jQuery('.window').not('[relLocation]').each(function (index) {

		jQuery(this).mousedown(function () {
			gvMyLastWindow = jQuery(this);
		});
		jQuery(this).attr('relLocation', 'true');
	});

}
/*
 *@criacao 2013.07.04 11:13 daniel.silva
 * Function that removes all elements script with no scr attribute
 */
function rmScriptTags() {
	jQuery("body").find("script").each(function () {
		if (!jQuery(this).prop("src")) {
			jQuery(this).remove();
		}
	});
}
/*
 *@created ivo.marinho 22-09-2014
 * Function that removes all block Titles that are empty
 */
function rmEmptyBlockTitles() {
	jQuery(".content-block-notitle:visible").each(function () {
		if (jQuery(this).html().replace(/(\r\n|\n|\r)/gm, "") == '<h2><span class="content-block-options"></span></h2>') {
			jQuery(this).remove();
		}
	});
}
/*
 *criacao	2012.11.09	17:33	duarte.ferreira
 *alteracao	2012.Mar.30	11:25	joao.barreto
 * Adicionados os botões default. Permite-se assim que outras aplicações introduzam botões custom sem afectar o comportamento default da framework.
 */
function allCLEditor() {
	jQuery("[rel=cleditor]").each(function () {
		l_controls = "bold italic underline strikethrough subscript superscript | font size " +
			"style | color highlight removeformat | bullets numbering | outdent " +
			"indent | alignleft center alignright justify | undo redo | " +
			"rule image link unlink | cut copy paste pastetext | print ";
		l_fonts = "Arial,Arial Black,Comic Sans MS,Courier New,Arial Narrow,Garamond," +
			"Georgia,Impact,Serif,Tahoma,Trebuchet MS,Verdana";
		l_useCSS = false;
		l_width = 800;

		jQuery.cleditor.defaultOptions.width = l_width;
		jQuery.cleditor.defaultOptions.controls = l_controls;
		jQuery.cleditor.defaultOptions.fonts = l_fonts;
		jQuery.cleditor.defaultOptions.useCSS = l_useCSS;

		if (typeof jQuery(this).attr("rel_options") !== "undefined") {
			jQuery.cleditor.defaultOptions.controls = jQuery(this).attr("rel_options");
		}

		jQuery(this).cleditor();
		jQuery(this).attr("rel", "");
		jQuery(this).attr("rel_options", "");

	})

}
