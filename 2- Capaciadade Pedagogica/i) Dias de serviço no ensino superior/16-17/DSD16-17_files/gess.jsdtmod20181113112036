/***************************moved from fw_interface_builder_js.usr_prefs_grids_js***********************************/

function submitForm(form_name) {
	jQuery('form[name=' + form_name + ']').ajaxSubmit(postAjaxOptions);
}

function clearFilterDefaultLine(obj) {

	var line_number = jQuery(obj).attr("line_number");
	var obj_value = "pv_value_" + line_number;
	var obj_delimiter = "pv_del_" + line_number;

	jQuery("input[name=" + obj_value + "]").val("");
	jQuery("select[name=" + obj_delimiter + "]").val("Igual a|=|");
}

function clearFormElements(form_name) {

	jQuery(form_name).find(":input").each(function () {
		switch (this.type) {

			case "text":
				jQuery(this).val("");
				break;
			case "select-multiple":
			case "select-one":
				jQuery(this).val("Igual a|=|");
				break;
			default:
				break;
		}
	});
}

function replaceValueData(obj, block_name) {

	var line_number = jQuery(obj).closest("tr").attr("line_number");
	var name_ac = "pv_attr_" + line_number;
	var value_ac = jQuery("input[name=" + name_ac + "]").val();
	var name_value = "pv_value_" + line_number;
	var val_value = jQuery("input[name=" + name_value + "]").val();
	var result;
	var params = "pv_filter_block_name=" + block_name + "&pv_line_number=" + line_number + "&pv_value=" + value_ac;

	if (value_ac.toString() != "" && val_value.toString() == "") {

		var input_obj = jQuery(obj).closest("td").siblings("[rel=valor]").find("input");
		var td_obj = jQuery(obj).closest("td").siblings("[rel=valor]");
		//alert("test1");
		result = ajaxReturn('fw_interface_builder_geral.new_filter_row_val', params);
		//alert("test2");
		td_obj.html(result);
		//alert("test3");
	}
	actAll();
}

/******************************moved from gessi_vld_atoms_js.profiles_main***********************************/

function vldprofiles_addProfileUsers(obj, profileID) {
	//get selected lines
	var arr = jQuery(obj).getObj("#gessi_vld_atoms_main_profiles_main_profile_notin_users_grid")
		.find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	if (arr.length == 0 || profileID == null)
		return;

	myLastObj = jQuery(obj);
	myNextPostAjaxFun = "jQuery(myLastObj).getObj('#gessi_vld_atoms_main_profiles_main_profile_notin_users_grid').find('.ui-jqgrid-btable').trigger('reloadGrid');";
	var params = getArrGetParams(arr, "parr_users_id");
	params += "&pv_profile_id=" + profileID;
	submitAjax('gessi_vld_atoms_main.profile_add_multi_users_sub', params);
}
function vldprofiles_addProfileAtoms(obj, profileID) {
	//get selected lines
	var arr = jQuery(obj).getObj("#gessi_vld_atoms_main_profiles_main_profile_notin_atoms_grid")
		.find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	if (arr.length == 0 || profileID == null)
		return;

	myLastObj = jQuery(obj);
	myNextPostAjaxFun = "jQuery(myLastObj).getObj('#gessi_vld_atoms_main_profiles_main_profile_notin_atoms_grid').find('.ui-jqgrid-btable').trigger('reloadGrid');";
	var params = getArrGetParams(arr, "parr_atoms_id");
	params += "&pv_profile_id=" + profileID;
	submitAjax('gessi_vld_atoms_main.profile_add_multi_atoms_sub', params);
}
//CM - 30-06-2011 - Refresh da combo "modulo"
function update_modulo_combo() {
	if (!myLastObj.val()) {
		myLastObj.getObj("#gessi_vld_atoms_main_profiles_main_profileslist_pesquisa_form_modulo").children().attr('disabled', 'disabled');
		myLastObj.getObj("#gessi_vld_atoms_main_profiles_main_profileslist_pesquisa_form_modulo").val("");

	} else {
		//myNextRefreshBlockFun="myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_profileslist_pesquisa_form_modulo').children().attr('disabled', 'false');";
		myNextRefreshBlockFun = "myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_profileslist_pesquisa_form_modulo').children().removeAttr('disabled');";
		refreshComponent({
			name: 'gessi_vld_atoms_main_profiles_main_profileslist_pesquisa_form_modulo',
			parr_name: ['PV_APL'],
			parr_value: [myLastObj.val()]
		});
	}

}

function update_modulo_combo2() {
	if (!myLastObj.val()) {
		myLastObj.getObj("#gessi_vld_atoms_main_profiles_main_vld_grupos_form_vld_grupos_modulo").children().attr('disabled', 'disabled');
		myLastObj.getObj("#gessi_vld_atoms_main_profiles_main_vld_grupos_form_vld_grupos_modulo").val("");
	} else {
		//alert(myLastObj.val());
		//myNextRefreshBlockFun="myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_vld_grupos_form_vld_grupos_modulo').children().attr('disabled', '');";
		myNextRefreshBlockFun = "myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_vld_grupos_form_vld_grupos_modulo').children().removeAttr('disabled');";
		refreshComponent({
			name: 'gessi_vld_atoms_main_profiles_main_vld_grupos_form_vld_grupos_modulo',
			parr_name: ['PV_APL_ID'],
			parr_value: [myLastObj.val()],
			mode: 'E'
		});
	}
}

function update_atmos_pag_combo() {
	if (!myLastObj.val()) {
		myLastObj.getObj("#gessi_vld_atoms_main_profiles_main_profile_notin_atoms_pesquisa_form_pag").children().attr('disabled', 'disabled');
		myLastObj.getObj("#gessi_vld_atoms_main_profiles_main_profile_notin_atoms_pesquisa_form_pag").val("");

	} else {
		//myNextRefreshBlockFun="myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_profile_notin_atoms_pesquisa_form_pag').children().attr('disabled', '');";
		myNextRefreshBlockFun = "myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_profile_notin_atoms_pesquisa_form_pag').children().removeAttr('disabled');";
		refreshComponent({
			name: 'gessi_vld_atoms_main_profiles_main_profile_notin_atoms_pesquisa_form_pag',
			parr_name: ['PV_MOD'],
