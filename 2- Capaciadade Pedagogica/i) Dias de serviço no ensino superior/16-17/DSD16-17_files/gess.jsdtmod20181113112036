/**
 * Plugin CLEeditor para permitir a criação/inserção de documentos nas páginas via ajax
 *
 **/
var myLastCleditor; 

 // funcao para atualizar a listagem de documentos da respectiva pagina
function actualiza_listagem_documentos(pv_page_id) {
	var pv_time = Date.now();
	if (pv_page_id)
		jQuery("#tab5").html(jQuery.ajax({
				url : "gessi_pages_ajax.show_documentos_pagina?pv_page_id=" + pv_page_id + "&pv_time=" + pv_time,
				type : "GET",
				async : false
			}).responseText);
}
// funcao para eliminar documenta de uma pagina
function eliminar_documento_pagina(pv_codigo, pv_page_id) {
	var confirma;
	confirma = window.confirm("Tem a certeza que deseja retirar o documento desta página?");
	if (confirma == true) {
		var pv_time = Date.now();
		jQuery.ajax({
			url : "gessi_pages_ajax.del_documentos_pagina?pv_page_id=" + pv_page_id + "&pv_time=" + pv_time + "&pv_codigo=" + encodeURIComponent(pv_codigo),
			type : "POST",
			async : false,
			success : function (data) {
				actualiza_listagem_documentos(pv_page_id);
			}
		});
	}
}
// funcao para fechar a popup da listagem de documentos da pagina
function fecha_popup() {
	jQuery("#cleditor_doc_list").parent().hide();
}
// funcao para inserir o novo documento na pagina
function insere_documento_pagina(p_obj) { 
	var pv_codigo = jQuery(p_obj).closest("#cleditor_doc_list").find("[name=p_codigo]").val();
	myLastCleditor.execCommand('inserthtml', '<br>##' + pv_codigo + '##', null, null);
}
// funcao para ir buscar os documentos via ajax
function get_documentos_pagina(p_page_id) {
	var pv_page_id = p_page_id;
	var pv_time = Date.now();

	if (pv_page_id)
		return jQuery.ajax({
			url : "gessi_pages_ajax.get_documentos_pagina?pv_page_id=" + pv_page_id + "&pv_time=" + pv_time,
			type : "GET",
			async : false
		}).responseText;
}
// trata de selecionar ou retirar a selecao das checkboxs nas linhas, consoante a checkbox do cabecalho
function trata_selecao_header() {
	var estado = jQuery("input:checkbox[name=\"selecao_header\"]").prop("checked");
	jQuery("input:checkbox[name=\"selecao\"]").each(function () {
		jQuery(this).prop("checked", estado);
	});
}
(function (jQuery) {
	// definicao do botao de insercao de documentos
	jQuery.cleditor.buttons.addDocument = {
		name : "addDocument",
		image : "CLEDITOR_BUTTONS_ADDDOC",
		title : "Adicionar documento",
		command : "inserthtml",
		popupName : "Adicionar documento",
		popupClass : "cleditorPrompt",
		//popupContent : get_documentos_pagina(),
		popupContent : "<div id=\"cleditor_doc_list\"></div>",
		buttonClick : addDocumentClick
	};
	
	// adicionar o botão no final
	// jQuery.cleditor.defaultOptions.controls = jQuery.cleditor.defaultOptions.controls.concat(" | addDocument");

	// tratamento do clique no botao
	function addDocumentClick(e, data) {
		// carrega documentos
		l_pag_id = data.editor.$main.find("textarea").attr("relPageId");
		
		myLastCleditor = data.editor;
		
		jQuery("#cleditor_doc_list").html(get_documentos_pagina(l_pag_id));
		jQuery(data.popup).find("input[type=button]").unbind("click").bind("click", function (e) {
			// vai buscar o editor/iframe
			var editor = data.editor;
			// coloca a checkbox do cabecalho nao selecionada
			jQuery("input:checkbox[name=\"selecao_header\"]").prop("checked", false);
			// vai buscar os documentos selecionados e coloca as checkbox nao selecionadas
			var documentos = "";
			jQuery("input:checkbox[name=\"selecao\"]:checked").each(function () {
				documentos = documentos + "<br>##" + jQuery(this).val() + "##";
				jQuery(this).prop("checked", false);
			});
			// insere os documentos no editor
			if (documentos) {
				editor.execCommand(data.command, documentos, null, data.button);
			}
			// esconde a popup de insercao
			editor.hidePopups();
			editor.focus();
		});
	}
})(jQuery);

function progressBars(obj) {
  /*variables to get values from the HTML*/
  var id_job = parseInt(obj.children("#progressbar").children("#progress-bar").attr("id_job"));
  var maxElements = parseInt(obj.children("#progressbar").children("#progress-bar").attr("maxElements"));
  var numElements = parseInt(obj.children("#progressbar").children("#progress-bar").attr("numElements"));
  var timeExecution = parseInt(obj.children("#progressbar").children("#progress-bar").attr("timeExecution"))
  obj.children("#legend").children("#completionPhrase").hide();

  var num = 0;

  //Timer for execution
  var id = setInterval(loaded, timeExecution);

  function loaded() {
    numElements = ajaxReturn("gessi_jobs_main.return_perc_elements", "pv_id=" + id_job).trim();
    num = numElements * 100 / maxElements;
    num = num.toFixed(0);

    //Print in the interfaces the changes of the progress bar
    obj.children("#progressbar").children("#progress-bar").css("width", num + "%");
    obj.children("#legend").attr('data-value', num);

    obj.children("#legend").children("#elements").children("#maxElements").text(maxElements);
    obj.children("#legend").children("#elements").children("#numElements").text(numElements);

    // When the progressbar get max value e change colors a present message that is terminate
    if (num >= 100) {
      obj.children("#legend").addClass("animate-opacity text-green");
      obj.children("#legend").children("#completionPhrase").show();
      obj.children("#legend").children("#elements").hide();
      clearInterval(id);
    }
  }
}

