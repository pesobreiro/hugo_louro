function vldAtoms_addInstituicoes(obj, atomID) {
	var arr = jQuery(obj).getObj("#gessi_vld_atoms_main_atoms_main_div_s_add_inst_grid").find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	if (arr.length == 0 || atomID == null)
		return;
	myLastObj = jQuery(obj);
	myNextPostAjaxFun = "jQuery(myLastObj).reloadGrid('gessi_vld_atoms_main_atoms_main_div_s_add_inst_grid')";
	var params = getArrGetParams(arr, "parr_inst_id");
	params += "&pv_atom_id=" + atomID;
	submitAjax('gessi_vld_atoms_main.atom_add_multi_inst_sub', params);
}

