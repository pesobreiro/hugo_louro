//hack to allow the click on qtip elements
jQuery.widget("ui.dialog", jQuery.ui.dialog, {
	_allowInteraction : function (event) {
		return !!jQuery(event.target).closest(".qtip").length || jQuery(event.target).closest(".cleditorPopup").length || this._super(event);
	}
});

jQuery.fn.dialogGessi = function (title) {
	if (!title)
		title = '';
	jQuery(this).dialog({
		title : title,
		height : 'auto',
		width : 'auto',
		position : 'center',
		//alteracao 2013.06.28 daniel.silva
		//deprecated
		//zIndex : 300000,
		modal : true,
		close : function (event, ui) {
			if (myNextCloseFaceboxFun !== "") {
				try {
					eval(myNextCloseFaceboxFun);
				} catch (err) {}
			}
			myNextCloseFaceboxFun = "";
			/*alteracao cristiano.martins 2016.04.20 GP109525*/
			//closeFacebox();
			closeFacebox(jQuery(this));
		},
		//alteracao 2014.07.18 daniel.silva
		//set max size
		maxHeight : jQuery(window).height() - 20,
		maxWidth : jQuery(window).width() - 20,

	});
};

function validadeResponseText(responseText) {
	if (responseText.indexOf("##GESSISEMPERMISSAO##") >= 0) {
		window.location.reload();
		return false;
	}
	return true;
}

function blockUI() {
	jQuery.blockUI({
		/*message : '<h1>' + gv_waitingMsg2 + '</h1>',*/
		message : '<center><div class="bubblingG"><span id="bubblingG_1"></span><span id="bubblingG_2"></span><span id="bubblingG_3"></span></div><br><div class="blockui-text"><p>' + gv_waitingMsg1 + '</p><p>' + gv_waitingMsg2 + '</p></div></center>',
		overlayCSS : {
			opacity : 0.8,
			'z-index' : '400000'
		},
		css : {
			border : 'none',
			padding : '15px',
			backgroundColor : 'transparent',
			'-webkit-border-radius' : '10px',
			'-moz-border-radius' : '10px',
			'z-index' : '400000',
			color : '#FFFFFF',
			width : 'auto',
			top : '35%',
			left : '35%'
		}
	});
}

function valForm(formData, jqForm, options) {
	myNextPostAjaxValue = null;
	myCurrJQForm = jqForm;
	if (jQuery(jqForm).validarForm(true)) {
		blockUI();
		return true;
	}
	return false;
}
/*alteracao cristiano.martins 2016.04.20 GP109525*/
/*function valPostResponse(responseText, statusText) {
jQuery.unblockUI();
responseText = responseText.replace("\n", "");
showFormResponse(responseText);
}*/
function valPostResponse(responseText, statusText, xhr, form) {
	jQuery.unblockUI();
	responseText = responseText.replace("\n", "");
	showFormResponse(responseText, form);
}
/*-----------------------------------------------------------*/

function errorPostResponse(responseText, statusText) {
	jQuery.unblockUI();
	showNotification(errorSubMsg);
	//jQuery(dialog).html(errorSubMsg).dialogGessi();
}

function showFormResponse(responseText, obj) {
	execMyNextPostAjaxFun = false;
	if (responseText.indexOf("##GESSISEMPERMISSAO##") >= 0) {
		window.location.reload();
		return;
	}
	// @modified fabio.maia 2016.04.22 GP 109525
	// necessário verificar se é passado o obj, pois em caso de não ser deve ir buscar a última facebox a ser criada
	// pois no caso do submitAjax não é passado nenhum objeto identificador
	// var divfacebox = jQuery(obj).closest('[id*=div_dialog_content]');
	var divfacebox = null;
	if (obj !== undefined) {
		divfacebox = jQuery(obj).closest('[id*=div_dialog_content]');
	} else {
		divfacebox = jQuery('[id*=div_dialog_content]:last');
	}
	if (divfacebox.length > 0){
		var divDialog = divfacebox.parent();
	}else{
		divfacebox = null;
		var divDialog = null;
	}
	// @end
	if (divfacebox != null && myNextPostAjaxTarget == null) {
		if (responseText.indexOf("##GESSIERRO##") >= 0 || responseText.indexOf("$ERRO$") >= 0) {
			responseText = responseText.replace("##GESSIERRO##", "");
			responseText = responseText.replace("$ERRO$", "");
			//jQuery('#div_dialog_sub')

			jQuery(obj).closest('[id*=div_dialog_content]').parent().find('[id*=div_dialog_sub]')
			.html(responseText
				 + '<p class="formulariobotoes"><input type="button" value="' + gv_backMsg + '"'
				//+ 'onclick="javascript:toggle_divs(\'div_dialog_sub\',\'div_dialog_content\');"></p>');

				 + 'onclick="javascript:toggle_divs(\'' + jQuery(obj).closest('[id*=div_dialog_content]').parent().find('[id*=div_dialog_sub]').attr('id') + '\', \'' + jQuery(obj).closest('[id*=div_dialog_content]').attr('id') + '\');"></p>');
			toggle_divs(jQuery(obj).closest('[id*=div_dialog_content]').attr('id'), jQuery(obj).closest('[id*=div_dialog_content]').parent().find('[id*=div_dialog_sub]').attr('id'));
		} else {
			execMyNextPostAjaxFun = true;
			execMyNextPostAjaxFun2 = true;
			showNotification(responseText, divDialog);
		}
		/*toggle_divs('div_dialog_content','div_dialog_sub');*/
	} else {
		if (responseText.indexOf("##GESSIERRO##") < 0 && responseText.indexOf("$ERRO$") < 0) {
			execMyNextPostAjaxFun = true;
			execMyNextPostAjaxFun2 = true;
			if (myNextPostAjaxTarget != null) {
				jQuery(myNextPostAjaxTarget).html(responseText);
				actAll();
			}
		} else {
			responseText = responseText.replace("##GESSIERRO##", "");
			responseText = responseText.replace("$ERRO$", "");
		}
		if (responseText.length > 0 && myNextPostAjaxTarget == null)
			showNotification(responseText);
	}
	//alteracao daniel.silva 2014.08.22 16:30
	//refresh job notifications
	if (responseText.indexOf("##NEW_GESSI_JOB##") > 0) {
		refreshNotsJobs()
	}

	evalMyNextPostAjaxFun();
	evalMyNextPostAjaxFun2();
	myNextPostAjaxTarget = null;
}
function evalMyNextPostAjaxFun() {
	if (myNextPostAjaxFun != "" && execMyNextPostAjaxFun) {
		try {
			eval(myNextPostAjaxFun);
			myNextPostAjaxFun = "";
		} catch (err) {}
	}
	execMyNextPostAjaxFun = false;
}
function evalMyNextPostAjaxFun2() {
	if (myNextPostAjaxFun2 != "" && execMyNextPostAjaxFun2) {
		try {
			eval(myNextPostAjaxFun2);
			myNextPostAjaxFun2 = "";
		} catch (err) {}
	}
	execMyNextPostAjaxFun2 = false;
}
function evalMyNextRefreshAjaxFun() {
	if (myNextRefreshAjaxFun != "") {
		try {
			eval(myNextRefreshAjaxFun);
		} catch (err) {}
	}
	myNextRefreshAjaxFun = "";
}
function evalMyNextRefreshAjaxFun2() {
	if (myNextRefreshAjaxFun2 != "") {
		try {
			eval(myNextRefreshAjaxFun2);
		} catch (err) {}
	}
	myNextRefreshAjaxFun2 = "";
}

function showNotification(text, obj) {
	if (closeFaceboxOnNotification)
		closeFacebox(obj);

	if (text.length > 0 && execShowNotification) {
		//if already exists the notification panel
		if (jQuery("#notification-panel").length)
			jQuery("#notification-content").find("#msg-body").html(text);
		else
			jQuery("#conteudo").append('<div id="dialog-panel"><div id="dialog-content">' + text + '</div></div>');
		jQuery("#dialog-panel").dialog({
			dialogClass : "no-close",
			buttons : [{
					text : "Voltar",
					click : function () {
						jQuery(this).dialog("close");
					}
				}
			],
			title : "Lista de Erros"
		});
		try {
			eval(myNotifShowFun);
		} catch (err) {}
	}
	execShowNotification = true;
	closeFaceboxOnNotification = true;
}
function hideNotification() {
	jQuery("#notification-panel").fadeOut('slow');
}

function pedidoAjax(procedimento, parametros, div_actualizacao, showInBox, force, delay, showLoading) {
	var options = {
		procedimento : procedimento,
		parametros : parametros,
		div_actualizacao : div_actualizacao,
		showInBox : showInBox,
		force : force,
		delay : delay,
		showLoading : showLoading
	};
	ajaxRequest(options);
}

// TODO: Change the div_actualizacao for any Jquery valid selector
function ajaxRequest(options) {

	if (!options.showInBox && typeof(options.div_actualizacao) != 'object' && options.div_actualizacao.charAt(0) != ".")
		options.div_actualizacao = "#" + options.div_actualizacao;

	myNextPostAjaxValue = null;
	var msec = 0;
	var title = '';
	//var actObj = jQuery(options.div_actualizacao);
	//2012.07.20 14:56 daniel.silva
	if (options.div_actualizacao) {
		//Check if gvMyLastWindow has defined
		if (gvMyLastWindow) {
			//Check if options.div_actualizacao has a jquery object
			if (options.div_actualizacao.jquery) {
				var l_div_act;
				//Get id of options.div_actualizacao object
				var myActId = options.div_actualizacao.attr('id');
				//If myActId has defined
				if (myActId) {
					l_div_act = "#" + options.div_actualizacao.attr('id');
				} else {
					l_div_act = options.div_actualizacao;
				}
				//get actObj using the gvMyLastWindow
				var actObj = gvMyLastWindow.getObj(l_div_act);
			} else {
				var actObj = gvMyLastWindow.getObj(options.div_actualizacao);
			}

		} else {
			//get actObj without using the gvMyLastWindow
			var actObj = jQuery(options.div_actualizacao);
		}
	}
	if (options.title)
		title = options.title;
	if (timeout)
		clearTimeout(timeout);
	if (options.delay > 0)
		msec = delay;
	if (options.showLoading == undefined)
		options.showLoading = true;
	if (options.async == undefined)
		options.async = true;

	if (options.div_actualizacao && jQuery(actObj).html() != imgHtmlLoad && options.showLoading) {
		if (options.force == null || options.force == true) {
			//alteracao 2013.03.13 18:12 daniel.silva
			try {
				jQuery(actObj).html(imgHtmlLoad);
			} catch (err) {}
		} else if (options.force == false && jQuery(options.div_actualizacao).html() == "") {
			//alteracao 2013.03.13 18:12 daniel.silva
			try {
				jQuery(actObj).html(imgHtmlLoad);
			} catch (err) {}
		}
	}
	if (options.showInBox == true)
		jQuery(loadDialog).dialogGessi(title);

	var d = new Date();
	if (options.parametros != null && options.parametros != "")
		options.parametros = options.parametros + "&pv_time=" + d.getTime();
	else
		options.parametros = "pv_time=" + d.getTime();
	if (!(options.force == false && jQuery(actObj).html() != ""))
		//timeout = setTimeout(function(){

		jQuery.ajax({
			type : "POST",
			url : options.procedimento,
			data : options.parametros,
			async : options.async,
			cache : false,
			success : function (resp) {
				resp = resp.replace("\n", "");
				if (!validadeResponseText(resp))
					return;
				if (options.showInBox == true) {
					//alteracao 2013.06.28 daniel.silva
					if (jQuery(loadDialog).hasClass("ui-dialog-content")) {
						jQuery(loadDialog).dialog("destroy").remove();
					}

					//$('body').append(jQuery'<div id="div_dialog"></div>');)
					/*alteracao cristiano.martins 2016.04.20 GP109525*/

					var date = new Date();
					div_dialog_time = date.getTime();
					var left;
					var left2 = 0;

					dialog = jQuery('<div id="div_dialog_' + div_dialog_time + '"></div>');

					var dialog_window =
						dialog.html('<div id="div_dialog_content_' + div_dialog_time + '">' + resp + '</div><div id="div_dialog_sub_' + div_dialog_time + '" style="visibility:hidden;display:none"></div>')
						.dialogGessi(title);
					//--------------------------------------------------------------------//
				} else {
					if (options.replace == true)
						jQuery(actObj).replaceWith(resp);
					else
						jQuery(actObj).html(resp);
				}

				actAll();
				evalMyNextRefreshAjaxFun();
				evalMyNextRefreshAjaxFun2();
				centerDialog();
			},
			error : function () {
				if (options.showInBox == true) {

					//alteracao 2013.06.28 daniel.silva
					if (jQuery(loadDialog).hasClass("ui-dialog-content")) {
						jQuery(loadDialog).dialog("destroy").remove();
					}
					jQuery(dialog).html(errorLoadMsg).dialogGessi(title);

				} else
					jQuery(actObj).html(errorLoadMsg);
			}
		});

	//}, msec);
}
/*Função que permite submeter informação para um procedimento*/
function submitAjax(procedimento, parametros, async) {
	myNextPostAjaxValue = null;
	jQuery.ajax({
		type : "POST",
		url : procedimento,
		data : parametros,
		cache : false,
		async : ((async || async == false) ? async : true),
		beforeSend : function () {
			blockUI();
		},
		success : function (resp) {
			if (!validadeResponseText(resp))
				return;
			valPostResponse(resp);
		},
		error : errorPostResponse
	});
}

/*Função que permite submeter informação para um procedimento*/
function submitAjaxNoResp(procedimento, parametros) {
	myNextPostAjaxValue = null;
	jQuery.ajax({
		type : "POST",
		url : procedimento,
		data : parametros,
		cache : false,
		success : function (resp) {
			if (!validadeResponseText(resp))
				return;
		}
	});
}
/*Ajax Function that returns the HTML Procedure Call*/
function ajaxReturn(procedimento, parametros) {
	var htmlresp = "";
	jQuery.ajax({
		type : "POST",
		url : procedimento,
		data : parametros,
		async : false,
		success : function (resp) {
			htmlresp = resp;
			if (!validadeResponseText(resp))
				return;
		},
		error : function () {
			htmlresp = "";
		}
	});
	return htmlresp;
}

/*Executes the ajax response*/
function evalAjax(procedimento, parametros) {
	var req = jQuery.ajax({
			type : "POST",
			url : procedimento,
			data : parametros,
			cache : false,
			success : function (resp) {
				eval(resp);
				//tries to execute an myNextEvalAjaxFun Function
				try {
					eval(myNextEvalAjaxFun);
				} catch (err) {}
				myNextEvalAjaxFun = "";
				removeAjaxRequest(req);
			},
			error : function () {
				removeAjaxRequest(req, true);
			}
		});
	myAjaxRequests.splice(0, 0, req);
}

//Aborts all pending Ajax Requests
function abortAllAjaxRequests() {
	var i = 0;
	for (i = 0; i < myAjaxRequests.length; i++) {
		myAjaxRequests[i].abort();
	}
	myAjaxRequests = [];
}

//Removes and Aborts an Ajax Request from queue
function removeAjaxRequest(req, abort) {
	var i = 0;
	for (i = 0; i < myAjaxRequests.length; i++) {
		if (myAjaxRequests[i] == req) {
			if (abort) {
				myAjaxRequests[i].abort();
			}
			myAjaxRequests = myAjaxRequests.slice(0, i).concat(myAjaxRequests.slice(i + 1));
		}
	}
	if (myAjaxRequests.length == 0) {
		actAll();
	}
}

