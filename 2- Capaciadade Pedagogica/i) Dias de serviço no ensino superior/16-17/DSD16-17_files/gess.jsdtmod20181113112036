//sysJs NameSpace
(function (window) {
	//Private Area
	var callArgs;
	
	//Public Area
	var sysJs = {
		//Get private object
		getCallArgs : function () {
			return this.callArgs;
		},
		//Set private object
		setCallArgs : function (p_args) {
			this.callArgs = p_args;
		},
		//Get private object position
		getCallArgsPos : function (p_pos) {
			return this.callArgs[p_pos];
		},
		//Load js associated with the namespace and call the function after loading
		loadNameSpace : function (ns, _f, _args) {
			var jsFile = "js/" + ns + ".js";
			
			$.ajax({
				url : jsFile,
				dataType : "script",
				async :false,
				success : function (data, textStatus, jqxhr) {
					//console.info('Load was performed...\n' + jsFile);
					var myArgs = "";
					
					sysJs.setCallArgs(_args);
					
					for (var i = 0; i < _args.length; i++) {
						if (i != 0) {
							myArgs = myArgs + ",";
						}
						myArgs = myArgs + "sysJs.getCallArgsPos(" + i + ")";
					};
					
					myArgs = ns + "." + _f + "(" + myArgs + ");";
					
					try {
						eval(myArgs);
					} catch (err) {
						console.error("Erro ao executar1: \n\n" + myArgs + "\n\n" + err);
					}
				}
			});
			/*$.getScript(jsFile, function (data, textStatus, jqxhr) {
			//console.info('Load was performed...\n' + jsFile);
			var myArgs = "";
			
			sysJs.setCallArgs(_args);
			
			for (var i = 0; i < _args.length; i++) {
			if (i != 0) {
			myArgs = myArgs + ",";
			}
			myArgs = myArgs + "sysJs.getCallArgsPos(" + i + ")";
			};
			
			myArgs = ns + "." + _f + "(" + myArgs + ");";
			
			try {
			eval(myArgs);
			} catch (err) {
			console.error("Erro ao executar1: \n\n" + myArgs + "\n\n" + err);
			}
			});*/
		}
	};
	window.sysJs = sysJs;
})(window);

//sysJs.framework NameSpace
(function (sysJs) {
	
	//--------------------------IMPORTANT-----------------------------------//
	//Public Area
	sysJs.framework = {
		exe : function () {
			sysJs.loadNameSpace("sysJs.framework", "exe", arguments);
			
		}
	}
	
})(sysJs);

//sysJs.docgestNav NameSpace
(function (sysJs) {
	
	//--------------------------IMPORTANT-----------------------------------//
	//Public Area
	sysJs.docgestNav = {
		exe : function () {
			sysJs.loadNameSpace("sysJs.docgestNav", "exe", arguments);
			
		}
	}
	
})(sysJs);

//sysJs.docgestParam NameSpace
(function (sysJs) {
	
	//--------------------------IMPORTANT-----------------------------------//
	//Public Area
	sysJs.docgestParam = {
		exe : function () {
			sysJs.loadNameSpace("sysJs.docgestParam", "exe", arguments);
			
		}
	}
	
})(sysJs);

//sysJs.docgestMng NameSpace
(function (sysJs) {
	
	//--------------------------IMPORTANT-----------------------------------//
	//Public Area
	sysJs.docgestMng = {
		exe : function () {
			sysJs.loadNameSpace("sysJs.docgestMng", "exe", arguments);
			
		}
	}
	
})(sysJs);

//sysJs.docgestTree NameSpace
(function (sysJs) {
	
	//--------------------------IMPORTANT-----------------------------------//
	//Public Area
	sysJs.docgestTree = {
		exe : function () {
			sysJs.loadNameSpace("sysJs.docgestTree", "exe", arguments);
			
		}
	}
	
})(sysJs);

