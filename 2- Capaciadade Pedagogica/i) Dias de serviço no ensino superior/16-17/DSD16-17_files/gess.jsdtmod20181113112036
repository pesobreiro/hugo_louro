	var TabNome = "";
	switch (p_type) {
		case "SVAC":
			TabNome = "Auto-Complete";
			break;
		case "MVAC":
			TabNome = "Auto-Complete (Múltiplos Valores)";
			break;
		case "BUTTON":
			TabNome = "Button";
			break;
		case "CHECKBOX":
			TabNome = "Checkbox";
			break;
		case "DATA":
			TabNome = "Data";
			break;
		case "DOMD":
			TabNome = "Domínio Designer";
			break;
		case "DOMG":
			TabNome = "Domínio Gessi";
			break;
		case "EDITOR":
			TabNome = "Editor Html";
			break;
		case "STRUCT":
			TabNome = "Estruturas";
			break;
		case "FILE":
			TabNome = "File";
			break;
		case "GRP":
			TabNome = "Grupo";
			break;
		case "VGO":
			TabNome = "Grupo de Validação";
			break;
		case "HORA":
			TabNome = "Hora";
			break;
		case "IMG":
			TabNome = "Imagem";
			break;
		case "HIDDEN":
			TabNome = "Input (Antigo Hidden)";
			break;
		case "INPUT":
			TabNome = "Input (Não USAR)";
			break;
		case "LABEL":
			TabNome = "Label";
			break;
		case "LINK":
			TabNome = "Link";
			break;
		case "PASSWORD":
			TabNome = "Password";
			break;
		case "PROC":
			TabNome = "Procedimento";
			break;
		case "PG":
			TabNome = "ProgressBar";
