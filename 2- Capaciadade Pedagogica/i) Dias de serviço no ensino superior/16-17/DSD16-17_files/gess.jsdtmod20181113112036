 *********************************************************************************************************************************************************************
 ********************************************************************************************************************************************************************/
/*
 * alteracao 2013.07.18 11:19 daniel.silva
 */
function allTabViews() {
	//alteracao 2015.01.08 17:29 daniel.silva
	//store active tab
	//converte todas as tabs da jquery
	jQuery('div[rel=tabs]').each(function () {
		lv_cookie_name = "";
		lb_storeActiveTab = false;
		if (jQuery(this).filter("[relStoreTab=Y]").length == 1) {
			lb_storeActiveTab = true;
			lv_cookie_name = jQuery(this).parent("div").attr("id");
		}

		jQuery(this).tabs({
			load : function (event, ui) {
				actAll();
			},
			//alteracao 2013.06.28 daniel.silva
			beforeLoad : function (event, ui) {
				//alteracao 2014.06.12 daniel.silva
				// condição para carregar o conteudo da tab uma única vez
				//
				if (ui.panel.html() && ui.tab.find("a[rel]").attr("rel") == "loadonce") {
					return false;
				}

				ui.panel.html(imgHtmlLoad);

				ui.jqXHR.error(function () {
					ui.panel.html(errorLoadMsg);
				});

				ui.ajaxSettings.url += "&pv_time=" + new Date().getTime();

			},
			beforeActivate : function (event, ui) {
				resizeAll();
			},
			//Reload all grids of newPanel
			activate : function (event, ui) {
				ui.newPanel.find('.ui-jqgrid-btable').each(function () {
					jQuery(this).trigger("reloadGrid");
				});
				if (lb_storeActiveTab) {
					jQuery.cookie(lv_cookie_name, ui.newTab.index(), {
						expires : 10
					});
				}
			},
			active : jQuery.cookie(lv_cookie_name)
		});

	});
	//retira o rel para não ser convertido novamente
	jQuery('div[rel=tabs]').attr("rel", "");
}

function allFormVals() {
	//converte todos os forms com o atributo rel=formvaljs
	jQuery('form[rel=formvaljs]').each(function () {
		jQuery(this).jqueryValForm();
	});
	//retira o rel para não ser convertido novamente
	jQuery('form[rel=formvaljs]').attr("rel", "");
}
function allFormAjax() {
	//converte todos os forms com o atributo rel=formajaxsubmit
	jQuery('form[rel=formajaxsubmit]').each(function () {
		jQuery(this).bind('submit', function () {
			jQuery(this).ajaxSubmit(postAjaxOptions);
			return false;
		})
		.jqueryValForm();
	});
	//retira o rel para não ser convertido novamente
	jQuery('form[rel=formajaxsubmit]').attr("rel", "");
}
function allAccordion() {

	try { //converte todos os forms com o atributo rel=accordion
		jQuery('div[rel=accordion]').each(function () {
			lActive = jQuery(this).attr("relActive");
			if (!lActive) {
				lActive = 0;
			}
			if (!isNaN(lActive)) {
				lActive = Number(lActive);
			}
			jQuery(this).accordion({
				collapsible : jQuery(this).hasClass("collapsible"),
				active : lActive,
				autoHeight : false

			});
		});
		//retira o rel para não ser convertido novamente
		jQuery('div[rel=accordion]').attr("rel", "");
	} catch (err) {}
}
/*criacao 2013.03.13 daniel.silva*/
function hideAllToolTip() {
	jQuery(document).bind('mousedown', function (event) {
		hideToolTip(jQuery(event.target));
	});
}
/*criacao 2013.03.13 daniel.silva*/
function hideToolTip(obj) {
	if (obj.parents('div.qtip').length === 0) {
		jQuery('[role="tooltip"]').each(function () {
			jQuery(this).qtip("hide");
		});
	}
}
/*criacao 2013.03.13 daniel.silva*/
function closeZombieToolTip() {
	jQuery('[role="tooltip"]').each(function () {
		if (jQuery(document).has(jQuery(this).qtip("api").elements.target).length == 0) {
			jQuery(this).remove();
		}
	});
}
/*alteracao 2013.03.13 daniel.silva*/
function allToolTips(obj) {
	var opposites = {
		bottomLeft : 'topRight',
		bottomRight : 'topLeft',
		bottomMiddle : 'topMiddle',
		topRight : 'bottomLeft',
		topLeft : 'bottomRight',
		topMiddle : 'bottomMiddle',
		leftMiddle : 'rightMiddle',
		leftTop : 'rightBottom',
		leftBottom : 'rightTop',
		rightMiddle : 'leftMiddle',
		rightBottom : 'leftTop',
		rightTop : 'leftBottom'
	};
	try { //converte todos os forms com o atributo rel=tip
		jQuery('[rel=tip]').each(function () {
			var tip = jQuery(this).attr("qtiptype");
			var hide = jQuery(this).attr("qtiphideevent");
			var show = jQuery(this).attr("qtipshow");

			if (!tip)
				tip = "leftMiddle";
			if (!hide)
				hide = "mouseout";
			if (!show)
				show = "mouseover";

			var fix = jQuery(this).attr("qtipfixed");
			var del = 0;
			if (fix) {
				fix = true;
				del = 300;
			} else
				fix = "false";

			var showObj = {
				when : {
					event : show
				},
				effect : {
					type : "fade"
				}
			};
			var hideObj = {};
			var titleObj = {};
			if (!jQuery(this).attr("qtipurl")) {
				hideObj = {
					when : {
						event : hide
					},
					fixed : fix,
					delay : del
				};
			} else {
				hideObj = false;
				titleObj = {

					text : '&nbsp;',
					button : "<img src='imagens/windowCloseOff'></img>"
				};
			}
			jQuery(this).qtip({
				show : showObj,
				hide : hideObj,
				position : {
					corner : {
						target : opposites[tip],
						tooltip : tip
					},
					adjust : {
						x : 10,
						screen : true
					}
				},
				content : {
					url : jQuery(this).attr("qtipurl"),
					method : "post",
					title : titleObj
				},
				style : {
					tip : tip,
					name : jQuery(this).attr("qtipname")
				},
				api : {
					//onContentUpdate
					onPositionUpdate : function () {
						jQuery('[role="tooltip"]').filter(":visible").each(function () {
							if (!jQuery(this).qtip("api").elements.target.hasClass("over")) {
								jQuery(this).qtip("hide");
							}
						});
					},
					//alteracao 2014.02.07 09:32 daniel.silva
					onContentUpdate : function () {
						actAll();
					}
				}
			});
		}).bind("mouseover", function (event) {
			event.preventDefault();
			return false;
		});
		jQuery('[rel=tip]').mouseover(function () {
			jQuery(this).addClass("over");
		});
		jQuery('[rel=tip]').mouseout(function () {
			jQuery(this).removeClass("over");
		});
		//retira o rel para não ser convertido novamente
		jQuery('[rel=tip]').attr("rel", "tipconverted");
	} catch (err) {}
}

function clearAllToolTips() {
	//Clears all loaded tooltips panels from the html and reloads again the new ones
	//not called yet
	jQuery('[rel=tipconverted]').attr("rel", "tip");
	jQuery('div.qtip').remove();
}

