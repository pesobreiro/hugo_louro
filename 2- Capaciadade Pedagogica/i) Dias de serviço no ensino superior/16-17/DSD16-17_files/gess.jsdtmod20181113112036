 */
function allprogressBars(){
	jQuery('div[rel=block_progress_bar]').each(function () {
		progressBars($(this));
	});

};

//total number of windows
var totalGessiWindows = 0;
var gessiWindows = [];

function openWindow(procedure, params, title, containment, height, width, windowID, refreshContent) {
	//@alteracao jose.duarte 2017.09.15 colocar o scroll no top antes de abrir 1 janela nova 
	//isto foi para corrigir quando os monitores são pequenos e os menus secundários tem muitos items as janelas ficavam invisiveis
	 $("#conteudo-inner").scrollTop(0);
	var options = {
		windowID: windowID,
		procedure: procedure,
		params: params,
		title: title,
		containment: containment,
		height: height,
		width: width,
		refreshContent: refreshContent
	};
	createWindow(options);
}

//Opens a new Window with the ajax content of procedure
function createWindow(options) {
	//checks if already exists a window with the same windowID
	if (getWindowArrayIndex(options.windowID) > -1) {
		presentWindow(options.windowID);
		if (options.refreshContent) {
			var winObj = gessiWindows[getWindowArrayIndex(options.windowID)];
			winObj.options.procedure = options.procedure;
			winObj.options.params = options.params;
			load_window_content(winObj);
		}
		return;
	}

	totalGessiWindows++;
	//saves window index
	var winIndex = totalGessiWindows;
	options.maximized = true;

	if (options.windowID == null || options.windowID == "")
		options.windowID = "window_" + winIndex;
	if (options.containment == null || options.containment == "")
		options.containment = "#conteudo-inner";

	if (options.height == null || options.height == "")
		options.height = 400;
	else options.maximized = false;

	if (options.width == null || options.width == "")
		options.width = 400;
	else options.maximized = false;

	if (options.title == null || options.title == "")
		options.title = "Janela " + winIndex;
	if (options.title == null || options.title == "")
		options.title = "Janela " + winIndex;

	//Appends the new Window object to containment
	jQuery(options.containment).append('<div class="window" id="'
		+ options.windowID
		+ '" index="'
		+ winIndex
		+ '" style="display:none;height:'
		+ options.height
		+ 'px; width:'
		+ options.width
		+ 'px; left:10px; top: 10px;" containment="'
		+ options.containment
		+ '" url="'
		+ options.procedure
		+ '" params="'
		+ options.params
		+ '"><div class="window-title"><span class="title">'
		+ options.title
		+ '</span>'
		+ '<span class="options">'
		+ '<img id="winhelpopt" src="imagens/gessi_page_help" style="display:none;" rel="pagehelp">'
		+ '<img id="winminopt" src="imagens/gessi_page_min" onclick="minimizeMyWindow(this)">'
		+ '<img id="winresizeopt" src="imagens/gessi_page_resize" onclick="maximizeMyWindow(this)">'
		+ '<img id="wincloseopt" src="imagens/gessi_page_close" onclick="closeMyWindow(this)">'
		+ '</span>'
		/*
		+ '<span class="titlebutton close" onclick="closeMyWindow(this)"></span>'
		+ '<span class="titlebutton resize" onclick="maximizeMyWindow(this)"></span>'
		+ '<span class="titlebutton minimize" onclick="minimizeMyWindow(this)"></span>'
		*/
		+ '</div><div class="window-content">'
		+ imgHtmlLoad
		+ '</div></div>');

	var myWindow = jQuery(options.containment).find("#" + options.windowID);

	myWindow.index = winIndex;
	myWindow.options = options;

	// sets containment overflow hidden to eliminate scrolls
	if (jQuery(options.containment).css("overflow") != "hidden") {
		jQuery(options.containment).css("overflow", "hidden").scrollTop(0);
	}

	//makes the window resizable and draggable
	jQuery(myWindow)
		.draggable({
			handle: "div.window-title",
			containment: "parent",
			stack: '.window',
			scroll: false,
			stop: function (event, ui) {
				saveWindowSize(jQuery(myWindow));
			}
		})
		.resizable({
			handles: 'n, e, s, w, ne, se, sw, nw',
			containment: "parent",
			minHeight: options.height,
			minWidth: options.width,
			stop: function (event, ui) {
				saveWindowSize(jQuery(myWindow));
			}
		})
		.children(".window-title").bind({
			click: function () {
				jQuery(this).parent().maxZIndex();
			},
			dblclick: function () {
				maximizeMyWindow(this);
			}
		});

	if (options.maximized) {
		//maximizes window to max width and height
		maximizeWindow(jQuery(myWindow));
	}

	//makes the window appear in the front
	jQuery(myWindow).fadeIn().maxZIndex();

	//creates the toolbar window option
	jQuery("#finaltoolbar-windows")
		.append('<input id="windowtoolbar_' + winIndex + '" type="button" value="' + options.title + '" onclick="showHideWindow(\'#' + options.windowID + '\');">');

	jQuery(myWindow).resize(function () {
		resizeWindowContent(jQuery(myWindow));
	});

	//adds this window element to array list
	gessiWindows.push(myWindow);

	load_window_content(myWindow);
}

function load_window_content(winObj) {
	//calls the Ajax Procedure
	var d = new Date();
	if (winObj.options.params != null && winObj.options.params != "")
		winObj.options.params = winObj.options.params + "&pv_time=" + d.getTime();
	else
		winObj.options.params = "pv_time=" + d.getTime();

	jQuery.ajax({
		type: "POST",
		url: winObj.options.procedure,
		data: winObj.options.params,
		cache: false,
		success: function (resp) {
			if (!validadeResponseText(resp))
				return;
			var pos = getWindowArrayIndex(jQuery(winObj));
			if (pos > -1) {
				gessiWindows[pos].children(".window-content").html(resp);
				actAll();
			}
		},
		error: function () {
			var pos = getWindowArrayIndex(jQuery(winObj));
			if (pos > -1) {
				gessiWindows[pos].children(".window-content").html(errorLoadMsg);
			}
		}
	});
}

//This functions makes an ajax request in the window context of the object obj to the selector element
jQuery.fn.extend({
	ajaxRequestOnMyWindow: function (procedure, parameters, selector, async) {
		var actElem = jQuery(this).getObj(selector);
		jQuery(actElem).html(imgHtmlLoad);
		var d = new Date();
		if (parameters != null && parameters != "")
			parameters = parameters + "&pv_time=" + d.getTime();
		else
			parameters = "pv_time=" + d.getTime();

		if (async == undefined) async = true;

		//console.log(parameters);

		jQuery.ajax({
			type: "POST",
			url: procedure,
			data: parameters,
			cache: false,
			async: async,
			success: function (resp) {
				if (!validadeResponseText(resp))
					return;
				resp = resp.replace("\n", "");
				//console.log(actElem);
				jQuery(actElem).html(resp);
				actAll();
			},
			error: function () {
				jQuery(actElem).html(errorLoadMsg);
			}
		});


	}
});

//determines the current max z-index for windows
jQuery.fn.maxZIndex = function () {
	var max = getWindowsCurrentMaxIndex();
	max++;
	jQuery(this).css("z-index", max);
};
//get from all visible window objects the max z-index
function getWindowsCurrentMaxIndex() {
	var zmax = 100;
	jQuery(".window").each(function () {
		var cur = parseInt(jQuery(this).css('z-index'));
		zmax = cur > zmax ? cur : zmax;
	});
	return zmax;
}
//checks if Window Object is currently on Top of max z-Index Windows
function isWindowMaxIndex(winObj) {
	if (jQuery(winObj).is(":visible") && getWindowsCurrentMaxIndex() == parseInt(jQuery(winObj).css('z-index'))) {
		return true;
	}
	return false;
}

function getWindowArrayIndex(winObj) {
	for (i = 0; i < gessiWindows.length; i++) {
		if (gessiWindows[i].options.windowID == jQuery(winObj).attr("id")
			|| "#" + gessiWindows[i].options.windowID == winObj
			|| gessiWindows[i].options.windowID == winObj) {

			return i;
		}
	}
	return -1;
}

function closeWindow(winObj) {
	var containment = jQuery(jQuery(winObj).attr("containment"));
	jQuery("#windowtoolbar_" + jQuery(winObj).attr("index")).remove();

	// sets containment overflow back to auto if there are any more visible windows
	if (jQuery(".window").length <= 1 && jQuery(containment).css("overflow") == "hidden") {
		jQuery(containment).css("overflow", "auto");
	}

	jQuery(winObj).remove();
	//removes from Window Array list
	for (i = 0; i < gessiWindows.length; i++) {
		if (jQuery(gessiWindows[i]).attr("id") == jQuery(winObj).attr("id")) {
			gessiWindows = gessiWindows.slice(0, i).concat(gessiWindows.slice(i + 1));
		}
	}
}

function closeMyWindow(obj) {
	closeWindow(jQuery(obj).closest(".window"));
}

function closeAllVisibleWindows() {
	var arr = jQuery(".window:visible");
	for (var k = 0; k < arr.length; k++) {
		closeWindow(arr[k]);
	}
}

function closeAllMinimizedWindows() {
	var arr = jQuery(".window").not(":visible");
	for (var k = 0; k < arr.length; k++) {
		closeWindow(arr[k]);
	}
}

function closeAllWindows() {
	for (var k = 0; k < gessiWindows.length; k++) {
		// because the window then closed is removed from the array, we have to always remove the first window (index 0)
		closeWindow(gessiWindows[0]);
	}
}

function maximizeWindow(w) {
	if (!w.hasClass("maximized")) {
		w.draggable("option", "disabled", true).resizable("option", "disabled", true);
		jQuery(w).find(".resize").removeAttr("onclick").unbind('click').bind('click', function () {
			restoreMyWindow(this);
		});
		w.children(".window-title").unbind('dblclick').bind('dblclick', function () {
			restoreMyWindow(this);
		});
		saveWindowSize(w);
	}

	//w.css({left:0,top:0,width:parseInt(w.parent().innerWidth()),height:parseInt(w.parent().innerHeight())});
	w.css({
		left: 0,
		top: 0,
		width: "99.55%",
		height: "98.75%"
	});
	w.addClass("maximized");
	resizeWindowContent(w);
}
function restoreWindow(w) {
	//padding adjustment
	var paddingAdj = 20;

	var parentWidth = parseInt(w.parent().innerWidth());
	var left = parseInt(w.attr("lastLeft"));
	var width = Math.min(parseInt(w.attr("lastWidth")), parentWidth);
	if ((width + left) > parentWidth)
		left = paddingAdj;

	var parentHeight = parseInt(w.parent().innerHeight());
	var top = parseInt(w.attr("lastTop"));
	var height = Math.min(parseInt(w.attr("lastHeight")), parentHeight);
	if ((height + top) > parentHeight)
		top = paddingAdj;
	//if its almost the same as all the parent dimensions, then it will redim a little less with the padding Adjustment
	if (width >= (parentWidth - paddingAdj))
		width = width - (paddingAdj + left);
	if (height >= (parentHeight - paddingAdj))
		height = height - (paddingAdj + top);
	// applies the new dimensions

	w.css({
		left: left,
		top: top,
		width: width,
		height: height
	});

	if (w.hasClass("maximized")) {
		jQuery(w).find(".resize").removeAttr("onclick").unbind('click').bind('click', function () {
			maximizeMyWindow(this);
		});
		w.children(".window-title").unbind('dblclick').bind('dblclick', function () {
			maximizeMyWindow(this);
		});
		w.draggable("option", "disabled", false).resizable("option", "disabled", false);
	}
	w.removeClass("maximized");
	resizeWindowContent(w);
}
function maximizeMyWindow(obj) {
	var w = jQuery(obj).closest(".window");
	maximizeWindow(w);
}
function restoreMyWindow(obj) {
	var w = jQuery(obj).closest(".window");
	restoreWindow(w);
}
//this function hides or shows window depending on its state
function showHideWindow(winObj) {
	if (jQuery(winObj).is(":visible") && !isWindowMaxIndex(winObj))
		jQuery(winObj).maxZIndex();
	else if (jQuery(winObj).is(":visible"))
		hideWindow(winObj);
	else
		showWindow(winObj);
}
//this function shows or puts window on max z-index
function presentWindow(winObj) {
	if (jQuery(winObj).is(":visible"))
		jQuery(winObj).maxZIndex();
	else
		showWindow(winObj);
}

function showWindow(winObj) {
	var pos = getWindowArrayIndex(winObj);
	if (pos > -1 && !jQuery(winObj).is(":visible")) {
		// sets containment overflow to hidden
		if (jQuery(gessiWindows[pos].attr("containment")).css("overflow") != "hidden") {
			jQuery(gessiWindows[pos].attr("containment")).css("overflow", "hidden").scrollTop(0);
		}
		gessiWindows[pos].appendTo(gessiWindows[pos].attr("containment"));
		gessiWindows[pos].fadeIn().maxZIndex();
		gessiWindows[pos].removeClass("minimized");
	}
}
function hideWindow(winObj) {
	var pos = getWindowArrayIndex(winObj);
	if (pos > -1 && jQuery(winObj).is(":visible")) {
		gessiWindows[pos].addClass("minimized").css("display", "none");

		// sets containment overflow back to auto if there are any more visible windows
		if (jQuery(".window").length <= 1 && jQuery(gessiWindows[pos].attr("containment")).css("overflow") == "hidden") {
			jQuery(gessiWindows[pos].attr("containment")).css("overflow", "auto");
		}

		gessiWindows[pos] = gessiWindows[pos].detach();
	}
}
function minimizeMyWindow(obj) {
	var w = jQuery(obj).closest(".window");
	title = w.find(".title").html();
	hideWindow(w);
}
function colapseMyWindow(obj) {
	var w = jQuery(obj).closest(".window");
	saveWindowSize(w);
	w.css("height", parseInt(w.children(".window-title").height()) + 12);
}
//refresh all the window content
function refreshMyWindow(obj, force) {
	var w = jQuery(obj).closest(".window");
	var d = new Date();
	var params = jQuery(w).attr("params");
	if (params != null && params != "")
		params = params + "&pv_time=" + d.getTime();
	else
		params = "pv_time=" + d.getTime();
	//alteracao daniel.silva 2012.08.21 12:41
	if (force == null || force == true)
		jQuery(w).children(".window-content").html(imgHtmlLoad);

	jQuery.ajax({
		type: "POST",
		url: jQuery(w).attr("url"),
		data: params,
		cache: false,
		success: function (resp) {
			if (!validadeResponseText(resp))
				return;
			jQuery(w).children(".window-content").html(resp);
			actAll();
		},
		error: function () {
			jQuery(w).children(".window-content").html(errorLoadMsg);
		}
	});
}
//changes the window params
function changeMyWindowRefreshParams(obj, params) {
	var w = jQuery(obj).closest(".window").attr("params", params);
}
//changes the window url and params
function changeMyWindowRefresh(obj, url, params) {
	var w = jQuery(obj).closest(".window").attr("url", url).attr("params", params);
}
//changes the window url and params and refresh the window with the new values
function changeMyWindowWithRefresh(obj, url, params) {
	var w = jQuery(obj).closest(".window").attr("url", url).attr("params", params);
	refreshMyWindow(obj);
}
function resizeWindowContent(winObj) {
	jQuery(winObj).children(".window-content").height(parseInt(jQuery(winObj).height()) - 40);
	resizeAll();
}
function saveWindowSize(winObj) {
	jQuery(winObj).attr({
		lastTop: winObj.css("top"),
		lastLeft: winObj.css("left"),
		lastWidth: winObj.width(),
		lastHeight: winObj.height()
	});
}
function changeMyWindowTitle(obj, newTitle) {
	var w = jQuery(obj).closest(".window");
	w.find(".title").html(newTitle);
}

//this function corrects the windows dimensions with the browser resize
jQuery(window).resize(function () {
	//for all maximized windows
	/*jQuery(".window.maximized").each(function () {
		maximizeWindow(jQuery(this));
	}); 
	//for all windows that are visible and not maximized
	jQuery(".window").not(".maximized").not(".minimized").each(function () {
		restoreWindow(jQuery(this));
	});*/
});

jQuery(document).ready(function($) {
                     // This is to make the form submit in all browsers when the Enter key is pressed
                     jQuery("#header-options").find("#user").keypress(function(event) {
                        if (event.keyCode == '13') document.authentication.submit();
                     });
                     jQuery("#header-options").find("#pass").keypress(function(event) {
                        if (event.keyCode == '13') document.authentication.submit();
                     });

                     jQuery(".header-option").click(
                          function(){
                                if(jQuery(this).hasClass("active"))
                                     jQuery(this).removeClass("active");
                                else{
                                     jQuery(".header-option").filter(".active").removeClass("active");
                                     //sets the same panel width as the parents buttons
                                     //var w = Math.max(jQuery(this).parent().width(),240);
									 var w = Math.max(jQuery(this).parent().width(),jQuery(this).children(".header-option-panel").width());
                                     jQuery(this).children(".header-option-panel").width(w);
                                     //activates the option
                                     jQuery(this).addClass("active");
                                }
                          }
                     );
                     jQuery(document).click(function() { //Click anywhere in the document
                          jQuery(".header-option").filter(".active").removeClass("active");
                     });
                     jQuery(".header-option").click(function(e) {
                          e.stopPropagation(); //Prevents the Panel from closing on click in the document
                     });
                     jQuery(".header-option-panel").click(function(e) {
                          e.stopPropagation(); //Prevents the Panel from closing on click in the document
                     });
                });

function desenha_menu_vertical() {
	jQuery(".menu-vertical-option").hover(
		function () {
			//If has suboptions
			if (jQuery(this).find(".menu-vertical-suboptionsdiv").length) {
				//verifies if the div content options is smaller than the main option button
				if (jQuery(this).find(".menu-vertical-suboptionsdiv").width() < (jQuery(this).width() + 10))
					jQuery(this).find(".menu-vertical-suboptionsdiv").width(jQuery(this).width() + 10);
				jQuery(this).find(".menu-vertical-suboptionsdiv").show();
				jQuery(this).find(".menu-vertical-optiontitle").removeClass("normal").addClass("hover");
			} else if (!jQuery(this).find(".menu-vertical-optiontitle").hasClass("active"))
				jQuery(this).find(".menu-vertical-optiontitle").removeClass("normal").addClass("hoveralone");
		},
		function () {
			//If has suboptions
			if (jQuery(this).find(".menu-vertical-suboptionsdiv").length) {
				jQuery(this).find(".menu-vertical-suboptionsdiv").hide();
				jQuery(this).find(".menu-vertical-optiontitle").addClass("normal").removeClass("hover");
			} else if (!jQuery(this).find(".menu-vertical-optiontitle").hasClass("active"))
				jQuery(this).find(".menu-vertical-optiontitle").addClass("normal").removeClass("hoveralone");
			
		});
}

function gessiMenuShowContent(menuId) {
	pedidoAjax('gessi_menus_main.show_menu_content', 'pv_menu_id=' + menuId, 'conteudo-inner');
}

//This function is used for the Ajax Options
jQuery.fn.extend({
		gessiMenuClickEvent : function () {
			//if there are open and visible windows then tries to close them first
			try {
				eval("closeAllVisibleWindows();");
			} catch (err) {}
			if (jQuery(this).find(".menu-vertical-optiontitle").length) {
				jQuery(".menu-vertical-optiontitle").removeClass("active").removeClass("hover").removeClass("hoveralone");
				jQuery(this).find(".menu-vertical-optiontitle").addClass("active").addClass("normal");
				jQuery(".menu-vertical-suboptionsdiv").hide();
			} else if (jQuery(this).find(".menu-vertical-suboption").length) {
				jQuery(".menu-vertical-optiontitle").removeClass("active").removeClass("hover").removeClass("hoveralone");
				jQuery(this).closest(".menu-vertical-option").find(".menu-vertical-optiontitle").addClass("active");
				jQuery(".menu-vertical-suboptionsdiv").hide();
			}
		},
		gessiMenuLoadContent : function (procedure) {
			jQuery(this).gessiMenuClickEvent();
			pedidoAjax(procedure, '', 'conteudo-inner');
		},
		//this function should be deleted after all menus have been converted to use gessiMenuShowContent
		menuVerticalClick : function (procedure) {
			jQuery(this).gessiMenuClickEvent();
			gessiMenuShowContent(jQuery(this).attr('menuId'));
		},
		gessiMenuShowContent : function () {
			jQuery(this).gessiMenuClickEvent();
			gessiMenuShowContent(jQuery(this).attr('menuId'));
		}
	});
 
//Adjust panel height
jQuery.fn.adjustPanel = function () {
	jQuery(this).find(".finaltoolbar-panel-content").css({
		'height' : 'auto'
	}); //Reset content height
	var windowHeight = jQuery(window).height(); //Get the height of the browser viewport
	var panelsub = jQuery(this).find(".finaltoolbar-panel-content").height(); //Get the height of subpanel
	var panelAdjust = windowHeight - 200; //Viewport height - 200px (Sets max height of subpanel)
	if (panelsub >= panelAdjust) { //If subpanel is taller than max height...
		jQuery(this).find(".finaltoolbar-panel-content").css({
			'height' : panelAdjust
		}); //Adjust subpanel to max height
	}
};
jQuery.fn.finaltoolbarmenuclick = function () {
	//only shows class active if the menu has a panel
	if (jQuery(this).find(".finaltoolbar-panel").length > 0) {
		if (jQuery(this).hasClass("active"))
			jQuery(this).removeClass("active");
		else {
			closeAllFinalToolBarMenu();
			jQuery(this).addClass("active");
			jQuery(this).find(".finaltoolbar-tooltip-text-count").hide();
		}
	}
};

jQuery(document).ready(function ($) {
	jQuery(".finaltoolbar-menu").click(
		function () {
		jQuery(this).finaltoolbarmenuclick();
	});
	jQuery(".finaltoolbar-menu").hover(
		function () {
		jQuery(this).children(".finaltoolbar-tooltip").show();
	},
		function () {
		jQuery(this).children(".finaltoolbar-tooltip").hide();
	});
	/*jQuery(window).resize(function () {
	jQuery(".finaltoolbar-panel").adjustPanel();
	});*/

	jQuery(document).click(function () { //Click anywhere in the document
		closeAllFinalToolBarMenu();
	});
	jQuery(".finaltoolbar-menu").click(function (e) {
		e.stopPropagation(); //Prevents the Panel from closing on click in the document
	});
	jQuery(".finaltoolbar-panel").click(function (e) {
		e.stopPropagation(); //Prevents the Panel from closing on click in the document
	});

});
function closeAllFinalToolBarMenu() {
	jQuery(".finaltoolbar-menu").filter(".active").removeClass("active");
}

var notiftimeout = null;
jQuery("#notification-panel").click(function () {
	clearTimeout(notiftimeout);
});
myNotifShowFun = "jQuery('#finaltoolbar-notif').finaltoolbarmenuclick();notiftimeout = setTimeout('closeAllFinalToolBarMenu()',3000);";

jQuery(window).resize(function () {
	if(jQuery("#finaltoolbar")){
		var h = jQuery("#cabecalho").height() + jQuery("#menu-vertical").height() + 65;
		try {
			if (jQuery("#barralocalizacao") && jQuery("#barralocalizacao").is(":visible"))
				h += jQuery("#barralocalizacao").height() + 5;
		} catch (err) {}
		jQuery("#conteudo-inner").height(parseInt(jQuery(window).height()) - h);
		resizeAll();
	}
});


var myNextRefreshBlockFun = "";
var intNextRefreshBlockFun = "";

function evalmyNextRefreshBlockFun() {
	if (myNextRefreshBlockFun != "") {
		try {
			eval(myNextRefreshBlockFun);
		} catch (err) {
			alert('Erro no eval');
		}
	}
	myNextRefreshBlockFun = "";
	jQuery().ready(function () {
		jQuery('.all_hide').hide();
		jQuery('.all_show').show();
		jQuery('.options_admin').hide();
		jQuery('.title_admin').hide();
	});

}

function loadMyNextRefreshAjaxFun(var_eval) {
	myNextRefreshAjaxFun = myNextRefreshAjaxFun + " " + var_eval + " " + intNextRefreshBlockFun;
}
function loadComponentOnDiv(optionsLoad) {
	// every component is draw inside a div,
	// we need to replace the div but the event happens on the component
	var l_selector = "";
	var parametros = "";
	var showloading = true;
	if (optionsLoad.name) {
		parametros += "pv_comp_name=" + optionsLoad.name;
	}
	if (optionsLoad.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
	}
	if (optionsLoad.selector) {
		if (!optionsLoad.selector.jquery) {
			l_selector = optionsLoad.selector.toLowerCase();
			l_selector = jQuery('#' + l_selector);
		} else {
			l_selector = optionsLoad.selector;
		}
	}
	if (optionsLoad.parr_name) {
		for (i = 0; i < optionsLoad.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsLoad.parr_name[i];
			parametros += "&parr_value=" + optionsLoad.parr_value[i];
		}
	}
	parametros += "&pv_first_multi=N";
	if (optionsLoad.show_title) {
		parametros += '&pv_show_title=' + optionsLoad.show_title;
	}
	if (optionsLoad.pv_param_id) {
		parametros += '&pv_param_id=' + optionsLoad.pv_param_id;
	} else {
		parametros += '&pv_param_id=' + getClosestParam(l_selector);
	}

	if (optionsLoad.showloading == false) {
		showloading = false;
	}
	intNextRefreshBlockFun = 'evalmyNextRefreshBlockFun();';
	myNextRefreshAjaxFun = 'evalmyNextRefreshBlockFun();';
	ajaxRequest({
		procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_component',
		parametros: parametros,
		div_actualizacao: l_selector,
		showLoading: showloading,
		replace: true
	});

	/*jQuery.ajax({
	type: "POST",
	url: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_component',
	data: parametros,
	cache: false,
	success: function(resp){
	if(!validadeResponseText(resp)) return;
	jQuery(l_selector).replaceWith(resp);
	evalmyNextRefreshBlockFun();
	actAll();
	}
	});*/

}

function refreshComponent(optionsRefresh) {
	// every component is draw inside a div,
	// we need to replace the div but the even happens on the component
	var showloading = true;
	var l_selector = "";
	var parametros = "";
	var replace = true;
	if (optionsRefresh.name) {
		l_selector = optionsRefresh.name.toLowerCase();
		parametros += "pv_comp_name=" + optionsRefresh.name;
	}
	if (optionsRefresh.div_name) {
		l_selector = jQuery('#' + optionsRefresh.div_name + ' #' + optionsRefresh.name.toLowerCase());
		//alert(l_selector);
	}

	if (optionsRefresh.async == undefined) {
		optionsRefresh.async = true;
	}
	if (optionsRefresh.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsRefresh.mode;
	}
	if (optionsRefresh.selector) {
		l_selector = optionsRefresh.selector;
	}
	if (optionsRefresh.replace != undefined) {
		replace = optionsRefresh.replace;
	}
	if (optionsRefresh.showloading == false) {
		showloading = false;
	}

	if (optionsRefresh.parr_name) {
		for (i = 0; i < optionsRefresh.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsRefresh.parr_name[i];
			parametros += "&parr_value=" + optionsRefresh.parr_value[i];
		}
	}
	if (optionsRefresh.show_title) {
		parametros += '&pv_show_title=' + optionsRefresh.show_title;
	}
	//if (!optionsRefresh.mode || optionsRefresh.mode!='I'){
	parametros += '&pv_param_id=' + getClosestParam(jQuery('#' + optionsRefresh.name.toLowerCase()));
	//}
	intNextRefreshBlockFun = 'evalmyNextRefreshBlockFun();';
	myNextRefreshAjaxFun = 'evalmyNextRefreshBlockFun();';
	if(!$('#'+l_selector).find('input').val()) {
		if (optionsRefresh.eventtrigger) {
			jQuery(optionsRefresh.eventtrigger).ajaxRequestOnMyWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_component', parametros, l_selector, optionsRefresh.async);
		} else {
			ajaxRequest({
				procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_component',
				parametros: parametros,
				div_actualizacao: l_selector,
				replace: replace,
				showLoading: showloading,
				async: optionsRefresh.async
			});
		}
	}

	/*jQuery.ajax({
	type: "POST",
	url: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_component',
	data: parametros,
	cache: false,
	success: function(resp){
	if(!validadeResponseText(resp)) return;
	jQuery(l_selector).replaceWith(resp);
	evalmyNextRefreshBlockFun();
	actAll();
	}
	});*/

}

jQuery.fn.extend({
	loadBlockOnDiv: function (optionsLoad) {
		var parametros = "";
		var title = "";
		var showloading = true;

		if (optionsLoad.title) {
			title = optionsLoad.title;
		}
		/*if (optionsLoad.selector) {
		l_selector = optionsLoad.selector.toLowerCase();
		}*/
		if (optionsLoad.name) {
			parametros += "pv_block_name=" + optionsLoad.name;
		}
		if (optionsLoad.name) {
			parametros += "&pv_show_all=S";
		}
		if (optionsLoad.mode) {
			parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
		}
		//alteracao 2014.07.07 12:16 daniel.silva
		if (optionsLoad.duplicate) {
			parametros += "&pv_draw_values_pk=N";
		}

		//if (!optionsLoad.mode || optionsLoad.mode!='I'){
		parametros += '&pv_param_id=' + getClosestParam(jQuery(this));
		//}
		if (optionsLoad.parr_name) {
			for (i = 0; i < optionsLoad.parr_name.length; i++) {
				parametros += "&parr_name=" + optionsLoad.parr_name[i];
				parametros += "&parr_value=" + optionsLoad.parr_value[i];
			}
		}
		if (optionsLoad.first_multi) {
			parametros += "&pv_first_multi=" + optionsLoad.first_multi;
		}
		if (optionsLoad.showloading == false) {
			showloading = false;
		}
		intNextRefreshBlockFun = 'evalmyNextRefreshBlockFun();';
		myNextRefreshAjaxFun = 'evalmyNextRefreshBlockFun();';
		ajaxRequest({
			procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block',
			parametros: parametros,
			div_actualizacao: optionsLoad.selector,
			showLoading: showloading,
			replace: true
		});

	}
});

function loadBlockOnDiv(optionsLoad) {
	//{block_name:valor,title,parr_name:valor,parr_value:valor,block_mode:valor}
	var parametros = "";
	var title = "";
	var l_selector = "";
	var replace = true;
	var showloading = true;

	if (optionsLoad.title) {
		title = optionsLoad.title;
	}
	if (optionsLoad.selector) {

		if (typeof (optionsLoad.selector) != 'object') {
			l_selector = jQuery('#' + optionsLoad.selector);
		} else {
			l_selector = optionsLoad.selector;
		}
	}
	if (optionsLoad.name) {
		parametros += "pv_block_name=" + optionsLoad.name;
	}
	if (optionsLoad.name) {
		parametros += "&pv_show_all=S";
	}
	if (optionsLoad.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
	}
	//alteracao 2014.07.07 12:16 daniel.silva
	if (optionsLoad.duplicate) {
		parametros += "&pv_draw_values_pk=N";
	}

	if (optionsLoad.tabs) {
		parametros += "&pv_tabx=" + optionsLoad.tabs;
	}

	if (optionsLoad.replace != undefined) {
		replace = optionsLoad.replace;
	}
	if (optionsLoad.pv_param_id) {
		parametros += '&pv_param_id=' + optionsLoad.pv_param_id;
	} else {
		parametros += '&pv_param_id=' + getClosestParam(l_selector);
	}

	if (optionsLoad.parr_name) {
		for (i = 0; i < optionsLoad.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsLoad.parr_name[i];
			parametros += "&parr_value=" + optionsLoad.parr_value[i];
		}
	}
	if (optionsLoad.first_multi) {
		parametros += "&pv_first_multi=" + optionsLoad.first_multi;
	}

	if (optionsLoad.showloading == false) {
		showloading = false;
	}

	ajaxRequest({
		procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block',
		parametros: parametros,
		div_actualizacao: l_selector,
		title: title,
		showLoading: showloading,
		replace: replace
	});

}
function loadBlockOnFaceBox(optionsLoad) {
	//{name:valor,title:valor,parr_name:valor,parr_value:valor,mode:valor}
	var parametros = "";
	var title = "";
	if (optionsLoad.title) {
		title = optionsLoad.title;
	}
	if (optionsLoad.name) {
		parametros += "pv_block_name=" + optionsLoad.name;
	}
	if (optionsLoad.name) {
		parametros += "&pv_show_all=S";
	}
	if (optionsLoad.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
	}
	//alteracao 2014.07.07 12:16 daniel.silva
	if (optionsLoad.duplicate) {
		parametros += "&pv_draw_values_pk=N";
	}

	if (optionsLoad.pv_param_id) {
		parametros += '&pv_param_id=' + optionsLoad.pv_param_id;
	}
	if (optionsLoad.parr_name) {
		for (i = 0; i < optionsLoad.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsLoad.parr_name[i];
			parametros += "&parr_value=" + optionsLoad.parr_value[i];
		}
	}
	ajaxRequest({
		procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block',
		parametros: parametros,
		title: title,
		showInBox: true
	});
	//pedidoAjax('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block',parametros,null,true);

}

