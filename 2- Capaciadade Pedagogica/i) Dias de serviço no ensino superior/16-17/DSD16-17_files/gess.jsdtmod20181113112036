/*
 *@criacao 2015.01.08 16:21 daniel.silva
 *@alteracao 2015.02.03 16:55 daniel.silva
 * force focus to autocomplete
 */
function focusAutocomplete() {

	jQuery('form').not('[relBind=1]').each(function () {
		jQuery(this).find('input:visible').filter(function () {
			return jQuery(this).val() == '';
		}).first().not('[readonly=readonly]').not('.hasDatepicker').focus();
		jQuery(this).attr('relBind', '1');
	});
}

