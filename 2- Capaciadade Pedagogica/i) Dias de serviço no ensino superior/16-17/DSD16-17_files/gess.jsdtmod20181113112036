function OnChangeAplBlockCmbx() {
	if (myLastObj.val()) {
		refreshComponent({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_modulo',
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()]
		});
		refreshComponent({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_pagina',
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()]
		});
		refreshComponent({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_bloco_pai',
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()]
		});
	}
}
function OnChangeModBlockCmbx() {
	if (myLastObj.val()) {
		var sel_apl_id = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_aplicacao').getValue();
		refreshComponent({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_pagina',
			parr_name: ['pv_apl_id', 'pv_mod_id'],
			parr_value: [sel_apl_id, myLastObj.val()]
		});
		refreshComponent({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_bloco_pai',
			parr_name: ['pv_apl_id', 'pv_mod_id'],
			parr_value: [sel_apl_id, myLastObj.val()]
		});
	}
}
function OnChangePagBlockCmbx() {
	if (myLastObj.val()) {
		var sel_apl_id = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_aplicacao').getValue();
		var sel_mod_id = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_modulo').getValue();
		refreshComponent({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_bloco_pai',
			parr_name: ['pv_apl_id', 'pv_mod_id', 'pv_pag_id'],
			parr_value: [sel_apl_id, sel_mod_id, myLastObj.val()]
		});
	}
}
function OnChangeAplCmbxRefresh(pv_element) {
	var combo_page;
	var combo_mod;
	var combo_block;
	if (pv_element == 'BLOCK') {
		combo_page = 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_pagina';
		combo_mod = 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_modulo';
		combo_block = 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_bloco_pai';
	} else if (pv_element == 'COMP') {
		combo_page = 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_pagina';
		combo_mod = 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_modulo';
		combo_block = 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_bloco';
	}
	if (myLastObj.val()) {
		refreshComponent({
			name: combo_mod,
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()],
			mode: 'E'
		});
		refreshComponent({
			name: combo_page,
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()],
			mode: 'E'
		});
		refreshComponent({
			name: combo_block,
			parr_name: ['pv_apl_id'],
			parr_value: [myLastObj.val()],
			mode: 'E'
		});
	}
}
function OnChangeModCmbxRefresh(pv_element) {
	var combo_page;
	var combo_block;
	var sel_apl_id;

	if (pv_element == 'BLOCK') {
		sel_apl_id = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_aplicacao').getValue();
		combo_page = 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_pagina';
		combo_block = 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_bloco_pai';
	} else if (pv_element == 'COMP') {
		sel_apl_id = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_apl').getValue();
		combo_page = 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_pagina';
		combo_block = 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_bloco';
	}

	if (myLastObj.val()) {
		refreshComponent({
			name: combo_page,
			parr_name: ['pv_apl_id', 'pv_mod_id'],
			parr_value: [sel_apl_id, myLastObj.val()],
			mode: 'E'
		});
		refreshComponent({
			name: combo_block,
			parr_name: ['pv_apl_id', 'pv_mod_id'],
			parr_value: [sel_apl_id, myLastObj.val()],
			mode: 'E'
		});
	}
}
function OnChangePagCmbxRefresh(pv_element) {
	var combo_block;
	var sel_apl_id;
	var sel_mod_id;
	if (pv_element == 'BLOCK') {
		sel_apl_id = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_aplicacao').getValue();
		sel_mod_id = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_modulo').getValue();
		combo_block = 'fw_interface_builder_geral_show_block_ins_tabs_apl_gessi_bloco_pai';
	} else if (pv_element == 'COMP') {
		sel_apl_id = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_apl').getValue();
		sel_mod_id = jQuery('#fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_modulo').getValue();
		combo_block = 'fw_interface_builder_geral_show_component_ins_tabs_apl_gessi_bloco';
	}
	if (myLastObj.val()) {
		refreshComponent({
			name: combo_block,
			parr_name: ['pv_apl_id', 'pv_mod_id', 'pv_pag_id'],
			parr_value: [sel_apl_id, sel_mod_id, myLastObj.val()],
			mode: 'E'
		});
	}
}
function OnChangeTipoBlocoCmbx() {
	//cristiano.martins 2011-10-31 12:00

	if (myLastObj.val()) {
		var tipo = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_geral_tipo').getValue();
		var TabNome = jQuery('#fw_interface_builder_geral_show_block_ins_tabs_geral_tipo').find('select').find('option').filter(':selected').html();
		refreshBlock({
			name: 'fw_interface_builder_geral_show_block_ins_tabs_tipo_blocos',
			parr_name: new Array('pv_tipo', 'pv_draw_mode'),
			parr_value: new Array(tipo, 'S'),
			tabs: 'S',
			mode: 'E'
		});
		jQuery('#fw_interface_builder_geral_show_block_ins_tabs').find('li').last().find('a').find('span').html(TabNome);
	}
}
function loadNodeConfig(event, pv_id, pv_type, pv_father_type, pv_page_id) {
	// alert(pv_id+" - "+pv_type+" "+pv_father_type + " aki"+pv_page_id);
	switch (pv_type) {
		case "blocks":
			if (pv_father_type == "PAG")
				loadBlockOnDiv({
					name: "fw_interface_param_geral_show_pag_config_edit_blocks",
					parr_name: ["pv_pag_id", "pv_bl_id"],
					parr_value: [pv_page_id, ""],
					mode: "E",
					selector: "fw_interface_param_geral_show_pag_config_edit",
					replace: false
				});
			else
				loadBlockOnDiv({
					name: "fw_interface_param_geral_show_pag_config_edit_blocks",
					parr_name: ["pv_bl_id"],
					parr_value: [pv_id],
					mode: "E",
					selector: "fw_interface_param_geral_show_pag_config_edit",
					replace: false
				});
			break;
		case "bl":
			//alteracao daniel.silva 04.04.2012
			loadBlockOnDiv({
				name: "fw_interface_param_geral_show_pag_config_edit_block",
				parr_name: ["pv_id", "pv_bl_id"],
				parr_value: [pv_id, ""],
				mode: "E",
				selector: "fw_interface_param_geral_show_pag_config_edit",
				replace: false
			});
			break;
		case "components":
			loadBlockOnDiv({
				name: "fw_interface_param_geral_show_pag_config_edit_components",
				parr_name: ["pv_bl_id"],
				parr_value: [pv_id],
				mode: "E",
				selector: "fw_interface_param_geral_show_pag_config_edit",
				replace: false
			});
			break;
		case "comp":
			//alteracao cristiano.martins 05.04.2012
			loadBlockOnDiv({
				name: "fw_interface_param_geral_show_pag_config_edit_comp",
				parr_name: ["pv_id", "pv_bl_id"],
				parr_value: [pv_id, ""],
				mode: "E",
				selector: "fw_interface_param_geral_show_pag_config_edit",
				replace: false
			});
			break;
		case "opt":
			loadBlockOnDiv({
				name: "fw_interface_param_geral_show_pag_config_edit_opc",
				parr_name: ["pv_opt_id", "pv_bl_id", "pv_pag_id", "pv_id"],
				parr_value: [pv_id, "", "", ""],
				mode: "E",
				selector: "fw_interface_param_geral_show_pag_config_edit",
				replace: false
			});
			break;
		case "docs":
			//alteracao cristiano.martins 2013.10.08
			loadBlockOnDiv({
				name: "fw_interface_param_geral_show_pag_config_docs",
				parr_name: ["pv_pag_id"],
				parr_value: [pv_page_id],
				mode: "E",
				selector: "fw_interface_param_geral_show_pag_config_edit",
				replace: false
			});
			break;
		//alteracao cristiano.martins 2013.10.04
		case "atm":
			loadBlockOnDiv({
				name: "fw_interface_param_geral_show_pag_config_edit_atom",
				parr_name: ["pv_id"],
				parr_value: [pv_id],
				mode: "E",
				selector: "fw_interface_param_geral_show_pag_config_edit",
				replace: false
			});
		default:
			break;
	}

}

function OnChangeDeprecatedCmbx(obj) {
	//cristiano.martins 2013-09-25
	var resp = "";
	var button_resp = "";
	if (obj.val() == "S") {
		resp = confirm("Tem a certeza que pretende colocar este objecto Deprecated?");
		if (resp != true) {
			//alert("Cancel");
			obj.val("N");
		}
	}
}

function onSelectTable(obj, val) {
	refreshBlock({
		name: "gessi_interface_main_main_create_form_columns",
		parr_name: new Array("pv_table_name"),
		parr_value: new Array(val)
	});
}

function fwSetStruct(pv_struct_id, pv_comp_id) {
	//alteracao cristiano.martins 2014.06.05 GP102342
	/*console.log(myLastObj);
	console.log(myLastObj.closest("div"));*/
	myLastObj.closest("div").find("[rel=structval]").val(pv_struct_id);
	myLastObj.closest("div").find("[rel=structval]").trigger("change");
	jQuery("div.qtip:visible").qtip("hide");
	myLastObj.closest("div").find("[rel=structspan]").html(ajaxReturn("struct_main.struct_assignment_meaning", "pv_struct_assigment=" + pv_struct_id));

	/*jQuery("#"+pv_comp_id).find("[rel=structval]").val(pv_struct_id);
	jQuery("#"+pv_comp_id).find("[rel=structval]").trigger("change");
	jQuery("div.qtip:visible").qtip("hide");
	jQuery("#"+pv_comp_id).find("[rel=structspan]").html(ajaxReturn("struct_main.struct_assignment_meaning","pv_struct_assigment="+pv_struct_id));*/
}

//cristiano.martins 2013.10.11
function refreshTabBlockType(p_type, p_view_mode) {
	var TabNome = "";

	switch (p_type) {
		case "DIV":
			TabNome = "Div";
			break;
		case "AGP":
			TabNome = "Agrupador";
			break;
		case "CALDIV":
			TabNome = "Eventos de Calendário";
			break;
		case "GRD":
			TabNome = "Flexigrid";
			break;
		case "GRDD":
			TabNome = "Flexigrid Dinámica";
			break;
		case "FRM":
			TabNome = "Form";
			break;
		case "MAT":
			TabNome = "Matrix";
			break;
		case "FILTERS":
			TabNome = "Pesquisa";
			break;
		case "TBL":
			TabNome = "Tabela";
			break;
		case "TBS":
			TabNome = "Tabs";
			break;
		case "TBX":
			TabNome = "Tabs por Ajax";
			break;
		default:
			TabNome = " ";
	}

	refreshBlock({
		name: "fw_interface_builder_geral_show_block_ins_tabs_tipo_blocos",
		parr_name: new Array("pv_tipo", "pv_draw_mode"),
		parr_value: new Array(p_type, "S"),
		tabs: "S",
		mode: p_view_mode
	});
	jQuery("#fw_interface_builder_geral_show_block_ins_tabs").find("li").last().find("a").find("span").html(TabNome);
}

