function loadBlockOnNewWindow(optionsLoad) {
	//{block_name:valor,title,parr_name:valor,parr_value:valor,block_mode:valor}
	var parametros = "";
	var title = "";
	if (optionsLoad.title) {
		title = optionsLoad.title;
	}
	if (optionsLoad.name) {
		parametros += "pv_block_name=" + optionsLoad.name;
	}
	if (optionsLoad.name) {
		parametros += "&pv_show_all=S";
	}
	if (optionsLoad.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
	}
	//alteracao 2014.07.07 12:16 daniel.silva
	if (optionsLoad.duplicate) {
		parametros += "&pv_draw_values_pk=N";
	}

	if (optionsLoad.pv_param_id) {
		parametros += '&pv_param_id=' + optionsLoad.pv_param_id;
	}
	if (optionsLoad.parr_name) {
		for (i = 0; i < optionsLoad.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsLoad.parr_name[i];
			parametros += "&parr_value=" + optionsLoad.parr_value[i];
		}
	}
	if (optionsLoad.windowid) {
		closeMyWindow(jQuery('#' + optionsLoad.windowid));
	}
	openWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block', parametros, title);

}

jQuery.fn.extend({
	loadBlockOnNewWindow: function (optionsLoad) {
		//{block_name:valor,title,parr_name:valor,parr_value:valor,block_mode:valor}
		var parametros = "";
		var title = "";
		if (optionsLoad.title) {
			title = optionsLoad.title;
		}

		if (optionsLoad.name) {
			parametros += "pv_block_name=" + optionsLoad.name;
		}
		if (optionsLoad.name) {
			parametros += "&pv_show_all=S";
		}
		if (optionsLoad.mode) {
			parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
		}
		//alteracao 2014.07.07 12:16 daniel.silva
		if (optionsLoad.duplicate) {
			parametros += "&pv_draw_values_pk=N";
		}

		//if (!optionsLoad.mode || optionsLoad.mode!='I'){
		parametros += '&pv_param_id=' + getClosestParam(this);
		//}
		if (optionsLoad.parr_name) {
			for (i = 0; i < optionsLoad.parr_name.length; i++) {
				parametros += "&parr_name=" + optionsLoad.parr_name[i];
				parametros += "&parr_value=" + optionsLoad.parr_value[i];
			}
		}
		if (optionsLoad.windowid) {
			closeMyWindow(jQuery('#' + optionsLoad.windowid));
		}
		openWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block', parametros, title);

	}
});

function refreshBlock(optionsRefresh) {
	var l_selector = "";
	var parametros = "";
	var showloading = true;
	//alert(optionsRefresh.name);
	var replace = true;
	if (optionsRefresh.name) {
		l_selector = optionsRefresh.name.toLowerCase().replace(".", "\\.");
		parametros += "pv_block_name=" + optionsRefresh.name;
		parametros += "&pv_show_all=S";
	}
	if (optionsRefresh.tabs) {
		parametros += "&pv_tabx=" + optionsRefresh.tabs;
	}
	if (optionsRefresh.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsRefresh.mode;
	}
	//alteracao 2014.07.07 12:16 daniel.silva
	if (optionsRefresh.duplicate) {
		parametros += "&pv_draw_values_pk=N";
	}

	if (optionsRefresh.page_number) {
		parametros += "&page_number=" + optionsRefresh.page_number;
	}
	if (optionsRefresh.selector) {
		l_selector = optionsRefresh.selector;
	}
	if (optionsRefresh.showloading == false) {
		showloading = false;
	}
	if (optionsRefresh.async == undefined) {
		optionsRefresh.async = true;
	}
	if (optionsRefresh.parr_name) {
		for (i = 0; i < optionsRefresh.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsRefresh.parr_name[i];
			parametros += "&parr_value=" + optionsRefresh.parr_value[i];
		}
	}
	//if (!optionsRefresh.mode || optionsRefresh.mode!='I'){
	//parametros += '&pv_param_id=' + getClosestParam(jQuery('#' + optionsRefresh.name.toLowerCase()));
	//}
	if (!optionsRefresh.cleanParam || optionsRefresh.cleanParam != 'Y') {
		parametros += '&pv_param_id=' + getClosestParam(jQuery('#' + optionsRefresh.name.toLowerCase()));
	}

	if (optionsRefresh.replace != undefined) {
		replace = optionsRefresh.replace;
	}

	intNextRefreshBlockFun = 'evalmyNextRefreshBlockFun();';
	myNextRefreshAjaxFun = 'evalmyNextRefreshBlockFun();';
	if (optionsRefresh.eventtrigger) {
		jQuery(optionsRefresh.eventtrigger).ajaxRequestOnMyWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block', parametros, l_selector, optionsRefresh.async);
	} else { //
		ajaxRequest({
			procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_block',
			parametros: parametros,
			div_actualizacao: l_selector,
			replace: replace,
			showLoading: showloading,
			async: optionsRefresh.async
		});
	}

}

function loadPageOnFaceBox(optionsLoad) {
	var parametros = "";
	var title = "";
	if (optionsLoad.title) {
		title = optionsLoad.title;
	}
	if (optionsLoad.name) {
		parametros += "pv_page_name=" + optionsLoad.name;
	}
	//parametros+="&pv_show_all=S";
	if (optionsLoad.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
	}

	//if (!optionsLoad.mode || optionsLoad.mode!='I'){
	//  parametros+='&pv_param_id='+getClosestParam(l_selector);
	//}
	if (optionsLoad.parr_name) {
		for (i = 0; i < optionsLoad.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsLoad.parr_name[i];
			parametros += "&parr_value=" + optionsLoad.parr_value[i];
		}
	}
	ajaxRequest({
		procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page',
		parametros: parametros,
		showInBox: true
	});
	//pedidoAjax('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page',parametros,null,true);

}

jQuery.fn.extend({
	loadPageOnFaceBox: function (optionsLoad) {
		var parametros = "";
		var title = "";
		if (optionsLoad.title) {
			title = optionsLoad.title;
		}
		if (optionsLoad.name) {
			parametros += "pv_page_name=" + optionsLoad.name;
		}
		//parametros+="&pv_show_all=S";
		if (optionsLoad.mode) {
			parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
		}
		//if (!optionsLoad.mode || optionsLoad.mode!='I'){
		parametros += '&pv_param_id=' + getClosestParam(this);
		//}
		if (optionsLoad.parr_name) {
			for (i = 0; i < optionsLoad.parr_name.length; i++) {
				parametros += "&parr_name=" + optionsLoad.parr_name[i];
				parametros += "&parr_value=" + optionsLoad.parr_value[i];
			}
		}
		ajaxRequest({
			procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page',
			parametros: parametros,
			showInBox: true
		});
		//pedidoAjax('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page',parametros,null,true);

	}
});

function loadPageOnNewWindow(optionsLoad) {
	var parametros = "";
	var title = "";
	if (optionsLoad.title) {
		title = optionsLoad.title;
	}
	if (optionsLoad.name) {
		parametros += "pv_page_name=" + optionsLoad.name;
	}
	//parametros+="&pv_show_all=S";
	if (optionsLoad.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
	}
	//if (!optionsLoad.mode || optionsLoad.mode!='I'){
	//parametros+='&pv_param_id='+getClosestParam(l_selector);
	//}
	if (optionsLoad.parr_name) {
		for (i = 0; i < optionsLoad.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsLoad.parr_name[i];
			parametros += "&parr_value=" + optionsLoad.parr_value[i];
		}
	}
	if (optionsLoad.windowid) {
		closeMyWindow(jQuery('#' + optionsLoad.windowid));
	}
	//openWindow
	openWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page', parametros, title);
}

jQuery.fn.extend({
	loadPageOnNewWindow: function (optionsLoad) {
		var parametros = "";
		var title = "";
		if (optionsLoad.title) {
			title = optionsLoad.title;
		}
		if (optionsLoad.name) {
			parametros += "pv_page_name=" + optionsLoad.name;
		}
		//parametros+="&pv_show_all=S";
		if (optionsLoad.mode) {
			parametros += "&pv_tipo_apresentacao=" + optionsLoad.mode;
		}
		//if (!optionsLoad.mode || optionsLoad.mode!='I'){
		parametros += '&pv_param_id=' + getClosestParam(this);
		//}
		if (optionsLoad.parr_name) {
			for (i = 0; i < optionsLoad.parr_name.length; i++) {
				parametros += "&parr_name=" + optionsLoad.parr_name[i];
				parametros += "&parr_value=" + optionsLoad.parr_value[i];
			}
		}
		if (optionsLoad.windowid) {
			closeMyWindow(jQuery('#' + optionsLoad.windowid));
		}
		openWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page', parametros, title);
	}
});

function refreshPage(optionsRefresh) {
	//selector,parr_name,parr_value,pag_mode
	var l_selector = "";
	var parametros = "";
	if (optionsRefresh.name) {
		l_selector = optionsRefresh.name.toLowerCase().replace(".", "\\.");
		parametros += "pv_page_name=" + optionsRefresh.name;
	}
	if (optionsRefresh.mode) {
		parametros += "&pv_tipo_apresentacao=" + optionsRefresh.mode;
	}

	if (optionsRefresh.parr_name) {
		for (i = 0; i < optionsRefresh.parr_name.length; i++) {
			parametros += "&parr_name=" + optionsRefresh.parr_name[i];
			parametros += "&parr_value=" + optionsRefresh.parr_value[i];
		}
	}
	//if (!optionsRefresh.mode || optionsRefresh.mode!='I'){
	parametros += '&pv_param_id=' + getClosestParam(jQuery('#' + l_selector));
	//}
	intNextRefreshBlockFun = 'evalmyNextRefreshBlockFun();';
	myNextRefreshAjaxFun = 'evalmyNextRefreshBlockFun();';
	if (optionsRefresh.eventtrigger) {
		jQuery(optionsRefresh.eventtrigger).ajaxRequestOnMyWindow('FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page', parametros, l_selector);
	} else {
		ajaxRequest({
			procedimento: 'FW_INTERFACE_BUILDER_GERAL.fw_int_draw_page',
			parametros: parametros,
			div_actualizacao: l_selector,
			replace: true
		});
	}

}

function getClosestParam(selector) {
	if (selector.attr('param'))
		return selector.attr('param');
	else
		if (selector.closest('[param]').attr('param'))
			return selector.closest('[param]').attr('param');
		else
			return "";
}

jQuery.fn.extend({
	reloadGrid: function (pv_id) {
		this.getObj('#' + pv_id).find('.ui-jqgrid-btable').trigger("reloadGrid");
	}
});

function reloadGrid(pv_id) {
    //Check if there is a window loaded on page

    if (typeof gvMyLastWindow === "undefined") {
        //No window selected so working on main page
        jQuery('#' + pv_id).find('.ui-jqgrid-btable').trigger("reloadGrid");
    } else {
        //There is a window open so lets look for a grid in that window
        gvMyLastWindow.find('#' + pv_id).find('.ui-jqgrid-btable').trigger("reloadGrid");
    }

}

jQuery.fn.extend({
	getSelectedId: function (pv_id) {
		var arr = this.getObj("#" + pv_id).find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
		var arrfinal = new Array();
		for (i = 0; i < arr.length; i++) {
			if (arr[i] && arr[i] != "") {
				arrfinal[arrfinal.length] = arr[i];
			}
		}
		return arrfinal;
	}
});

// returns the selected ID from the flexigrid as an array
function getSelectedId(pv_id) {
	var arr = jQuery().getObj("#" + pv_id).find(".ui-jqgrid-btable").jqGrid('getGridParam', 'selarrrow');
	var arrfinal = new Array();
	for (i = 0; i < arr.length; i++) {
		if (arr[i] && arr[i] != "") {
			arrfinal[arrfinal.length] = arr[i];
		}
	}
	return arrfinal;
}

function setSubmitVar(elem, selector) {

	//execShowNotification=false;
	//closeFaceboxOnNotification=false;
	myNextPostAjaxFun = "jQuery('#" + selector + "_title').children().append('<img src=\'imagens/OkForm\'>');jQuery('#" + selector + "').find(':input').not(':button').each(function(index) { if(jQuery(this).is(':hidden')){jQuery(this).remove();}else{jQuery(this).parent().html(jQuery(this).val());}});";
	myNextCloseFaceboxFun = 'refreshPage({name:"pay_tg_abs_geral.main"})';

}

function setRejectVar(elem, selector) {
	//execShowNotification=false;
	//closeFaceboxOnNotification=false;
	jQuery('#' + selector + '_title').children().append('<img src=\'imagens/Apagar\'>');
	jQuery('#' + selector).find(':input').not(':button').each(function (index) {
		if (jQuery(this).is(':hidden')) {
			jQuery(this).remove();
		} else {
			jQuery(this).parent().html(jQuery(this).val());
		}
	});
	elem.remove();
}
jQuery.fn.extend({
	removeMultiDivRow: function (pv_id) {
		jQuery(this).closest('form').find('#form_submit_' + pv_id).html('');
		jQuery(this).closest('.ui-jqgrid-btable').jqGrid('delRowData', pv_id);
	}
});
// Function that returns the value of a FW component based on the fw_type attr

jQuery.fn.extend({
	setValue: function (pv_value) {
		var elem = jQuery(this);
		var result = '';
		switch (elem.attr('fw_type').toUpperCase()) {
			case 'AC':
				result = elem.find('input:hidden').val(pv_value);
				break;
			case 'BUTTON':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'CHECKBOX':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'DATA':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'DOMD':
				result = elem.find('select').val(pv_value);
				break;
			case 'DOMG':
				result = elem.find('select').val(pv_value);
				break;
			case 'ERRO':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'FILE':
				result = elem.find('input').val(pv_value);
				break;
			case 'VGO':
				result = elem.find('select').val(pv_value);
				break;
			case 'HIDDEN':
				result = elem.val(pv_value);
				break;
			case 'HORA':
				result = elem.find('[rel=timepicker]').val(pv_value);
				break;
			case 'IMG':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'INFO':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'INPUT':
				result = elem.find('input').val(pv_value);
				break;
			case 'LABEL':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'LINK':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'OPTION':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'PASSWORD':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'PROC':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'SELECT':
				result = elem.find('select').val(pv_value);
				break;
			case 'TEXTAREA':
				result = elem.find('textarea').val(pv_value);
				break;
			default:
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
		};
		return elem;
	}
});

jQuery.fn.extend({
	getValue: function (pv_id) {
		var elem = jQuery(this).getObj('#' + pv_id);
		var result = '';
		switch (elem.attr('fw_type').toUpperCase()) {
			case 'AC':
				result = elem.find('input:hidden').val();
				break;
			case 'BUTTON':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'CHECKBOX':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'DATA':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'DOMD':
				result = elem.find('select').val();
				break;
			case 'DOMG':
				result = elem.find('select').val();
				break;
			case 'ERRO':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'FILE':
				result = elem.find('input').val();
				break;
			case 'VGO':
				result = elem.find('select').val();
				break;
			case 'HIDDEN':
				result = elem.val();
				break;
			case 'HORA':
				result = elem.find('[rel=timepicker]').val();
				break;
			case 'IMG':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'INFO':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'INPUT':
				result = elem.find('input').val();
				break;
			case 'LABEL':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'LINK':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
			case 'OPTION':
				alert('Value not found. fw_type=' + elem.attr('fw_type'));
				break;
