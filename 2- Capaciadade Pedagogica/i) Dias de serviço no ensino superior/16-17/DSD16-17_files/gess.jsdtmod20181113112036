}

function pageContentMoveDown(p_content_id) {

    var params= {};
    
    params.p_id = p_content_id;
    
    myNextRefreshAjaxFun="location.reload();";

    var options = {
        procedimento: 'web_gessi_estatico.edit_content_movedown',
        parametros: $.param(params),
        showInBox: false,
        div_actualizacao: "",
        showLoading:true
    };

    ajaxRequest(options);

}
function reloadGridTimer(options) {
	refresher = new Object();

	if (options.element) {
		refresher.element = options.element;
	} else {
		refresher.element = jQuery(this);
	}
	refresher.grid = options.grid;

	if (options.time) {
		refresher.time = options.time;
	} else {
		refresher.time = 10;
	}

	if (jQuery(refresher.element).closest('html').length > 0) {
		jQuery(refresher.element).reloadGrid(refresher.grid);
		setTimeout(function () {
			reloadGridTimer(options)
		}, refresher.time * 1000);
	}
}

function sumValGrid(val, name, record) {
	var tot_val;

	if (val == undefined) {
		tot_val = "0";
	} else {
		tot_val = val;
	}

	tot_val = tot_val.split(".").join("").replace(",", ".");
	var recVal = record[name];

	if (recVal == undefined) {
		return "0";
	}
	recVal = recVal.split(".").join("").replace(",", ".");
	tot_val = parseFloat(tot_val || 0) + parseFloat((recVal || 0));
	tot_val = formatNumber(tot_val, 2, 3, '.', ',');

	return tot_val;
}

function removeGroup(p_grid_id) {
	jQuery("#" + p_grid_id).jqGrid('groupingRemove', true);
	jQuery('#fw-jqGrid-dialog').dialog("close");
}
function runGroup(p_grid_id, p_obj) {
	larr_group = [];
	larr_groupText = [];

	p_obj.closest("#fw-jqGrid-dialog").find("form").find("tr").each(function () {
		var l_group_val = jQuery(this).find("[name=parr_col_group]").val();
		var l_group_label = jQuery(this).find("[name=parr_col_group] option:selected").text();
		var l_tot_val = jQuery(this).find("[name=parr_col_tot]").val();
		var l_tot_label = jQuery(this).find("[name=parr_col_tot] option:selected").text();
		var lgroupText = "";

		larr_group[larr_group.length] = l_group_val;

		lgroupText = l_group_label + ":<b>  {0} </b> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; ";
		if (l_tot_val) {
			lgroupText = lgroupText + l_tot_label + ": <b>{" + l_tot_val + "}</b> ";
		}
		lgroupText = lgroupText + "&nbsp;&nbsp;&nbsp;&nbsp; <span style=\"color :#777\">({1} linhas)</span>";

		larr_groupText[larr_groupText.length] = lgroupText;

	});

	jQuery("#" + p_grid_id).each(function () {
		jQuery(this).jqGrid(
			'groupingGroupBy', larr_group, {
				groupText: larr_groupText,
				groupColumnShow: [true, true],
				groupCollapse: false,
				groupOrder: ['asc', 'asc']
			});
	});
	jQuery('#fw-jqGrid-dialog').dialog("close");
}

function loadGroup(p_grid) {
	var l_grid_id = p_grid.attr("id");
	var l_html = "";
	var l_col_group = [];
	var l_col_tot = [];
	var l_colModel;
	var l_colNames;

	l_colModel = p_grid.jqGrid('getGridParam', 'colModel');
	l_colNames = p_grid.jqGrid('getGridParam', 'colNames');

	jQuery.each(l_colNames, function (idx, val) {

		if (l_colModel[idx].name != "subgrid" && l_colModel[idx].name != "cb" & val != "Opções") {
			jQuery.merge(l_col_group, [{
				"name": l_colModel[idx].name,
				"label": val
			}
			]);

		}

		if (l_colModel[idx].sorttype == "number") {
			jQuery.merge(l_col_tot, [{
				"name": l_colModel[idx].name,
				"label": val
			}
			]);

		}

	});

	l_html = l_html + "<pre style=\"font-weight: bold; font-size: 13px; color: red;\">Os totais apresentados são calculados <br>exclusivamente sobre os dados visíveis no momento.</pre>";
	l_html = l_html + "<form>";
	l_html = l_html + "<table  style=\"width: 100%;\" class=\"form\">";
	l_html = l_html + "<tr>";
	l_html = l_html + "<td class='formularionome'>";
	l_html = l_html + "Agrupar";
	l_html = l_html + "</td>";
	l_html = l_html + "<td>";
	l_html = l_html + "<select name=\"parr_col_group\">";
	jQuery.each(l_col_group, function (idx, val) {
		l_html = l_html + "<option value='" + l_col_group[idx].name + "'>";
		l_html = l_html + l_col_group[idx].label;
		l_html = l_html + "</option>";
	});
	l_html = l_html + "</select>";

	l_html = l_html + "</td>";

	l_html = l_html + "<td class='formularionome'>";
	l_html = l_html + "Total";
	l_html = l_html + "</td>";
	l_html = l_html + "<td>";
	l_html = l_html + "<select name=\"parr_col_tot\">";
	l_html = l_html + "<option></option>";
	jQuery.each(l_col_tot, function (idx, val) {
		l_html = l_html + "<option value='" + l_col_tot[idx].name + "'>";
		l_html = l_html + l_col_tot[idx].label;
		l_html = l_html + "</option>";
	});
	l_html = l_html + "</select>";

	l_html = l_html + "</td>";

	l_html = l_html + "<td>";
	l_html = l_html + "<img class=\"icon_button visible\" onclick='jQuery(this).closest(\"table\").append(newRow=jQuery(this).closest(\"tr\").clone());newRow.find(\"img\").each(function () {if(jQuery(this).hasClass(\"visible\")){jQuery(this).remove()};jQuery(this).removeClass(\"hidden\");});' src='imagens/sysnovare_add_icon'>";
	l_html = l_html + "<img class=\"icon_button hidden\" onclick='jQuery(this).closest(\"tr\").remove();'	src='imagens/sysnovare_remove_icon'>";
	l_html = l_html + "<td>";
	l_html = l_html + "</tr>";
	l_html = l_html + "</table>";
	l_html = l_html + "</form><br><br><br>";
	l_html = l_html + "<table  style=\"width: 100%;\" class=\"form\"><tbody><tr><td class=\"formfield\"><center>";
	l_html = l_html + "<div><input type=\"button\" onclick='runGroup(\"" + l_grid_id + "\",jQuery(this))' value=\"Agrupar\">&nbsp;&nbsp;";
	l_html = l_html + "<input type=\"button\" onclick='removeGroup(\"" + l_grid_id + "\")' value=\"Remover Agrupamento\"></div>";
	l_html = l_html + "</center></td></tr></tbody></table>";

	jQuery("#fw-jqGrid-dialog").html(l_html); 
	jQuery("#fw-jqGrid-dialog").dialog({title:'Configurar Grupos'});

	//Global
	grid = p_grid;
	centerDialog();
}

//Nesta função iremos carregar na grid o filtro de templates guardados
//
function loadTemplInGrid(p_grid) {
	var $grid = p_grid;
	var grid_id = $grid.attr('ID');
	
	//var $container = jQuery('#' + grid_id + '_toppager_right');

	var $container = jQuery($grid.jqGrid('getGridParam', 'pager') + '_right');
	var $topContainer = jQuery($grid.jqGrid('getGridParam', 'toppager') + '_right');


	var templObj = $grid.jqGrid('getGridParam', 'gessi_filter_template');
	var l_filter = $grid.jqGrid('getGridParam', 'postData').filters;

	var lKeys = getKeys(templObj);
	var lValues = getValues(templObj);
	var l_html = '';
	var l_selected = '';

	var $templTopContainer = $topContainer.find('.jqgrid-template-list');
	var $templContainer = $container.find('.jqgrid-template-list');
    //Se a lista de templates estiver vazia não há porque mostrar o select
	if (getKeys(templObj).length > 0) {
		$templTopContainer.fadeTo(0,1);
		$templContainer.fadeTo(0,1);
	}else {
		$templTopContainer.fadeTo(0,0);
		$templContainer.fadeTo(0,0);
	}	

	l_html = '';
	l_html = l_html + '<select style="height: 17px;padding: 0;width: 80px;">';
	l_html = l_html + '<option></option>';
	jQuery.each(templObj, function (key, value) {
		l_selected = '';
		var l_tmplFilter = templObj[key];
		l_tmplFilter = JSON.stringify(l_tmplFilter);

		if (l_filter == l_tmplFilter) {
			l_selected = ' selected ';
		}

		l_html = l_html + '<option value="' + key + '" ' + l_selected + '>' + key + '</option>';
	});
	l_html = l_html + '</select>';
	$templContainer.html(l_html);
	$templContainer.find('select').bind('change', function () {
		var l_tmplFilter = templObj[jQuery(this).val()];

		l_tmplFilter = JSON.stringify(l_tmplFilter);

		//Set value on the TOP combo
		$templTopContainer.find('select').val(jQuery(this).val());

		if (!l_tmplFilter) {
			l_tmplFilter = '';
		}

		var l_postData = $grid.jqGrid('getGridParam', 'postData');
		jQuery.extend(l_postData, {
			filters: l_tmplFilter
		});

		$grid.trigger('reloadGrid');
		//jQuery(p_grid)[0].clearToolbar();
	});

	$templTopContainer.html(l_html);
	$templTopContainer.find('select').bind('change', function () {
		var l_tmplFilter = templObj[jQuery(this).val()];

		l_tmplFilter = JSON.stringify(l_tmplFilter);

		//Set value on the BOTTOM combo
		$templContainer.find('select').val(jQuery(this).val());

		if (!l_tmplFilter) {
			l_tmplFilter = '';
		}

		var l_postData = $grid.jqGrid('getGridParam', 'postData');
		jQuery.extend(l_postData, {
			filters: l_tmplFilter
		});

		$grid.trigger('reloadGrid');
		//jQuery(p_grid)[0].clearToolbar();
	});
}

function loadNewTemplate(p_grid_id, p_obj) {
	var $grid = jQuery('#' + p_grid_id);
	var templObj = $grid.jqGrid('getGridParam', 'gessi_filter_template');
	var newTempl = {};
	var l_name = p_obj.closest('#fw-jqGrid-dialog').find("#saveTempl").find('[name=pv_name]').val();
	var l_templ = p_obj.closest('#fw-jqGrid-dialog').find("#saveTempl").find('[name=pv_filter]').val();
	newTempl[l_name] = jQuery.parseJSON(l_templ);

	jQuery.extend(templObj, newTempl);

	loadTemplInGrid($grid);
	jQuery('#fw-jqGrid-dialog').dialog("close");
}

function delTemplate(p_grid_id, p_obj) {
	var $grid = jQuery('#' + p_grid_id);
	var templObj = $grid.jqGrid('getGridParam', 'gessi_filter_template');

	p_obj.closest('#fw-jqGrid-dialog').find("#delTempl").find("[type=checkbox]:checked").each(function () {
		delete templObj[jQuery(this).val()];
	});

	loadTemplInGrid($grid);
	jQuery('#fw-jqGrid-dialog').dialog("close");
}
/*
* Guardar template
*/
function saveTemplate(p_grid) {
	var l_grid_id = p_grid.attr('id');
	var $grid = jQuery('#' + l_grid_id);
	var l_html = '';
	var l_filter = $grid.jqGrid('getGridParam', 'postData').filters;
	var templObj = $grid.jqGrid('getGridParam', 'gessi_filter_template');
	var length_templObj = getKeys(templObj).length;
	var lb_exist = true;

	if (l_filter.length == 0 || l_filter == '{"groupOp":"AND","rules":[]}') {
		lb_exist = false;

	}

	var l_grid_name = $grid.jqGrid('getGridParam', 'gessi_usr_block_name');

	if (lb_exist) {

		l_html = l_html + '<div id="saveTempl">';
		l_html = l_html + '<form action="GESSI_USR_PREFS_MAIN.FILTERS_TEMPATES_SUB">';
		l_html = l_html + '<table  style="width: 100%;" class="form">';
		l_html = l_html + '<tr>';
		l_html = l_html + '<td class=\'formularionomeobr\'>';
		l_html = l_html + 'Nome';
		l_html = l_html + '</td>';
		l_html = l_html + '<td>';
		l_html = l_html + '<input name="pv_name" required validar="S" size="40">';
		l_html = l_html + '<input name="pv_filter" type="hidden" value=' + l_filter + ' >';
		l_html = l_html + '<input name="pv_grid_id" type="hidden" value="' + l_grid_name + '">';
		l_html = l_html + '</td>';
		l_html = l_html + '</tr>';
		l_html = l_html + '</table>';
		l_html = l_html + '<br>';
		l_html = l_html + '<table  class="form"><tbody><tr><td class="formfield"><center>';
		l_html = l_html + '<div><input type="button" onclick="loadNewTemplate(\'' + l_grid_id + '\', jQuery(this));jQuery(this).closest(\'form\').ajaxSubmit(postAjaxOptions);" value="Gravar">&nbsp;&nbsp;';
		if (length_templObj > 0) {
			l_html = l_html + "<input type=\"button\" onclick='jQuery(\"#saveTempl\").hide();jQuery(\"#delTempl\").show();' value=\"Gerir Templates\"></div>";
		}
		l_html = l_html + '</center></td></tr></tbody></table>';
		l_html = l_html + '</form>';
		l_html = l_html + '</div>';
	}
	if (lb_exist || length_templObj != 0) {
		l_html = l_html + '<div id="delTempl"';
		if (lb_exist) {
			l_html = l_html + 'style="display: none;"';
		}
		l_html = l_html + '>';

		l_html = l_html + '<form action="GESSI_USR_PREFS_MAIN.FILTERS_TEMPATES_DELSUB">';
		l_html = l_html + '<table  style="width: 100%;" class="form">';

		jQuery.each(templObj, function (key, value) {

			l_html = l_html + '<tr>';
			l_html = l_html + '<td class=\'formularionome\'>';
			l_html = l_html + key;
			l_html = l_html + '</td>';
			l_html = l_html + '<td>';
			l_html = l_html + '<input name="parr_name" value="' + key + '" type="checkbox">';
			l_html = l_html + '</td>';
			l_html = l_html + '</tr>';

		});
		l_html = l_html + '<input name="pv_grid_id" type="hidden" value="' + l_grid_name + '">';
		l_html = l_html + '</table>';
		l_html = l_html + '<br>';
		l_html = l_html + '<table  class="form"><tbody><tr><td class="formfield"><center>';
		l_html = l_html + '<div><input type="button" onclick="delTemplate(\'' + l_grid_id + '\', jQuery(this));jQuery(this).closest(\'form\').ajaxSubmit(postAjaxOptions);" value="Remover">&nbsp;&nbsp;';
		l_html = l_html + "</div>";
		l_html = l_html + '</center></td></tr></tbody></table>';
		l_html = l_html + '</form>';
		l_html = l_html + '</div>';
	} else {
		alert('Nenhum Filtro Aplicado.');
		return;
	}

	jQuery('#fw-jqGrid-dialog').html(l_html);
	jQuery('#fw-jqGrid-dialog').dialog( {title: "Gravar Filtro como Template"});
	//Global
	grid = p_grid;
	centerDialog();
}

//Quando é aplicado um filtro na grid mostrados uma mensagem no header e footer
function showFilterMsg(p_grid) {
	var l_grid_id = p_grid.attr('id');
    //console.log(l_grid_id);
	var $grid = jQuery('#' + l_grid_id);
	var l_filter = $grid.jqGrid('getGridParam', 'postData').filters;

	if (l_filter.length == 0 || l_filter == '{"groupOp":"AND","rules":[]}') {
		jQuery('.jqgrid-filter-active').fadeTo(0,0);

	} else {
		jQuery('.jqgrid-filter-active').fadeTo(0,1);
	}

}
function storeFilterPageNum(p_grid) {
	var l_grid_id = p_grid.attr('id');
	var $grid = jQuery('#' + l_grid_id);
	var l_filter = $grid.jqGrid('getGridParam', 'postData').filters;
	var l_page = $grid.jqGrid('getGridParam', 'postData').page;
	var l_grid_name = $grid.jqGrid('getGridParam', 'gessi_usr_block_name');
	var l_rows = $grid.jqGrid('getGridParam', 'postData').rows;

	var param = "pv_filter=" + l_filter;
	param += "&pv_page_number=" + l_page;
	param += "&pv_grid_id=" + l_grid_name;
	param += "&pv_page_reg_number=" + l_rows;

	submitAjaxNoResp("GESSI_USR_PREFS_MAIN.GRIDS_FILTERS_SUB", param);

}

function usr_prefs_grids_init(grid, explorejn) {
	/*
	 *alteracao 2013.10.26 12:07 add top pager buttons
	 */
	if (!jQuery(grid).jqGrid("getGridParam", "gessi_usr_pager_id"))
		return;
	

	//get pager and top pager identifiers
	var pager = "" + jQuery(grid).jqGrid("getGridParam", "pager");
	var top_pager = "" + jQuery(grid).jqGrid("getGridParam", "toppager");

	//console.log(jQuery(grid).jqGrid('getGridParam', 'toppager'));

	var pagerRight = jQuery(jQuery(grid).jqGrid('getGridParam', 'pager') + '_right');
	var topPagerRight = jQuery(jQuery(grid).jqGrid('getGridParam', 'toppager') + '_right');

	var p_explore_jn = explorejn;


	//Add active filter div to top and bottom
    pagerRight.prepend('<div class="jqgrid-filter-active">Filtro activo</div>');
	topPagerRight.prepend('<div class="jqgrid-filter-active">Filtro activo</div>');
	//Add template filter div
    pagerRight.prepend('<div class="jqgrid-template-list"></div>');
	topPagerRight.prepend('<div class="jqgrid-template-list"></div>');
    

	//alteracao 2012.09.07 15:55 daniel.silva
	//Add Separator
	//
	jQuery(grid).jqGrid("navSeparatorAdd", pager);
	jQuery(grid).jqGrid("navSeparatorAdd", top_pager);

	jQuery(grid).jqGrid("setGridParam", {
		search: true,
		postData: {
			"filters": grid.jqGrid("getGridParam", "gessi_usr_template")
		}
	}).trigger("reloadGrid");

	//Add the search option to each column
	jQuery(grid).jqGrid('filterToolbar', {
		defaultSearch: "cn",
		searchOperators: true,
		stringResult: true,
		searchOnEnter: true,
		operandTitle: "Selecione o operador de pesquisa"
	});

	//alteracao 2013.11.01 16:56 daniel.silva
	//function to search in tollbar after 1 sec

	var timeoutHnd,
		k = jQuery.ui.keyCode,
		toSkip = [k.TAB, k.SHIFT, k.ALT, k.ESCAPE, k.LEFT, k.UP, k.RIGHT, k.DOWN, k.HOME, k.END, k.INSERT];
	jQuery(grid).closest(".ui-jqgrid-view").find(".ui-jqgrid-hdiv .ui-search-toolbar input[type=text]").keydown(function (e) {
		var key = e.which;
		if (key === k.ENTER) {
			jQuery(grid)[0].triggerToolbar();
			timeoutHnd = 0;
			//return false;
		}
		if (jQuery.inArray(key, toSkip) < 0) {
			if (timeoutHnd) {
				clearTimeout(timeoutHnd);
				timeoutHnd = 0;
			}
			timeoutHnd = setTimeout(function () {
				jQuery(grid)[0].triggerToolbar();
				timeoutHnd = 0;
			}, 1000);
		}
	});

	//Add the button to excel export
	//alteracao 2012.09.07 15:55 daniel.silva
	//add icon
	jQuery(grid).jqGrid("navButtonAdd", pager, {
		caption: "",
		title: "Exportar para Excel",
		buttonicon: 'ui-icon-document',
		onClickButton: function () {
			var rows = jQuery(grid).jqGrid("getGridParam", "rowNum"),
				page = jQuery(grid).jqGrid("getGridParam", "page"),
				sortname = jQuery(grid).jqGrid("getGridParam", "sortname"),
				sortorder = jQuery(grid).jqGrid("getGridParam", "sortorder"),
				gridrecords = null,
				rownum = null,
				test = null,
				linkbuild = "FW_INTERFACE_BUILDER_GERAL.fw_generic_grid_excel?name_array=pv_block_name&name_array=pv_param_id&name_array=nd&name_array=sidx&name_array=sord&value_array=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_block_name") + "&value_array=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_param_id") + "&value_array=1&value_array=" + sortname + "&value_array=" + sortorder;

			/*                if (grid.jqGrid("getGridParam", "gessi_usr_template") != "")
			linkbuild = linkbuild + "&name_array=_search&value_array=true&name_array=filters&value_array=" + grid.jqGrid("getGridParam", "gessi_usr_template");
			else
			linkbuild = linkbuild + "&name_array=_search&value_array=false";
			 */
			var gfilters = jQuery(grid).jqGrid("getGridParam", "postData").filters;

			if (gfilters)
				linkbuild = linkbuild + "&name_array=_search&value_array=true&name_array=filters&value_array=" + gfilters;
			else
				linkbuild = linkbuild + "&name_array=_search&value_array=false";

			gridrecords = grid.jqGrid("getGridParam", "records");

			if (parseInt(gridrecords) + 0 > parseInt(rows) + 0) {
				if (confirm("Deseja obter todos os registos no Excel?")) {
					linkbuild = linkbuild + "&name_array=rows&value_array=" + gridrecords + "&name_array=page&value_array=1";
				} else {
					linkbuild = linkbuild + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
				}
			} else {
				linkbuild = linkbuild + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
			}
			//alteracao jose.duarte 2015.08.17 GP 107213 no caso de exportar uma subgrid devemos mandar o id da mastergrid
			var trclass = jQuery(grid).closest('tr').attr("class");
			if (trclass == "ui-subgrid") {
				var master_grid_id = jQuery(grid).closest('tr').prev().attr("id");

				if (master_grid_id != "undefined") {
					linkbuild = linkbuild + "&name_array=master_grid_id&value_array=" + master_grid_id;
				}
			}
			//fim alteracao jose.duarte 2015.08.17 GP 107213
			window.open(linkbuild);
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", top_pager, {
		caption: "",
		title: "Exportar para Excel",
		buttonicon: 'ui-icon-document',
		onClickButton: function () {
			var rows = jQuery(grid).jqGrid("getGridParam", "rowNum"),
				page = jQuery(grid).jqGrid("getGridParam", "page"),
				sortname = jQuery(grid).jqGrid("getGridParam", "sortname"),
				sortorder = jQuery(grid).jqGrid("getGridParam", "sortorder"),
				gridrecords = null,
				rownum = null,
				test = null,
				linkbuild = "FW_INTERFACE_BUILDER_GERAL.fw_generic_grid_excel?name_array=pv_block_name&name_array=pv_param_id&name_array=nd&name_array=sidx&name_array=sord&value_array=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_block_name") + "&value_array=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_param_id") + "&value_array=1&value_array=" + sortname + "&value_array=" + sortorder;

			/*                if (grid.jqGrid("getGridParam", "gessi_usr_template") != "")
			linkbuild = linkbuild + "&name_array=_search&value_array=true&name_array=filters&value_array=" + grid.jqGrid("getGridParam", "gessi_usr_template");
			else
			linkbuild = linkbuild + "&name_array=_search&value_array=false";
			 */
			var gfilters = jQuery(grid).jqGrid("getGridParam", "postData").filters;

			if (gfilters)
				linkbuild = linkbuild + "&name_array=_search&value_array=true&name_array=filters&value_array=" + gfilters;
			else
				linkbuild = linkbuild + "&name_array=_search&value_array=false";

			gridrecords = grid.jqGrid("getGridParam", "records");

			if (parseInt(gridrecords) + 0 > parseInt(rows) + 0) {
				if (confirm("Deseja obter todos os registos no Excel?")) {
					linkbuild = linkbuild + "&name_array=rows&value_array=" + gridrecords + "&name_array=page&value_array=1";
				} else {
					linkbuild = linkbuild + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
				}
			} else {
				linkbuild = linkbuild + "&name_array=rows&value_array=" + rows + "&name_array=page&value_array=" + page;
			}
			//alteracao jose.duarte 2015.08.17 GP 107213 no caso de exportar uma subgrid devemos mandar o id da mastergrid
			var trclass = jQuery(grid).closest('tr').attr("class");
			if (trclass == "ui-subgrid") {
				var master_grid_id = jQuery(grid).closest('tr').prev().attr("id");

				if (master_grid_id != "undefined") {
					linkbuild = linkbuild + "&name_array=master_grid_id&value_array=" + master_grid_id;
				}
			}
			//fim alteracao jose.duarte 2015.08.17 GP 107213
			window.open(linkbuild);

		}
	});

	jQuery(grid).jqGrid("navButtonAdd", pager, {
		caption: "",
		title: "Esconder/Mostrar colunas",
		buttonicon: 'ui-icon-newwin',
		onClickButton: function () {
			jQuery(grid).jqGrid('columnChooser');
			//Add bind to submit button to save preferences on DB
			jQuery("[multiple=multiple]").bind("mouseup", function () {
						usr_prefs_grid_visibility_array(grid);
					});
					jQuery("[multiple=multiple]").bind("keyup", function () {
						usr_prefs_grid_visibility_array(grid);
					});
					jQuery(".ui-dialog-buttonset").find("[role=button]").each(
						function (i) {
							if (i == 0)
								jQuery(this).bind("click", function () {
									usr_prefs_grid_submit_column_visible(grid);
								});
							else
								jQuery(this).bind("click", function () {
									usr_prefs_grid_clear_visible_columns();
								});
						});
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", top_pager, {
		caption: "",
		title: "Esconder/Mostrar colunas",
		buttonicon: 'ui-icon-newwin',
		onClickButton: function () {
			jQuery(grid).jqGrid('columnChooser');

		    jQuery("[multiple=multiple]").bind("mouseup", function () {
						usr_prefs_grid_visibility_array(grid);
					});
					jQuery("[multiple=multiple]").bind("keyup", function () {
						usr_prefs_grid_visibility_array(grid);
					});
					jQuery(".ui-dialog-buttonset").find("[role=button]").each(
						function (i) {
							if (i == 0)
								jQuery(this).bind("click", function () {
									usr_prefs_grid_submit_column_visible(grid);
								});
							else
								jQuery(this).bind("click", function () {
									usr_prefs_grid_clear_visible_columns();
								});
						});

		}
	});


	if (p_explore_jn == "Y") {
		jQuery(grid).jqGrid("navSeparatorAdd", pager);
		jQuery(grid).jqGrid("navButtonAdd", pager, {
			caption: "",
			title: "Ver Tabela(s) de Histórico(s)",
			buttonicon: "ui-icon-zoomin",
			onClickButton: function () {
				openWindow("FW_EXPLORE_MAIN.EXPLORE_TABLES_MAIN", "pv_block_name=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_block_name"), "Histórico");
			}
		});

		jQuery(grid).jqGrid("navSeparatorAdd", top_pager);
		jQuery(grid).jqGrid("navButtonAdd", top_pager, {
			caption: "",
			title: "Ver Tabela(s) de Histórico(s)",
			buttonicon: "ui-icon-zoomin",
			onClickButton: function () {
				openWindow("FW_EXPLORE_MAIN.EXPLORE_TABLES_MAIN", "pv_block_name=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_block_name"), "Histórico");
			}

		});
	}
	//alteracao 2014.09.19 12:30 daniel.silva
	jQuery(grid).jqGrid("navSeparatorAdd", pager);
	jQuery(grid).jqGrid("navSeparatorAdd", top_pager);

	//alteracao 2012.09.07 15:55 daniel.silva
	//Add the button to toggle search toolbar
	//

	jQuery(grid).jqGrid("navButtonAdd", pager, {
		caption: "",
		title: "Esconder/Mostrar Filtros de Pesquisa",
		buttonicon: 'ui-icon-pin-s',
		onClickButton: function () {
			jQuery(grid)[0].toggleToolbar();
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", top_pager, {
		caption: "",
		title: "Esconder/Mostrar Filtros de Pesquisa",
		buttonicon: 'ui-icon-pin-s',
		onClickButton: function () {
			jQuery(grid)[0].toggleToolbar();
		}
	});

	//alteracao 2012.09.07 15:55 daniel.silva
	//Add the button to clear search toolbar
	//
	jQuery(grid).jqGrid("navButtonAdd", pager, {
		caption: "",
		title: "Limpar Filtros de Pesquisa",
		buttonicon: 'ui-icon-circle-close',
		onClickButton: function () {
			jQuery(grid)[0].clearToolbar();
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", top_pager, {
		caption: "",
		title: "Limpar Filtros de Pesquisa",
		buttonicon: 'ui-icon-circle-close',
		onClickButton: function () {
			jQuery(grid)[0].clearToolbar();
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", pager, {
		caption: "",
		title: "Agrupar",
		buttonicon: "ui-icon-calculator",
		onClickButton: function () {
			loadGroup(grid);
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", top_pager, {
		caption: "",
		title: "Agrupar",
		buttonicon: "ui-icon-calculator",
		onClickButton: function () {
			loadGroup(grid);
		}
	});

	//alteração 2014.09.25 18:44 daniel.silva
	//Filters templates
	loadTemplInGrid(jQuery(grid));

	jQuery(grid).jqGrid("navButtonAdd", pager, {
		caption: "",
		title: "Gravar Filtro como Template",
		buttonicon: "ui-icon-disk",
		onClickButton: function () {
			saveTemplate(jQuery(this));
		}
	});

	jQuery(grid).jqGrid("navButtonAdd", top_pager, {
		caption: "",
		title: "Gravar Filtro como Template",
		buttonicon: "ui-icon-disk",
		onClickButton: function () {
			saveTemplate(jQuery(this));
		}
	});
}

function usr_prefs_grid_pageInRange(grid)
/*
alteracao 2015.01.28 19:02 daniel.silva
 */ {
	var totalRegs = jQuery(grid).jqGrid("getGridParam", "records"),
		regsPerPage = jQuery(grid).jqGrid("getGridParam", "rowNum"),
		page = jQuery(grid).jqGrid("getGridParam", "page"),
		result = 0;

	result = totalRegs / regsPerPage;

	result = (Math.ceil(result) ? Math.ceil(result) : 1);

	myLastObj = grid;
	/*alteracao cristiano.martins 2015.08.27 SR10870
		quando a pagina é maior que o total de paginas existente é necessário corrigir a página e refrescar a grid
		Nota: pode ser necessário passar este parametro , total: ( Math.ceil(result) ? Math.ceil(result) : 1 )
	*/
	if (page > result/*&& page != 1*/) {
		setTimeout(function () {
			myLastObj.jqGrid("setGridParam", {
				page: 1
			});
			myLastObj.trigger("reloadGrid");
		}, 5);
	}

}

function usr_prefs_grid_sortable(grid) {
	//console.log(grid);
	return {
		update: function (permutation) {
			usr_prefs_grid_submitDrag(permutation, grid)
		}
	}
}

function usr_prefs_grid_submitDrag(permutation, grid) {
	var y = jQuery(grid).jqGrid("getGridParam", "colModel");
	var i = 0,
		index = 0,
		l = y.length,
		j,
		cmName;
	var column_name = "";
	var indexorder = "";

	j = "[";

	if (grid.jqGrid("getGridParam", "multiselect")) {
		//Alterado i=0 para i=1 devido ao facto da 1ª coluna ser a coluna das checkbox
		j = j + "0,";
		for (i = 1; i < l; i++) {
			column_name = column_name + "&parr_column_name=" + y[i].classes;
			indexorder = indexorder + "&parr_indexorder=" + i;
			j = j + i + ",";
		}
	} else {
		for (i = 0; i < l; i++) {
			column_name = column_name + "&parr_column_name=" + y[i].classes;
			indexorder = indexorder + "&parr_indexorder=" + i;
			j = j + i + ",";
		}
	}
	j = j.substring(0, j.length - 1);
	j = j + "]";
	submitAjaxNoResp("GESSI_USR_PREFS_MAIN.grids_save_indexorder", "pv_grid_id=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_block_name") + column_name + indexorder);
	grid.jqGrid("setGridParam", {
		remapColumns: eval(j)
	});
}

function usr_prefs_grid_startsWith(expression, pattern) {
	var len = pattern.length;
	var match;
	var result = false;
	if (expression.length > len) {
		match = expression.substring(0, len);
		if (match == pattern)
			result = true;
		else
			result = false;
	} else {
		result = false;
	}
	return result;
}

//Global String that stores the columns that are selected to be visible to the user.
var usr_prefs_grid_visible_columns = "";

function usr_prefs_grid_visibility_array(grid) {
	var colModel = jQuery(grid).jqGrid("getGridParam", "colModel");

	usr_prefs_grid_visible_columns = "";
	jQuery("[multiple=multiple]").find(":selected").each(
		function () {
			usr_prefs_grid_visible_columns = usr_prefs_grid_visible_columns + "&parr_column=" + usr_prefs_grid_returnColModNameByIndex(grid, jQuery(this).val())
		});
}

function usr_prefs_grid_clear_visible_columns() {
	usr_prefs_grid_visible_columns = "";
}

function usr_prefs_grid_submit_column_visible(grid) {
	if (usr_prefs_grid_visible_columns != "") {
		submitAjaxNoResp("GESSI_USR_PREFS_MAIN.grids_save_visible", "pv_grid_id=" + jQuery(grid).jqGrid("getGridParam", "gessi_usr_block_name") + usr_prefs_grid_visible_columns);
		usr_prefs_grid_visible_columns = "";
	}
}

function usr_prefs_grid_returnColModNameByIndex(grid, index) {
	var colModel = jQuery(grid).jqGrid("getGridParam", "colModel");
	return colModel[index].classes;
}
