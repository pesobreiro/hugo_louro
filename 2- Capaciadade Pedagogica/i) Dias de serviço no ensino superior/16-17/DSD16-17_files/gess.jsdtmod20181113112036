//CM - 2011-07-06- - Refresh da combo "modulo"
function update_modulo_combo() {
	if (!myLastObj.val()) {
		myLastObj.getObj("#gessi_vld_atoms_main_atoms_main_div_p_pesquisa_form_modulo").children().attr('disabled', 'disabled');
		myLastObj.getObj("#gessi_vld_atoms_main_atoms_main_div_p_pesquisa_form_modulo").val("");

	} else {
		//myNextRefreshBlockFun="myLastObj.getObj('#gessi_vld_atoms_main_profiles_main_profileslist_pesquisa_form_modulo').children().attr('disabled', 'false');";
		myNextRefreshBlockFun = "myLastObj.getObj('#gessi_vld_atoms_main_atoms_main_div_p_pesquisa_form_modulo').children().removeAttr('disabled');";
		refreshComponent({
			name: 'gessi_vld_atoms_main_atoms_main_div_p_pesquisa_form_modulo',
			parr_name: ['PV_APL'],
			parr_value: [myLastObj.val()]
		});
	}
}

function inst_atoms_update_modulo_combo(PV_SIGLA) {
	if (!myLastObj.val()) {
		myLastObj.getObj("#gessi_vld_atoms_main_inst_atoms_main_div_s_pesquisa_form_modulo").attr('disabled', 'disabled');
		myLastObj.getObj("#gessi_vld_atoms_main_inst_atoms_main_div_s_pesquisa_form_modulo").val("");

	} else {
		//myNextRefreshBlockFun="myLastObj.getObj('#gessi_vld_atoms_main_inst_atoms_main_div_s_pesquisa_form_modulo').attr('disabled', '');";
		myNextRefreshBlockFun = "myLastObj.getObj('#gessi_vld_atoms_main_inst_atoms_main_div_s_pesquisa_form_modulo').children().removeAttr('disabled');";
		refreshComponent({
			name: 'gessi_vld_atoms_main_inst_atoms_main_div_s_pesquisa_form_modulo',
			parr_name: ['PV_APL', 'PV_SIGLA'],
			parr_value: [myLastObj.val(), PV_SIGLA]
		});
	}

}

